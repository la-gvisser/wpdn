//Type 240
void IS-IS::Adjacency Three-Way State()
{
	g[11] = g[10];
	HBYT 3 0 90 c2 Adjacency Three-Way State:;
	DLNG 0 0 90 c2 Extended Local Circuit ID:;
	g[10] -= 5;
	g[11] -= 5;
	if (g[11] >= 4)
	{
		g[11] -= 4;
		CHR# 0 b 90 c2 Neighbor System ID;
		DLNG 0 0 90 c2 Neighbor Extended Local Circuit ID;
	}
	g[10] -= g[10];
	TNXT -IS-IS Variable Headers;
}
