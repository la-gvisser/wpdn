		CBIT 7 1 0 d4 1... .... Reserved;
		CLSE 0 0 0 d4 0... .... Reserved;
		CBIT 6 1 0 d4 .1.. .... Reserved;
		CLSE 0 0 0 d4 .0.. .... Reserved;
		CBIT 5 1 0 d4 ..1. .... Enhanced;
		CLSE 0 0 0 d4 ..0. .... Enhanced;
		CBIT 4 1 0 d4 ...1 .... Dedicated 1+1: set;
		CLSE 0 0 0 d4 ...0 .... Dedicated 1+1: not set;
		CBIT 3 1 0 d4 .... 1... Dedicated 1:1: set;
		CLSE 0 0 0 d4 .... 0... Dedicated 1:1: not set;
		CBIT 2 1 0 d4 .... .1.. Shared: set;
		CLSE 0 0 0 d4 .... .0.. Shared: not set;
		CBIT 1 1 0 d4 .... ..1. Unprotected: set;
		CLSE 0 0 0 d4 .... ..0. Unprotected: not set;
		CBIT 0 1 0 d4 .... ...1 Extra Traffic: set;
		CLSE 0 0 0 d4 .... ...0 Extra Traffic: not set;
	LABL 0 0 0 c8;
	BBYT 0 41 90 c2 Reserved:;
	g[12] -= 2;
}


