	TNXT -TP Param Codes;
void TP Disconnect Request Jmp;
	LABL 0 0 0 b1 Disconnect Request TPDU\Disconnect:;
	HWRD 0 0 90 c2 Dst. Reference:;
	HWRD 0 0 90 c2 Src. Reference:;
	DBYT 0 3 90 c2 Reason:;
	SLTE 80 3;
	SKIP 3;
	CST# ffffffff 3 0 14 Disc Reasons LT 128;
	SGTE 7f 3;
	SKIP 1;
	CST# 7f 3 0 14 Disc Reasons GE 128;
	SUBG 5 1;
	TNXT -TP Param Codes;
void TP Disconnect Confirm Jmp;
