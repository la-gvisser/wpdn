void IS-IS::MT-Capability::SPB-Inst()
{
	D64B( 0 , 0, 0x90, 0xc2, "CIST Root Identifier:" );
	HLNG( 0 , l[3], 0x90, 0xc2, "CIST External Root Path Cost:" );
	DWRD( 0 , l[4], 0x90, 0xc2, "Bridge Priority:" );
	BBIT( 15, 0, 0x90, 0xc2, "reserved:" );
	BBIT( 1, 0, 0x90, 0xc2, "V-Bit:" );
	DWRD( 0 , l[5], 0x90, 0xc2, "SPSourceID:" );
	DBYT( 0 , l[6], 0x90, 0xc2, "Number of Trees:" );
	l[2] = 1;
	while(l[6] > 0)
	{
		LABL 0 0 0 b7 VLAN-ID Tuple ;
		DGLB( 0 , l[2], 0x07, 0x07 );
		GLNG( 1, l[8] );
		LABL( 0 , 0, 0, 0x07, ":  " );
		CST# 0 48 0 07 IS-IS::ECT-Algo;
		BBIT 1 0 90 c2 U:;
		BBIT 1 0 90 c2 M:;
		BBIT 1 0 90 c2 A:;
		BBIT 5 0 90 c2 reserved:;
		HLNG 0 48 90 c2 ECT Algorithm:;
		CST# 0 48 0 14 IS-IS::ECT-Algo;
		HBIT c 0 90 c2 Base Vid:;
		HBIT c 0 90 c2 SPVid:;
		LABL 0 0 0 c8;
		l[2] += 1;
		l[6] -= 1;
	}
	g[12] -= g[12];
}

