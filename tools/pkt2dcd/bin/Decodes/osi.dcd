	LABL 0 0 0 b1 Data TPDU\Data TPDU:;
	WHOA 0 b 0 2 TP DT Opt;
	MARK 1 a 0 0 TP DT Opt;
	WHOA 0 b 0 2 TP DT Opt;
	TNXT -DT Class 0,1 - Normal;
void DT Class 0,1 - Normal;
	TPDU-NR-EOT;
	SUBG 1 1;
	TNXT -TPDU Data;
void DT Class 2,3,4 - Normal;
	HWRD 0 0 90 c2 Dst. Reference:;
	TPDU-NR-EOT;
	SUBG 3 1;
	TNXT -TP Param Codes;
void DT Class 2,3,4 - Extended;
	HWRD 0 0 90 c2 Dst. Reference:;
	GBYT 0 3;
	DIVG 80 3;
	GLNG 0 4;
	ANDG 7fffffff 4;
	INCR 4;
	DGLB 0 3 90 c2 End of TPDU:;
	DGLB 0 4 90 c2 Send Sequence No.:;
	SUBG 6 1;
	TNXT -TP Param Codes;
void TPDU-NR-EOT;
	GBYT 0 3;
	DIVG 80 3;
	GBYT 0 4;
	ANDG 7f 4;
	INCR 1;
	DGLB 0 3 90 c2 End of TPDU:;
	DGLB 0 4 90 c2 Send Sequence No.:;
	TRTS;
void Unknown TPDU Service;

void TP Param Codes;
	BREM 0 5;
	TGTE 4 5 0 0 RETURN;
	TEQU 0 1 0 0 TPDU Data;
	SGTE 5dc 1;
	SKIP 1;
	TNXT -TPDU Data;
	HBYT 0 4 90 c2 Parameter Code:;
	BREM 0 5;
	TGTE 4 5 0 0 RETURN;
	TEQU 85 4 0 0 TPDU Ack Time;
	TEQU 86 4 0 0 TPDU Resid Err;
	TEQU 87 4 0 0 TPDU Priority;
	TEQU 88 4 0 0 TPDU Transit Delay;
	TEQU 89 4 0 0 TPDU Throughput;
	TEQU 8a 4 0 0 TPDU Subsequence;
	TEQU 8b 4 0 0 TPDU Reass Time;
	TEQU 8c 4 0 0 TPDU Flow Ctl Confirm;
	TEQU 8f 4 0 0 TPDU Selective Ack Params;
	TEQU c0 4 0 0 TPDU Size;
	TEQU c1 4 0 0 TSAP-ID Calling;
	TEQU c2 4 0 0 TSAP-ID Called;
	TEQU c3 4 0 0 TPDU Checksum;
	TEQU c4 4 0 0 TPDU Version;
	TEQU c5 4 0 0 TPDU Protection;
	TEQU c6 4 0 0 TPDU Add Options;
	TEQU c7 4 0 0 TPDU Alt Proto;
	TEQU e0 4 0 0 TPDU User Defined;
	TEQU f0 4 0 0 Max TPDU Size;
	TEQU f2 4 0 0 TPDU Inactivity;
	TNXT -TPDU Unknown;

void TPDU Ack Time;
	LABL 0 0 0 14 Acknowledge Time;
	DBYT 0 4 a0 92 Parameter Length:;
	DWRD 0 0 a0 92 Max. Acknowledge Time:;
	LABL - " milliseconds";
	SUBG 2 1;
	SUBG 4 81;
	TNXT -TP Param Codes;
void TPDU Resid Err;
	LABL 0 0 0 14 Residual Error Rate;
	DBYT 0 4 a0 92 Parameter Length:;
	DBYT 0 3 a0 92 Target Value:;
	LABL 0 0 0 4  (power of 10);
	DBYT 0 3 a0 92 Minimum Acceptable:;
	LABL 0 0 0 4  (power of 10);
	DBYT 0 3 a0 92 TSDU Size:;
	LABL 0 0 0 4  (power of 2);
	SUBG 5 1;
	TNXT -TP Param Codes;
void TPDU Priority;
	LABL 0 0 0 14 Priority;
	DBYT 0 4 a0 92 Parameter Length:;
	DWRD 0 3 a0 92 Priority:;
	LABL 0 0 0 4  (0 is the highest priority);
	SUBG 4 1;
	TNXT -TP Param Codes;
void TPDU Transit Delay;
	LABL 0 0 0 14 Transit Delay;
	DBYT 0 4 a0 92 Parameter Length:;
