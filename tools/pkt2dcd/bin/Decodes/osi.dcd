void IS-IS::MT-Port-Capability::VLAN-FLAGs()
{
	HWRD( 0, 0, 0x90, 0xc2, "Port ID:" );
	CHR#( 2, 0, 0x90, 0xc2, "Sender Nickname:" );
	BBIT( 1, 41, 0x90, 0xc2, "AF:" );
		CEQU( 1, 41, 0, 0x14, "Originating IS believes it is appointed forwarder for the VLAN and port on which the containing IIH PDU was sent" );
	BBIT( 1, 41, 0x90, 0xc2, "AC:" );
		CEQU( 1, 41, 0, 0x14, "Originating port configured as an access port (TRILL traffic disabled)" );
	BBIT( 1, 41, 0x90, 0xc2, "VM:" );
		CEQU( 1, 41, 0, 0x14, "VLAN mapping detected on this link" );
	BBIT( 1, 41, 0x90, 0xc2, "BY:" );
		CEQU( 1, 41, 0, 0x14, "Bypass pseudonode" );
	HBIT c 0 90 c2 Outer VLAN ID:;
	BBIT( 1, 41, 0x90, 0xc2, "TR:" );
		CEQU( 1, 41, 0, 0x14, "Originating port configured as a trunk port (endstation service disabled)" );
	BBIT( 1, 0, 0x90, 0xc2, "Res.:" );
	BBIT( 1, 0, 0x90, 0xc2, "Res.:" );
	BBIT( 1, 0, 0x90, 0xc2, "Res.:" );
	HBIT c 0 90 c2 Outer VLAN ID:;
	g[12] -= 8;
}

void IS-IS::MT-Port-Capability::Enabled-VLANs()
{
	BBIT( 4, 0, 0x90, 0xc2, "RESV:" );
	HBIT c 0 90 c2 Start VLAN ID:;
	g[12] -= 2;
	CHR#( 0 , g[12], 0x90, 0xc2 , "VLAN bit-map:");
	g[12] -= g[12];
}

void IS-IS::MT-Port-Capability::AppointedFwrdrs()
{
	l[2] = 1;
	while (g[12] != 0)
	{
		LABL 0 0 0 b7  Appointment Information: ;
		DGLB( 0 , l[2], 0x07, 0x07 );
		LABL( 0 , 0, 0, 0x07, ":  " );
		CHR#( 2, 0, 0x90, 0xc2, "Appointee Nickname:" );
		BBIT( 4, 0, 0x90, 0xc2, "RESV:" );
		HBIT c 0 90 c2 Start VLAN ID:;
		BBIT( 4, 0, 0x90, 0xc2, "RESV:" );
		HBIT c 0 90 c2 End VLAN ID:;
		g[12] -= 6;
		l[2] += 1;
	}
}

void IS-IS::MT-Port-Capability::SPB-MCID()
{
	while (g[12] != 0)
	{
		LABL 0 0 0 b7  MCID Configuration Identifier;
			DBYT 0 0 90 c2  Configuration ID Format Selector:;
			CHR# 20 0 90 c2 Configuration Name:;
			DWRD 0 0 90 c2 Revision Level:;
			HEX# 10 0 90 c2 Configuration Digest:;
		LABL 0 0 0 c8;
		g[12] -= 51;
		LABL 0 0 0 b7  Auxiliary MCID Configuration Identifier;
			DBYT 0 0 90 c2  Configuration ID Format Selector:;
			CHR# 20 0 90 c2 Configuration Name:;
			DWRD 0 0 90 c2 Revision Level:;
			HEX# 10 0 90 c2 Configuration Digest:;
		LABL 0 0 0 c8;
		g[12] -= 51;
		//DGLB 0 c 90 c2 Unknown Field Data:;
	}
}

void IS-IS::MT-Port-Capability::SPB-Digest()
{
	BBIT 3 0 90 c2 reserved:;
	BBIT 1 0 90 c2 V:;
	BBIT 2 0 90 c2 A:;
	BBIT 2 0 90 c2 D:;
	CHR# 20 0 90 c2 Configuration Digest:;
	g[12] -= 33;
}

void IS-IS::MT-Port-Capability::SPB Base VLAN-Identifier()
{
	l[2] = 1;
	while (g[12] != 0)
	{
		LABL 0 0 0 b7 ECT-VID Tuple ;
		DGLB( 0 , l[2], 0x07, 0x07 );
		GLNG( 0, l[8] );
		LABL( 0 , 0, 0, 0x07, ":  " );
		CST# 0 48 0 07 IS-IS::ECT-Algo;
		HLNG 3 41 90 c2 ECT Algorithm:;
		CST# 0 41 0 14 IS-IS::ECT-Algo;
		BBIT c 0 90 c2 Base Vid:;
		BBIT 1 0 90 c2 U:;
		BBIT 2 0 90 c2 M:;
		BBIT 2 0 90 c2 reserved:;
		g[12] -= 6;
		LABL 0 0 0 c8;
		l[2] += 1;
	}
}

//144
void IS-IS::MT-Capability()
{	
	g[11] = g[10];
	BWRD( 0, 0, 0x47, 0xb7, "Header:" );
	DECR 2;
	BBIT( 1, 0, 0x90, 0xc2, "Overload Bit:" );
	BBIT( 3, 0, 0x90, 0xc2, "reserved:" );
	HBIT( 12, 0, 0x90, 0xc2, "MTID:" );
	LABL 0 0 0 b8;
	g[11] -= 2;
	while (g[11] !=0)
	{
		GBYT 0 9;
		CST# 0 9 0 b7 IS-IS::MT-Capability::Mean;
		CLSE 0 0 0 b7 Unknown Field;
		DBYT 0 9 90 c2 Field Code:;
		CLSE 0 0 0 14 Unknown Field Code;
		DBYT 0 c 90 c2 Field Length:;		
		//SUBG c 81;
		g[11] -= 2;
		//g[12] -= 2;
		g[11] -= g[12];
		DGLB 0 c 90 c2 Unknown Field Data:;
		TSB# 0 9 0 0 IS-IS::MT-Capability::Fields;
		if (g[12] != 0 )
		{
			DUMP 0 c 90 c2 Unknown Field Data:;
			
		}
		LABL 0 0 0 b8;
	}
	SUBG a 81;
	TNXT -IS-IS Variable Headers;
}

