void ISO TP Svcs;
	LABL 0 0 0 b1 TPDU - Transport Protocol Data Units\TPDU:;
	DBYT 0 1 90 c2 Length Indicator:;
	GBIT 4 2;
	HBYT 0 3 90 c2 TPDU Code:;
	ANDG f 3 80;
	SUBG 1 1;
	CST# ffffffff 2 0 14 TP Service Types;
	CLSE 0 0 0 14 Unknown Service;
	TST# ffffffff 2 0 14 TP Svc Type Jumps;
	TLSE -Unknown TPDU Service;

void TP Expedited Data Jmp;
	LABL 0 0 0 b1 Expedited Data TPDU\Expedited Data:;
	WHOA 0 b 0 2 TP ED Opt;
	MARK 1 a 0 0 TP ED Opt;
	WHOA 0 b 0 2 TP ED Opt;
	TNXT -ED Class 1,2,3,4 - Normal;

void ED Class 1,2,3,4 - Normal;
	HWRD 0 0 90 c2 Dst. Reference:;
	GBYT 0 3;
	DIVG 80 3;
	GBYT 0 4;
	ANDG 7f 4;
	INCR 1;
	DGLB 0 3 90 c2 End of TPDU:;
	DGLB 0 4 90 c2 Identification No.:;
	SUBG 3 1;
	TNXT -TP Param Codes;
void ED Class 2,3,4 - Extended;
	HWRD 0 0 90 c2 Dst. Reference:;
	GBYT 0 3;
	DIVG 80 3;
	GLNG 0 4;
	ANDG 7fffffff 4;
	INCR 4;
	DGLB 0 3 90 c2 End of TPDU:;
	DGLB 0 4 90 c2 Identification No.:;
	SUBG 6 1;
	TNXT -TP Param Codes;
void TP Expedited Data Acknowledge Jmp;
	LABL 0 0 0 b1 Expedited Data Acknowledge TPDU\Data ACk:;
	HWRD 0 0 90 c2 Dst. Reference:;
	WHOA 0 b 0 2 TP EA Opt;
	MARK 1 b 0 0 TP EA Opt;
	WHOA 0 b 0 2 TP EA Opt;
	TNXT -EA Class 1,2,3,4 - Normal;

void EA Class 1,2,3,4 - Normal;
	DBYT 0 0 90 c2 Next Sequence No.:;
	SUBG 3 1;
	TNXT -TP Param Codes;

void EA Class 2,3,4 - Extended;
	DLNG 0 4 90 c2 Next Sequence No.:;
	SUBG 6 1;
	TNXT -TP Param Codes;

void TP Reject Jmp;
	WHOA 0 b 0 2 TP RJ Opt;
