void IS-IS::MT-Capability::SPB-I-OALG()
{
	HLNG 0 48 90 c2 Opaque ECT Algorithm:;
	CST# 0 48 0 14 IS-IS::ECT-Algo;
	g[12] -= 4;
	DUMP( 0 , g[12], 0x90, 0xc2 , "Opaque ECT Information:");
}

void IS-IS::MT-Capability::SPBM-SI()
{
	ETHR( 0 , 0, 0x90, 0xc2, "B-MAC ADDRESS:" );
	BBIT( 0x4, 0, 0x90, 0xc2, "reserved:" );
	BBIT( 0xc, 0, 0x90, 0xc2, "Base VID:" );
	g[12] -= 8;
	l[2] = 1;
	while( g[12] > 0)
	{
		LABL 0 0 0 b7 I-SID # ;
		DGLB( 0 , l[2], 0x07, 0x07 , " ");
		BBIT( 1, 0, 0x90, 0xc2, "Transmit:" );
		BBIT( 1, 0, 0x90, 0xc2, "Recieve:" );
		BBIT( 6, 0, 0x90, 0xc2, "Reserved:" );
		BBIT( 0x18, 0, 0x90, 0xc2, "I-SID:" );
		g[12] -= 4;
		l[2] += 1;
		LABL 0 0 0 c8;
	}
}

void IS-IS::MT-Capability::SPBV-ADDR()
{
	BBIT( 0x2, 0, 0x90, 0xc2, "Reserved:" );
	BBIT( 0x2, 0, 0x90, 0xc2, "SR:" );
	BBIT( 0xc, 0, 0x90, 0xc2, "SPVID:" );
	g[12] -= 2;
	l[2] = 1;
	while( g[12] > 0)
	{
		LABL 0 0 0 b7 MAC Address # ;
		DGLB( 0 , l[2], 0x07, 0x07 , " ");
		BBIT( 1, 0, 0x90, 0xc2, "Transmit:" );
		BBIT( 1, 0, 0x90, 0xc2, "Recieve:" );
		BBIT( 6, 0, 0x90, 0xc2, "Reserved:" );
		ETHR( 0, 0, 0x90, 0xc2, "MAC Address:" );
		g[12] -= 7;
		l[2] += 1;
		LABL 0 0 0 c8;
	}
}
//Type 222
void IS-IS::MT-ISN()
{
	BBIT 4 0 90 c2 Reserved;
	HBIT c 0 90 c2 MT ID;
	ETHR 7 0 90 c2 System ID:;
	HBYT 0 0 90 c2 Pseudonode #:;
	BBIT 18 0 90 c2 Default metric;
	DBYT 0 0 90 c2 Sub TLV Length:;
	g[10] -= 13;
	g[11] = g[10];
	while (g[11] !=0)
	{
		GBYT 0 9;
		CST# 0 9 0 b7 IS-IS::TLV_22_141_222::Names;
		CLSE 0 0 0 b7 Unknown Field;
		DBYT 0 9 90 c2 Field Code:;
		CLSE 0 0 0 14 Unknown Field Code;
		DBYT 0 c 90 c2 Field Length:;
		//SUBG c 81;
		g[11] -= 2;
		g[11] -= g[12];
		DGLB 0 c 90 c2 Unknown Field Data:;
		TSB# 0 9 0 0 IS-IS::TLV_22_141_222::Calls;
		if (g[12] != 0 )
		{
			DUMP 0 c 90 c2 Unknown Field Data:;
		}
		LABL 0 0 0 b8;
	}
	SUBG a 81;
	
	TNXT -IS-IS Variable Headers;
}


//Type 229
void IS-IS::M-Topologies()
{
	l[2] = 1;
	g[11] = g[10];
	while (g[11] !=0)
	{
		LABL 0 0 0 b7 Multi-Topology # ;
		DGLB( 0 , l[2], 0x07, 0x07 , " ");
		BBIT( 1, 0, 0x90, 0xc2, "Overload:" );
		BBIT( 1, 0, 0x90, 0xc2, "Attach:" );
		BBIT( 2, 0, 0x90, 0xc2, "reserved:" );
		HBIT( 0xc, 0, 0x90, 0xc2, "MT ID:" );
		LABL 0 0 0 c8;
		l[2] += 1;
		g[11] -= 2;
	}
	g[10] -= g[10];
	TNXT -IS-IS Variable Headers;
}
