	HBYT 0 0 a0 c2 Stop Output Char:;
*   output start char 
	HBYT 0 0 a0 c2 Start Output Char:;
*   input stop char 
	HBYT 0 0 a0 c2 Stop Input Char:;
*   input start char 
	HBYT 0 0 a0 c2 Start Input Char:;
*   data length -= 6 bytes 
	ADDG fffffffa 5;
*   Parameter n: 
	MOVE 0 2;
	TNXT -LAT_Aux Parm;
void LAT_Aux Parm;
*   global_1 -- parameter length 
*   global_2 -- parameter number 
*   global_3 -- number of slots 
*   global_4 -- current slot 
*   global_5 -- data length 
*   global_6 -- slot type etc. 
*   Parameter n: 
	ADDG 1 2;
	DGLB 0 2 1 b1 Parameter ;
	DBYT 0 7 a0 c2 Parameter Code:;
	CST# 0 7 0 14 Aux Param Types;
*   Parameter Length 
	DBYT 0 1 a0 c2 Parameter Length:;
*   Data length -= 2 
	ADDG fffffffe 5;
	TST# 0 7 0 10 LAT Aux Types;
