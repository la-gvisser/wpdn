	DUMP 0 2 a0 c2 Software ID String:;
	ADDG 1 2;
	TNXT -End Other Info;
void Processor;
	DBYT 0 1 a0 c2 Processor:;
	CEQU 0 1 0 14 System Processor;
	CEQU 1 1 0 14 Communication Processor;
*   decr packet count by prev bytes 
	TNXT -Other Info;
void Other Info;
*   no entries left 
	TGTE 2 10 0 0 MOP Done;
	DWRD 40000000 1 a0 b2 Info Type:;
	TLTE 64 1 0 0 itypeGE100;
*   display indexed string of in MOP Info Types 
	CST# 0 1 0 14 MOP Info Types;
*   branch to named indexed string 
	TST# 0 1 0 40 MOP Info Types;
	TLSE -Unknown Info Type;
