void DECnet SCP Connect;
	LABL 0 0 0 b1 SCP Connect - Session Control Protocol\SCP COnnect:;
*   branch if connect confirm 
	TEQU 2 1 0 0 SCP Conn Confirm;
	LABL 0 0 0 c3 Destination End User Name;
	SCP End User Name;
	LABL 0 0 0 c3 Source End User Name;
	SCP End User Name;
	HBYT 0 3 a0 c2 SCP Information:;
	CBIT 0 3 0 f4 RQSTRID, PASSWD & ACCOUNT Included;
	CLSE 0 0 0 f4 RQSTRID, PASSWD & ACCOUNT Not Included;
	CBIT 1 3 0 f4 USRDATA Included;
	CLSE 0 0 0 f4 USRDATA Not Included;
*   version bits 
	MOVE 3 84;
*   version bits 
	ANDG 60 4;
	CEQU 0 4 0 f4 SCP Version 1.0;
