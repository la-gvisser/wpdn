* Decoder version: 5.0.1.0

void DECnet MOP Dump/Load;
	LABL 0 0 0 b1 DECnet MOP Dump/Load;
	TNXT -MOP;
void DECnet MOP Remote Console;
	LABL 0 0 0 b1 DECnet MOP Remote Console\DECNet MOP:;
	TNXT -MOP;

void DECnet Phase IV;
	LABL 0 0 0 b1 DECnet Phase IV;
	DWRD 40000000 10 a0 c2 Length:;
	ADDG 2 10;
	TNXT -After Dlink;

void After Dlink;
*   get the flag byte without advancing 
	GBYT 0 1;
*   if LSB set, control pkt 
	TBIT 7 1 0 0 Padding;
*   if LSB set, control pkt 
	TBIT 0 1 0 0 Control Packet;
*   if next LSB set, data pkt 
	TBIT 1 1 0 0 Data Packet;
	LABL 0 0 0 b1 Unknown DECnet Phase IV Type;
	DUMP 0 10 a0 c2 DECnet Data Area:;

void Data Packet;
	LABL 0 0 0 b1 Packet Route Header - ;
	TBIT 2 1 0 0 Long Data Packet;
	LABL 0 0 0 31 Short Format;
	BBYT 0 2 90 c2 Flags:;
	CBIT 4 2 0 f4 Returning Packet ;
	CBIT 3 2 0 f4 Return Requested;
	CLSE 0 0 0 f4 No Return Requested;
	LABL 0 0 0 f4 SFDP; 
	DWRD 40000000 0 90 c2 Destination Address:;
	DWRD 40000000 0 90 c2 Source Address:;
*   get the next byte without advancing 
	GBYT 0 2 0 82;
	TLTE 40 2 0 0 Bad Format;
	DBYT 0 0 90 c2 Visit Count:;
	DUMP 0 10 a0 c2 DECnet Data Area:;

void Long Data Packet;
	LABL 0 0 0 31 Long Format;
	BBYT 0 2 90 c2 Flags:;
	CBIT 6 2 0 f4 Discarded ;
	CBIT 5 2 0 f4 Intra-Ethernet Packet ;
	CBIT 4 2 0 f4 Returning Packet ;
	CBIT 3 2 0 f4 Return Requested;
	CLSE 0 0 0 f4 No Return Requested;
	LABL 0 0 0 f4 LFDP;
	DBYT 0 0 90 c2 Dest. Area:;
	DBYT 0 0 90 c2 Dest. Subarea:;
	ETHR 0 0 90 c2 Dest. ID:;
	DBYT 0 0 90 c2 Source Area:;
	DBYT 0 0 90 c2 Source Subarea:;
	ETHR 0 0 90 c2 Source ID:;
	DBYT 0 0 90 c2 Next Level 2 Router:;
	DBYT 0 0 90 c2 Visit Count:;
	DBYT 0 0 90 c2 Service Class:;
	DBYT 0 0 90 c2 Protocol Type:;
	TNXT -DECnet NSP;
void Bad Format;
	DUMP 0 0 a0 c2 Remaining Data in Badly Formatted Packet:;
void Control Packet;
	LABL 0 0 0 b1 Routing Layer Control Packet;
	BBIT 4 0 a0 c2 Reserved:;
	DBIT 3 2 a0 c2 Control Type:;
*   yields index into str# 
	ADDG 1 2;
*   display indexed string in Types 
	CST# 0 2 0 14 Control Packet Types;
	BBIT 1 0 a0 c2 Control Packet Indicator:;
*   decr count by flag byte 
*   xfer to indexed string of in Types 
	TST# 0 2 0 0 Control Packet Types;
	CLSE 0 0 0 14 Unknown;
	DUMP 0 10 a0 c2 Data Area:;
void Initialization;
	LABL 0 0 0 b1 Initialization Message;
	DWRD 40000000 0 a0 c2 Source Node:;
	BBIT 4 0 a0 c2 Unused (must be zero):;
	BBIT 1 2 a0 c2 Blocking:;
	CEQU 0 2 0 14 Not Requested;
	CLSE 0 0 0 14 Requested;
	BBIT 1 2 a0 c2 Verification Message:;
	CEQU 0 2 0 14 Not Required;
	CLSE 0 0 0 14 Required;
	DBIT 2 2 a0 c2 Node Type:;
*   yields index into str# 
	ADDG 1 2;
*   display indexed string in Types 
	CST# 0 2 0 14 DECnet Node Types;
	DWRD 40000000 0 a0 c2 Block Size:;
	DBYT 0 0 a0 c2 Version Number:;
	DBYT 0 0 a0 c2 ECO Number:;
	DBYT 0 0 a0 c2 User ECO Number:;
	DWRD 40000000 0 a0 c2 Hello Timer:;
	HBYT 0 1 a0 c2 Reserved Image Count:;
	DUMP 0 1 a0 c2 Reserved Image Field:;
void Verification;
	LABL 0 0 0 b1 Verification Message;
	DWRD 40000000 0 a0 c2 Source Node:;
	HBYT 0 1 a0 c2 Function Value Image Count:;
	DUMP 0 1 a0 c2 Function Value Image Field:;
void Hello and Test;
	LABL 0 0 0 b1 Hello and Test Message;
	DWRD 40000000 0 a0 c2 Source Node:;
	HBYT 0 1 a0 c2 Test Data Image Count:;
	DUMP 0 1 a0 c2 Test Data Image Field:;
void Level 1 Routing;
	LABL 0 0 0 b1 Level 1 Routing Message;
	DWRD 40000000 0 a0 c2 Source Node:;
	HBYT 0 0 a0 c2 Reserved:;
*   decr count by these two fields byte 
	TNXT -L1 Segment Loop;
void L1 Segment Loop;
	LABL 0 0 0 b1 Segment Information;
	DWRD 40000000 1 a0 c2 Count of IDs in Segment:;
	DWRD 40000000 5 a0 c2 Starting ID:;
*   decr count 
	TNXT -L1 Seg Loop Continue;
void L1 Seg Loop Continue;
	DGLB 0 5 1 b1 Routing Info ID #;
	CGTE 9 5 1 1  ;
	CGTE 63 5 1 1  ;
	CGTE 3e7 5 1 1  ;
	CGTE 270f 5 1 1  ;
	ADDG 1 5;
	GBYT 0 e;
	INCR 1;
	GBYT 0 c;
	INCR 1;
	MOVE c 8d;
	DIVG 4 c 0 2;
	ANDG 1f c 0 2;
	DGLB 0 c 0 12 Hops:;
	CGTE 9 c 0 2  ;
	ANDG 3 d;
	MULG 100 d;
	ADDG d 8e;
	DGLB 0 e 0 12 Cost:;
*   decr remaining ID count 
	SUBG 1 1;
	TLTE 1 1 0 0 L1 Seg Loop Continue;
	TGTE 2 10 0 0 DNA Checksum;
	TNXT -L1 Segment Loop;

void Level 2 Routing;
	LABL 0 0 0 b1 Level 2 Routing Message;
	DWRD 40000000 0 a0 c2 Source Node:;
	HBYT 0 0 a0 c2 Reserved:;
	TNXT -L2 Segment Loop;

void L2 Segment Loop;
	LABL 0 0 0 b1 Segment Information;
	DWRD 40000000 1 a0 c2 Count of Areas in Segment:;
	DWRD 40000000 5 a0 c2 Starting Area:;
	TNXT -L2 Seg Loop Continue;

void L2 Seg Loop Continue;
	DGLB 0 5 1 b1 Routing Info Area #;
	CGTE 9 5 1 1  ;
	CGTE 63 5 1 1  ;
	CGTE 3e7 5 1 1  ;
	CGTE 270f 5 1 1  ;
	ADDG 1 5;
	GBYT 0 e;
	INCR 1;
	GBYT 0 c;
	INCR 1;
	MOVE c 8d;
	DIVG 4 c 0 2;
	ANDG 1f c 0 2;
	DGLB 0 c 0 12 Hops:;
	CGTE 9 c 0 2  ;
	ANDG 3 d;
	MULG 100 d;
	ADDG d 8e;
	DGLB 0 e 0 12 Cost:;
	SUBG 1 1;
	TLTE 1 1 0 0 L2 Seg Loop Continue;
	TGTE 2 10 0 0 DNA Checksum;
	TNXT -L2 Segment Loop;
void Ethernet Router Hello;
	LABL 0 0 0 b1 Ethernet Router Hello Message;
	DBYT 0 0 a0 c2 Version Number:;
	DBYT 0 0 a0 c2 ECO Number:;
	DBYT 0 0 a0 c2 User ECO Number:;
	ETHR 0 0 a0 c2 Transmitter System ID:;
	HBYT 0 1 a0 c2 Info:;
	CBIT 6 1 0 f4 Blocking Requested;
	CBIT 5 1 0 f4 No Multicast;
	CLSE 0 1 0 f4 Multicast OK;
	CBIT 4 1 0 f4 Verification Failed;
	CBIT 3 1 0 f4 Reject;
	CBIT 2 1 0 f4 Verification Required;
	CBIT 1 1 0 f4 Level 1 Router;
	CBIT 0 1 0 f4 Level 2 Router;
	DWRD 40000000 0 a0 c2 Block Size:;
	DBYT 0 0 a0 c2 Priority:;
	HBYT 0 0 a0 c2 Area (reserved):;
	DWRD 40000000 0 a0 c2 Hello Timer:;
	HBYT 0 0 a0 c2 MPD (reserved):;
	HBYT 0 1 a0 c2 Ethernets Image Count:;
*  *** TNXT to mcr enet router hello ***
	DUMP 0 1 a0 c2 Ethernets Image Field:;
void Ethernet Endnode Hello;
	LABL 0 0 0 b1 Ethernet Endnode Hello Message;
	DBYT 0 0 a0 c2 Version Number:;
	DBYT 0 0 a0 c2 ECO Number:;
	DBYT 0 0 a0 c2 User ECO Number:;
	ETHR 0 0 a0 c2 Transmitter System ID:;
	HBYT 0 1 a0 c2 Info:;
	CBIT 6 1 0 f4 Blocking Requested;
	CBIT 5 1 0 f4 No Multicast;
	CLSE 0 1 0 f4 Multicast OK;
	CBIT 4 1 0 f4 Verification Failed;
	CBIT 3 1 0 f4 Reject;
	CBIT 2 1 0 f4 Verification Required;
	LABL 1 1 0 f4 Endnode;
	DWRD 40000000 0 a0 c2 Block Size:;
	HBYT 0 0 a0 c2 Area (reserved):;
	HEX# 8 0 a0 c2 Verification Seed:;
	ETHR 0 0 a0 c2 Neighbor System ID:;
	DWRD 40000000 0 a0 c2 Hello Timer:;
	HBYT 0 0 a0 c2 MPD (reserved):;
	HBYT 0 1 a0 c2 Test Data Image Count:;
	DUMP 0 1 a0 c2 Test Data Image Field:;
void DNA Checksum;
	HWRD 40000000 0 a0 c2 Checksum:;

void Padding;
*   flag bit w/ count incl cnt byte 
	HBYT 0 1 a0 c2 Padding Flag and Count:;
*   yields count of pad bytes: no bit nor count byte 
	ADDG ffffff7f 1;
	DUMP 0 1 a0 c2 Padding:;
*   this line & next decr packet count by pad bytes 
	MULG ffffffff 1;
*   try it again 
	TNXT -After Dlink;

void DECnet Diagnostic Protocol;
	LABL 0 0 0 b1 DECnet Diagnostic Protocol;
	DUMP 0 10 a0 c2 DECnet Data Area:;
void DECnet LAT;
	LABL 0 0 0 b1 DECnet LAT - Local Area Transport\DECnet LAT:;
	DBIT 6 2 a0 c2 Message Type:;
	CST# ffffffff 2 0 14 Message Types;
	CLSE 0 2 0 14 Unknown;
	BBIT 2 3 a0 c2 Flags:;
	CBIT 1 3 0 f4 To Host;
	CLSE 1 3 0 f4 From Host;
	CBIT 0 3 0 f4 Response Requested;
	CLSE 0 3 0 f4 No Response Requested;
	TST# ffffffff 2 0 0 LAT Message Types;
	DUMP 0 0 a0 c2 Unknown Data:;
void LAT_Solicit_Cmd;
	LABL 0 0 0 b1 Solicit Command;
	LAT_Solicit_Hdr;
	DWRD 40000000 0 d0 c2 Request Identifier:;
	DWRD 40000000 0 d0 c2 Entry Identifier:;
	DBYT 0 2 d0 c2 Command Type:;
	CST# 0 2 0 14 LAT Solicit Cmd Types;
	DBYT 0 2 d0 c2 Command Modifier:;
	CBIT 0 2 0 f4 Send Status Periodically;
	CLSE 0 0 0 f4 Don't Send Status Periodically;
	CBIT 1 2 0 f4 Send Status When Queue Depth Changes;
	CLSE 0 0 0 f4 Don't Send Status When Queue Depth Changes;
	PSTR 0 0 d0 c2 Destination Object Node Name:;
	DBYT 0 7 d0 c2 Node Group Length:;
	LABL 0 0 0 c2 Source Node Subject Groups:           ;
	MOVE 0 1;
	MOVE 0 3;
	LAT Group Enable;
	PSTR 0 0 d0 c2 Source Subject Node Name:;
	PSTR 0 0 d0 c2 Source Subject Port Name:;
	PSTR 0 0 d0 c2 Source Subject Description:;
	PSTR 0 0 d0 c2 Destination Object Service Name:;
	PSTR 0 0 d0 c2 Destination Object Port Name:;
	TNXT -LAT_Solicit Params;
void LAT_Solicit_Status;
	LABL 0 0 0 b1 Solicit Status;
	LAT_Solicit_Hdr;
	DWRD 40000000 0 d0 c2 Status Retransmit Timer:;
	DBYT 0 3 d0 c2 Entries Counter:;
	GBYT 0 4;
	PSTR 0 0 d0 c2 Subject Node Name:;
	SBIT 0 4;
	SKIP 80000001;
	HBYT 0 0 d0 c2 Padding Byte:;
	LAT_Solicit_Entries;
	DWRD 40000000 0 d0 c2 Entries Counter:;
	TNXT -LAT_Solicit Params;
void LAT_Solicit_Entries;
	SNEQ 0 3;
	SKIP 1;
	TRTS;
	SUBG 1 3;
	DBYT 0 4 b0 c2 Entry Length:;
	HBYT 0 1 b0 e2 Entry Status:;
	CBIT 0 1 0 f4 No Additional Information Provided;
	CBIT 1 1 0 f4 Request Is Already Queued;
	CBIT 2 1 0 f4 Entry Is Accepted For Processing;
	CBIT 3 1 0 f4 Periodic Status Returns Are Not Supported;
	CBIT 4 1 0 f4 Queue Depth Status Is Not Supported;
	CBIT 7 1 0 f4 Operation Was NOT Successfully Completed;
	CLSE 0 0 0 f4 Operation Successfully Completed;
	DBYT 0 1 b0 e2 Entry Error:;
	CST# ffffffff 1 0 14 LAT Utility Reason;
	HBYT 0 0 b0 e2 Reserved:;
	DWRD 40000000 0 b0 e2 Request Identifier:;
	DWRD 40000000 0 b0 e2 Entry Identifier:;
	DWRD 40000000 0 b0 e2 Elapsed Queue Time:;
	DWRD 40000000 0 b0 e2 Minimum Queue Position:;
	DWRD 40000000 0 b0 e2 Maximum Queue Position:;
	PSTR 0 0 b0 e2 Object Service Name:;
	PSTR 0 0 b0 e2 Object Port Name:;
	PSTR 0 0 b0 e2 Subject Description:;
	SBIT 0 4;
	SKIP 80000001;
	HBYT 0 0 b0 e2 Padding Byte:;
	TNXT -LAT_Solicit_Entries;
void LAT_Solicit_Msg;
	LABL 0 0 0 b1 Solicit Message;
	LAT_Solicit_Hdr;
	DWRD 40000000 0 d0 c2 Solicit Identifier:;
	DWRD 40000000 0 d0 c2 Response Timer:;
	PSTR 0 0 d0 c2 Destination Node Name:;
	DBYT 0 7 d0 c2 Node Group Length:;
	LABL 0 0 0 c2 Source Node Groups Enabled:           ;
	MOVE 0 1;
	MOVE 0 3;
	LAT Group Enable;
	PSTR 0 0 d0 c2 Source Node Name:;
	PSTR 0 0 d0 c2 Destination Service Name:;
	TNXT -LAT_Solicit Params;
void LAT_Solicit_Response;
	LABL 0 0 0 b1 Solicit Response;
	LAT_Solicit_Hdr;
	DWRD 40000000 0 d0 c2 Solicit Identifier:;
	HWRD 40000000 1 d0 c2 Response Status:;
	CBIT 1 1 0 14 Requested Service Is Not Offered;
	DWRD 40000000 1 d0 c2 Source Node Status:;
	CST# ffffffff 1 0 f4 LAT Svc Rsp Stt;
	DWRD 40000000 0 d0 c2 Source Node Address:;
	DWRD 40000000 0 d0 c2 Node Multicast Timer:;
	PSTR 0 0 d0 c2 Destination Node Name:;
	DBYT 0 7 d0 c2 Node Group Length:;
	LABL 0 0 0 c2 Source Node Groups:           ;
	MOVE 0 1;
	MOVE 0 3;
	LAT Group Enable;
	PSTR 0 0 d0 c2 Source Node Name:;
	PSTR 0 0 d0 c2 Source Node Description:;
	DBYT 0 5 d0 c2 Service Count:;
	LAT Service Entry;
	TNXT -LAT_Solicit Params;
void LAT Service Entry;
	SNEQ 0 5;
	SKIP 1;
	TRTS;
	SUBG 1 5;
	DBYT 0 4 b0 c2 Service Entry Length:;
	PSTR 0 0 b0 e2 Service Class:;
	DBYT 0 0 b0 e2 Service Status:;
	DBYT 0 0 b0 e2 Service Rating:;
	DBYT 0 7 b0 c2 Service Group Length:;
