void NSP Msg Struct;
	DWRD 40000000 0 a0 c2 Logical Link Dst. Addr.:;
	DWRD 40000000 0 a0 c2 Logical Link Src. Addr.:;
*   get lo byte next field without advancing 
	GBYT 0 3;
	INCR 1;
*   get hi byte next field without advancing 
	GBYT 0 4;
*   is first optional field there? 
	SBIT 7 4;
*   skip if not 
	SKIP 8000001b;
*   just the number part 
	ANDG f 4;
*   shift it up 
	MULG 100 4;
*   just the number part 
	ADDG 4 83;
*   get hi byte next field again 
	GBYT 0 4;
	DIVG 10 4;
*   just the qual part 
	ANDG 7 4;
	DGLB 0 4 a0 c2 Ack Qualifier:;
*   display indexed string 
	CST# ffffffff 4 0 14 NSP Ack Qualifier Types;
	CLSE 0 0 0 14 Reserved;
	DGLB 0 3 a0 c2 Ack Message Number:;
	INCR 1;
*   get lo byte next field without advancing 
	GBYT 0 3;
	INCR 1;
*   get hi byte next field without advancing 
	GBYT 0 4;
*   is 2nd optional field there? 
	SBIT 7 4;
*   skip if not 
	SKIP 8000000c;
*   just the number part 
	ANDG f 4;
*   shift it up 
	MULG 100 4;
*   just the number part 
	ADDG 4 83;
*   get hi byte next field again 
	GBYT 0 4;
	DIVG 10 4;
*   just the qual part 
	ANDG 7 4;
	DGLB 0 4 a0 c2 Ack Qualifier:;
*   display indexed string 
	CST# 1 4 0 14 NSP Other Ack Qual Types;
	CLSE 0 0 0 14 Reserved;
	DGLB 0 3 a0 c2 Message Ack'd:;
	INCR 2;
	DECR 1;
	TRTS;
void NSP Data Seg Num;
*   get lo byte next field without advancing 
	GBYT 0 3;
	INCR 1;
*   get hi byte next field without advancing 
	GBYT 0 4;
	MULG 100 4 0 14;
*   bytes now swapped 
	ADDG 4 83 0 14;
*   2nd copy 
	MOVE 3 84;
*   just the number part 
	ANDG fff 3;
	DGLB 0 3 a0 c2 Data Segment Number:;
	CBIT c 4 0 14 Delayed ACK Allowed;
	CLSE 0 0 0 14 Do Not Delay ACK;
	INCR 1;
	TRTS;
