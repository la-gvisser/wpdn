	BBIT 4 7 a0 c2 Parity:;
	MOVE 7 88;
	DIVG 2 7;
	ANDG 3 7;
	ADDG 1 7;
	CST# 0 7 0 14 Parity;
	ANDG 1 8;
	CEQU 0 8 0 4 , Disabled;
