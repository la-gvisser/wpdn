void LAT_Solicit Params;
	DBYT 0 7 a0 c2 Parameter Code:;
	TEQU 0 7 0 0 AFP The End;
	DBYT 0 1 a0 c2 Parameter Length:;
	SEQU 0 1;
	SKIP 1;
	DUMP 0 1 a0 c2 Parameter Data:;
	TNXT -LAT_Solicit Params;
void LAT_Run;
*   message type = 0, 1, or 2 
	LABL 0 0 0 b1 Run;
*   number of slots 
	DBYT 0 3 a0 c2 Number of Slots:;
*   destination link id 
	DWRD 40000000 0 a0 c2 Destination ID:;
*   source link id 
	DWRD 40000000 0 a0 c2 Source ID:;
*   sequence number 
	DBYT 0 0 a0 c2 Sequence Number:;
*   Ack number 
	DBYT 0 0 a0 c2 Acknowledgment Number:;
*   Slot Data 
*   global_3 -- number of slots 
*   global_4 -- current slot 
	MOVE 0 4;
	TLTE 1 3 0 0 LAT Slot Data;
void LAT Slot Data;
*   global_3 -- number of slots 
*   global_4 -- current slot 
*   global_5 -- data length 
*   global_6 -- slot type etc. 
*   *** NOTE **** the pointer is not byte aligned at the end of this *** 
	LABL 0 0 0 b1 "Local Area Transport Data (Slot ";
	DGLB 0 4 1 31;
	LABL 0 0 0 31 ")";
*   destination sublink = 1 byte 
	DBYT 0 0 a0 c2 Destination Sublink ID:;
*   Source sublink = 1 byte 
	DBYT 0 0 a0 c2 Source Sublink ID:;
*   data length = 1 byte 
	DBYT 0 5 a0 c2 Data Length:;
	MOVE 5 90 0 40;
*   slot type / credits 
	DBIT 4 6 a0 c2 Slot Type:;
*   next slot 
	CST# ffffffff 6 0 14 Slot Types;
	TST# ffffffff 6 0 0 LAT Slot Types;
void LAT_Data;
*   global_3 -- number of slots 
*   global_4 -- current slot 
*   global_5 -- data length 
*   global_6 -- slot type etc. 
*   *** NOTE **** the pointer is not byte aligned at this stage *** 
*   credits 
	DBIT 4 0 a0 c2 Credits:;
*   data 
	DUMP 0 5 a0 c2 Slot Data:;
	TNXT -LAT_Next Slot;

void LAT_Next Slot;
*   global_3 -- number of slots 
*   global_4 -- current slot 
*   global_5 -- data length 
*   global_6 -- slot type etc. 
*   if length was odd, increment packet pointer 
	INCR 0 10;
*   next slot -- current slot ++, number of slots --, if 0 <= number of slots then next slot 
	ADDG 1 4;
	ADDG ffffffff 3;
	TLTE 1 3 0 0 LAT Slot Data;

void LAT_Unknown Data;
*   global_3 -- number of slots 
*   global_4 -- current slot 
*   global_5 -- data length 
*   global_6 -- slot type etc. 
*   *** NOTE **** the pointer is not byte aligned at this stage *** 
	DBIT 4 0 a0 c2 Unknown:;
*   data 
	DUMP 0 5 a0 c2 Unknown Data:;
	TNXT -LAT_Next Slot;
void LAT_Start;
*   global_3 -- number of slots 
*   global_4 -- current slot 
*   global_5 -- data length 
*   global_6 -- slot type etc. 
*   *** NOTE **** the pointer is not byte aligned at this stage *** 
	DBIT 4 0 a0 c2 Credits:;
*   service class = 1 byte 
	DBYT 0 0 a0 c2 Service Class:;
*   Minimum Attention slot size = 1 byte 
	DBYT 0 0 a0 c2 Min. Attn. Slot Size:;
*   Minimum data slot size = 1 byte 
	DBYT 0 0 a0 c2 Min. Data Slot Size:;
*   Data Length -= (4 bytes + 1 byte string length) 
	ADDG fffffffb 5;
*   Data Length -= next string length 
	GBYT 0 7;
*   Destination service name = Pstr 
	MULG ffffffff 7;
*   Data Length -= next string length 
	ADDG 7 85;
*   Source description = Pstr 
	LABL 0 0 0 c2 Dest. Service Name:;
*   Parameter n: 
	Non-null Pascal String;
	GBYT 0 7;
	MULG ffffffff 7;
	ADDG ffffffff 7;
	ADDG 7 85;
	LABL 0 0 0 c2 Source Description:;
	Non-null Pascal String;
	MOVE 0 2;
	TNXT -LAT_Start Parm;
void LAT_Start Parm;
*   global_1 -- parameter length 
*   global_2 -- parameter number 
*   global_3 -- number of slots 
*   global_4 -- current slot 
*   global_5 -- data length 
*   global_6 -- slot type etc. 
*   Parameter n: 
	ADDG 1 2;
	DGLB 0 2 1 b1 Parameter ;
	DBYT 0 7 a0 c2 Parameter Code:;
	CST# 0 7 0 14 Start Param Types;
*   Parameter Length 
	DBYT 0 1 a0 c2 Parameter Length:;
*   Data length -= 2 
	ADDG fffffffe 5;
	TST# 0 7 0 10 LAT Start Types;
void LAT_Flags;
*   global_1 -- parameter length 
*   global_2 -- parameter number 
*   global_3 -- number of slots 
*   global_4 -- current slot 
*   global_5 -- data length 
*   global_6 -- slot type etc. 
*   flag 
	DBYT 0 7 a0 c2 Flag:;
	CBIT 1 3 0 f4 Logins not allowed;
	CLSE 1 3 0 f4 Logins allowed;
	CBIT 0 3 0 f4 Line is remote;
	CLSE 0 3 0 f4 Line is local;
*   skip over next byte 
	INCR 1;
*   slot length -= 2 
	ADDG fffffffe 5;
*   if slot length == 0 then next slot 
	TGTE 1 5 0 0 LAT_Next Slot;
*   else next parameter 
	TNXT -LAT_Start Parm;
void LAT_Dest Node;
*   global_1 -- parameter length 
*   global_2 -- parameter number 
*   global_3 -- number of slots 
*   global_4 -- current slot 
*   global_5 -- data length 
*   global_6 -- slot type etc. 
*   length of parameter was length of string, so backup to length of string 
	LABL 0 0 0 14 Node Name Length;
	DECR 1;
*   Data Length -= next string length 
	GBYT 0 7;
	MULG ffffffff 7;
	ADDG ffffffff 7;
	ADDG 7 85;
*   destination node port name 
	PSTR 0 0 a0 c2 Destination Node Name:;
	CEQU ffffffff 7 0 14 No String Present;
*   if slot length == 0 then next slot 
	TGTE 1 5 0 0 LAT_Next Slot;
*   else next parameter 
	TNXT -LAT_Start Parm;
void LAT_Source Node;
*   global_1 -- parameter length 
*   global_2 -- parameter number 
*   global_3 -- number of slots 
*   global_4 -- current slot 
*   global_5 -- data length 
*   global_6 -- slot type etc. 
*   length of parameter was length of string, so backup to length of string 
	LABL 0 0 0 14 Node Name Length;
	DECR 1;
*   Data Length -= next string length 
	GBYT 0 7;
	MULG ffffffff 7;
	ADDG ffffffff 7;
	ADDG 7 85;
*   destination node port name 
	PSTR 0 0 a0 c2 Source Node Name:;
	CEQU ffffffff 7 0 14 No String Present;
*   if slot length == 0 then next slot 
	TGTE 1 5 0 0 LAT_Next Slot;
*   else next parameter 
	TNXT -LAT_Start Parm;
void LAT_Service Group;
*   global_1 -- parameter length 
*   global_2 -- parameter number 
*   global_3 -- number of slots 
*   global_4 -- current slot 
*   global_5 -- data length 
*   global_6 -- slot type etc. 
*   length of parameter was length of service groups, so backup to length of string 
	LABL 0 0 0 14 Node Group Length;
	DECR 1;
*   Data Length -= service group length 
	GBYT 0 7;
	MULG ffffffff 7;
	ADDG ffffffff 7;
	ADDG 7 85;
*   destination Source Service Group 
	LABL 0 0 a0 c6 Service Group Codes:;
	GBYT 0 7;
	INCR 1;
	LAT_Node Groups;
*   if slot length == 0 then next slot 
	TGTE 1 5 0 0 LAT_Next Slot;
*   else next parameter 
	TNXT -LAT_Start Parm;
void LAT_Unknown Start;
*   global_1 -- parameter length 
*   global_2 -- parameter number 
*   global_3 -- number of slots 
*   global_4 -- current slot 
