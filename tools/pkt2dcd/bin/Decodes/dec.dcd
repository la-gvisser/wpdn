	DBYT 0 4 a0 c2 Requestor ID Length:;
	DUMP 0 4 a0 40   ;
	DBYT 0 4 a0 c2 Password Length:;
	DUMP 0 4 a0 40   ;
	DBYT 0 4 a0 c2 Account ID Length:;
	DUMP 0 4 a0 40   ;
*   connect data included? 
	SBIT 1 3;
*   skip if not 
	SKIP 80000004;
	DBYT 0 4 a0 c2 Connect Data Length:;
	DUMP 0 4 a0 40   ;
*   placeholder only - no-op 
	SBIT 0 3;
