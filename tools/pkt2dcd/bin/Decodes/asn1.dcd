// Decoder version: 5.0.1.0

// ASN1 Routines


// out params: g1=type, g2=length, g3=value, g4=enum, g5=class, g6=return, g7=use functable
// g11=enum testing;

void ASN1_Init;
	MOVE 0 7;
	MOVE 0 8;
	MOVE 0 6;
	MOVE 0 4;
	MOVE 1 11;  //turn on enum testing

void ASN1_ValueFuncOn;
	MOVE 1 7;
	MOVE 1 8;

void ASN1_DisableEnum;
	MOVE 0 11;  //turn off enum testing

void ASN1_Value;
	//feature to just return in case somebody else handled it
	SEQU 0 6;
	SKIP 2;
	MOVE 0 6;
	TRTS;
	
	ASN1_Value2;

	SEQU 1 8;
	SKIP 6;
	SEQU 6 2;  //not for OIDS
	SKIP 4;
	SEQU 0 7;
	SKIP 2;
	ASN1_ValueFunc;
	MOVE 0 7;  //only do it once
	MOVE 0 8;  //only do it once

	TEQU 0 11 0 0 RETURN;  //enum testing is off
	INCR 2;  // skip the next enum
	ASN1_EnumTest;

void ASN1_ValueFunc;
	TST# 0 7 0 0 ASN1_ValueFuncStr;

// these are functions that output a special message after the value

str# ASN1_ValueFuncStr;
	OID_ifTypeFunc;
	OID_ifStatusFunc;
	OID_probeCapabilitiesFunc;
	OID_ipRouteProtoFunc;
	OID_tcpConnStateFunc;
	OID_egpNeighStateFunc;
	OID_SystemServicesFunc;

void ASN1_Value2;
	ASN1_Enum;
	LABL 0 0 0 2 :;

	ASN1_Type;

	SNEQ 5 1;					// could be OPTIONAL / NULL
	SKIP 3;
	LABL 0 0 0 54 NULL;
	DBYT 0 2 5;
	TRTS;

	SNEQ 0 5;					// could be OPTIONAL / NULL
	SKIP 2;
	LABL 0 0 0 54 NULL;
	TRTS;

	// TAG part of TLV format
	// test for contex-specific class
	// Nothing for Universal or Private class
	SBIT 6 5;						// APPLICATION class if true
	SKIP 4;
	//TBIT 7 5 0 0 RETURN;			// context-specific class if true
	SBIT 7 5;
		SKIP 80000002;
		ASN1_ValueDisplayContextSpecific;	
		TRTS;
	
	// LENGTH part of TLV format
	ASN1_Length;

	SNEQ 0 2;					// could be OPTIONAL / NULL
	SKIP 2;
	LABL 0 0 0 54 NULL;
	TRTS;

	//do we have enough bytes
	SLTE 2 90;
	SKIP 2;
	// HEX# 0 10 5;
	// BD: Do not know why HEX was used, left in beacuase there might be 
	// BD: Another protocol I don't know that needs it
	DUMP 0 10 90 5;
	TRTS;

	// Value part of TLV format
	ASN1_ValueDisplay;

	TRTS;


void ASN1_ValueDisplay;
	// test for application Class
	SBIT 7 5;
	SKIP 1;
	TBIT 6 5 0 0 ASN1_Application;

	TEQU  1  1 0 0 ASN1_Boolean;
	TEQU  2  1 0 0 ASN1_Integer;
	TEQU  3  1 0 0 ASN1_BitString;
	TEQU  4  1 0 0 ASN1_OctetString;
	TEQU  6  1 0 0 ASN1_OID;
	TEQU 13  1 0 0 ASN1_PrintableString;
	// BD: Actually Teletex String
	TEQU 14  1 0 0 ASN1_PrintableString;
	// BD: Actually IA5String
	TEQU 16  1 0 0 ASN1_PrintableString;
	TEQU 17  1 0 0 ASN1_UTCTime;
	TEQU 18  1 0 0 ASN1_GeneralizedTime;
	TEQU 1b  1 0 0 ASN1_GeneralString;
	// BD: Actually BMPString
	TEQU 1e  1 0 0 ASN1_PrintableString;
	

	// if the length is less then 5 then integer
	TGTE 5 1 0 0 ASN1_Integer;

	// otherwise
	DUMP 0 2 90 5;

// BD: dumps data for display for context specific classes in ASN1
void ASN1_ValueDisplayContextSpecific;	
	DECR 2;									// BD: Some funky offset some where, fixing it will need to find		
	ASN1_TypeLength;
	BREM 0 36;
	SLTE 36 82;								// BD: Deals with some Kerberos packets that fragmented, should help other
	SKIP 80000006;							// BD: fragmented packets
		MOVE 2 B7;
		SUBG 36 B7; 
		SEQU 0 37;							// BD: if packet data croses over length of packet, for fragmented packets
		SKIP 2;								// BD: Dump data and account for FCS
			SUBG 37 82;
			SUBG 4 2;
	DUMP 0 2 90 5;
	TRTS;


//out params: g[1]=low 5 bits g[5]=whole byte

void ASN1_Type;
	DBYT 0 1 5;
	MOVE 1 85;  	// save class
	ANDG  1f 1;  	// only interested in the 5 low bits (for now


void ASN1_TypeLength;
	ASN1_Type;
	ASN1_Length;

void ASN1_SkipValue;
	ASN1_Type;

	TEQU 5 1 0 0 RETURN;
	ASN1_Length;

	// do not skip a whole sequence or set
	TEQU 10 1 0 0 RETURN;
	TEQU 11 1 0 0 RETURN;

	INCR 0 2;

void ASN1_SkipTag;
	ASN1_Type;
	TEQU 5 1 0 0 RETURN;
	ASN1_Length;

void ASN1_IPAddress;
	IPLG 0 0 90;

void ASN1_Counter;
	ASN1_Integer;
	LABL 0 0 0 14 Counter;

void ASN1_Gauge32;
	ASN1_Integer;
	LABL 0 0 0 14 Gauge32;

void ASN1_TimeTicks;
	ASN1_Integer;
	LABL 0 0 0 14 TimeTicks (hundredths of seconds since an epoch);

void ASN1_Opaque;
	ASN1_OctetString;
	LABL 0 0 0 14 Opaque;

void ASN1_Counter64;
	ASN1_Integer;
	LABL 0 0 0 14 Counter64;

void ASN1_Application;
	TEQU 0 1 0 0 ASN1_IPAddress;
	TEQU 1 1 0 0 ASN1_Counter;
	TEQU 2 1 0 0 ASN1_Gauge32;
	TEQU 3 1 0 0 ASN1_TimeTicks;
	TEQU 4 1 0 0 ASN1_Opaque;
	TEQU 6 1 0 0 ASN1_Counter64;

	// test for application class
	SBIT 7 5;
	SKIP 5;
	SBIT 6 5;
	SKIP 80000003;
	HEX# 0 2 90;
	LABL 0 0 0 14 Unknown application class;
	TRTS;

// This function does the same things as parse length except it just stores the amount
// of length octets in g3
// WARNING: This depends on caller to put packet pointer in proper place, put packet pointer
// over length
// out params: g[51]=number of length octets
void ASN1_ParseLengthOctets;
	GBYT 0 33 5;
	ANDG 80 33;
	SNEQ 80 33;
	SKIP 2;
		GBYT 0 33 5;
		ANDG 7f 33;		// if long form then bits 7 to 1 is the number subsequent octets
	TRTS;


void ASN1_ParseLength;
	DBYT 0 2 5;

	SBIT 7 2;	// test for long form in bit 8
	SKIP 80000004;
	MOVE  2 83; // if long form then bits 7 to 1 is the number subsequent octets
	ANDG  7f 3;
	MOVE  0 2;
	ASN1_LengthLoop;
	

void ASN1_Length;
	ASN1_ParseLength;
	TRTS;

// in  params: g3=number of octets
// out params: g2=length
// loc params: g5=current byte

void ASN1_LengthLoop;
	GBYT  0  5;
	ANDG f0  5;
	DIVG 10  5;
	MULG 10  2;	// shift left
	ADDG  5 82;	// add new nibble	

	GBYT  0  5;
	ANDG  f  5;
	MULG 10  2;	// shift left
	ADDG  5 82;	// add new nibble	
  
	INCR  1;

	SUBG 1 3; 
	TEQU 0 3 0 0 RETURN; // are we done?

	TNXT -ASN1_LengthLoop;
	
	TRTS; 


void ASN1_EnumTest;
	TEQU 0 11 0 0 RETURN;  //enum testing is off
	DECR  2;   // go back because we usually skip it
	GBYT  0 4; // get the id
	ANDG 1f 4; // we only look at the low 5 bits
	GBYT  0 2;
	ANDG e0 2;
	SEQU a0 2;
	SKIP 1;
	MOVE ff 4;
	INCR  2;   // go back to where we came from


void ASN1_Enum;
	TEQU 0 11 0 0 RETURN;  //enum testing is off
	ASN1_EnumTest;
	ASN1_EnumDisplay;


void ASN1_EnumDisplay;
	TEQU 0 11 0 0 RETURN;  //enum testing is off
	TEQU ff 4 0 0 RETURN;
	LABL  0 0 0 0 [;
	DGLB  0 4 0 0;
	LABL  0 0 0 0 ];
	TRTS;

void ASN1_ParseIdentifier;
	TLTE  1   2  0 30 ASN1_IdentifierLine;
	TRTS;

void ASN1_IdentifierLine;
	MOVE 0 5;
	ASN1_SubIDLoop;
	TEQU 0 8 0 30 ASN1_FirstSubID;
	DGLB 0 5 0 0 .;
	TNXT -ASN1_ParseIdentifier;

void ASN1_FirstSubID;
	MOVE  1  8  0 30;
	MOVE  5 89  0 30;
	DIVG 28  9  0 30;
	DGLB  0  9 90;
	MULG ffffffd8 9 0 30;
	ADDG  9 85  0 30;
	DGLB  0  5  0  0 .;
	TNXT -ASN1_ParseIdentifier;

void ASN1_SubIDLoop;
	TLTE  1  2  0 30 ASN1_SubIDNextLoop;
	ADDG  4 85;
	TRTS;

void ASN1_SubIDNextLoop;
	GBYT  0  4;
	INCR  1  0;
	SUBG  1  2; 
	SNEQ  0  2;
	SKIP  2;
	ADDG  4 85;
	TRTS;
	TBIT  7  4  0 30 ASN1_SubIDStrip7Loop;
	ADDG  4 85;
	TRTS;

void ASN1_SubIDStrip7Loop;
	ANDG 7f  4;
	ADDG  4 85;
	MULG 80  5;
	TNXT -ASN1_SubIDLoop;

void ASN1_SubIDNext;
	MOVE  0  5;
	TNXT -ASN1_SubIDNext2;

void ASN1_SubIDNext2;
	GBYT  0  4;
	INCR  1  0;
	SUBG  1  2;
//	TGTE  0  2  0  0 RETURN;
	TBIT  7  4  0 30 ASN1_SubIDStrip7;
	ADDG  4 85;
	TRTS;

void ASN1_SubIDStrip7;
	ANDG 7f  4;
	ADDG  4 85;
	MULG 80  5;
	TNXT -ASN1_SubIDNext2;

void ASN1_OctetString()
{
	l[1] = g[2];
	l[2] = 1;
	if (g[2] <= 0x50)
	{
		CHR# 0 2 90 0;
	}
	if (g[2] > 0x50)
	{
		while (l[1] > 0)
		{
			if (l[1] >= 0x50)
			{
				LABL( 0, 0, 0, 0xc2, "Line " );
				DGLB 0 42 2 0;
				LABL 0 0 0 2 :;
				CHR# 50 0 90 0;
				l[1] -= 0x50;
				l[2] += 1;
			}
			if (l[1] <= 0x50)
			{
				LABL( 0, 0, 0, 0xc2, "Line " );
				DGLB 0 42 2 0;
				LABL 0 0 0 2 :;
				CHR# 0 41 90 0;
				l[1] = 0;
				l[2] += 1;
			}
		}
	}
	g[2] = 0;
}

void ASN1_GeneralizedTime;
	CHR# 4 0 90;
	LABL 0 0 0 0 "-";
	CHR# 2 0 90;
	LABL 0 0 0 0 "-";
	CHR# 2 0 90;
	
	CHR# 2 0 20; // skip 2 spaces
	LABL 0 0 0 0 :;
	CHR# 2 0 90;
	LABL 0 0 0 0 :;
	CHR# 2 0 90;

	LABL 0 0 0 20 "(";
	CHR# 1; // skip 2 spaces
	LABL 0 0 0 0 ")";

	TRTS;
	
void ASN1_UTCTime;

	CHR# 0 2 90;
	DECR 0 2;

	// BD: Date
	INCR 2;
	CHR# 2 0 34;
	DECR 4;
	CLTE 1 2 0 4 -;
	INCR 4;
	CHR# 2 0 4;
	DECR 6;
	CLTE 1 2 0 4 -;
	CHR# 2 0 4;
	INCR 4;

	// BD: Time
	CHR# 2 0 34;
	CLTE 1 2 0 4 :;
	CHR# 2 0 4;

	SEQU D 2;
	SKIP 80000003;	
	  CLTE 1 2 0 4 :;
	  CHR# 2 0 4;
	  GBYT 0 31;

	SEQU 11 2;
	SKIP 80000003;
	   CLTE 1 2 0 4 :;
	   CHR# 2 0 4;
	   GBYT 0 31;

	SGTE D 31;
	SKIP 1;
	   CHR# 1 0 4;	
	   
	TRTS;


void ASN1_GeneralString;
	GSTR 2e 3;
	CHR#  0 3 90;
	TRTS;
	
void ASN1_PrintableString;
	CHR# 0 2 90 0;
	TRTS;

void ASN1_BitString;
	SLTE 15 2;
	SKIP 80000008;
	   BREM 0 30;
	   SUBG 4 30;
	   SLTE 30 82;
	   SKIP 80000002;
	     DUMP 1 0 90;
	     TRTS;
	   DUMP 0 2 90;
	   TRTS;
	MOVE 0 32;
	ASN1_BitStringLoop;
	TRTS;

void ASN1_BitStringLoop;
	SUBG 1 2;
	BBYT 80000000 3 90 5;
	ADDG 1 32;					// BD: stop looping if gets infinite loop for some bad Kerberos packets
	SEQU 0 2;
	SKIP 4;						
	LABL 0 0 0 0 -;
	SLTE 15 32;					// BD: check to make sure not gone past 15 recursive calls
		SKIP 1;					// BD: skip if has
	ASN1_BitStringLoop;
	TRTS;

void ASN1_Boolean;
	GBYT 0 3;     // put the boolean into 3
	INCR 1;
	SNEQ 0 3;
	SKIP 2;
	LABL 0 0 0 54 FALSE;
	TRTS;
	LABL 0 0 0 54 TRUE;
	TRTS;


// in params : g1=type, g2=length
// out params: g3=value

void ASN1_Integer;
	// 0 byte integer
	SNEQ  0 2;
	SKIP  2;
	LABL 0 0 90 5 NULL;
	TRTS;

	// 1 byte integer
	SNEQ  1 2;
	SKIP  2;
	DBYT  1 3 90 5;
	TRTS;

	// 2 byte integer
	SNEQ  2 2;
	SKIP  2;
	DWRD  1 3 90 5;
	TRTS;

	// 3 byte integer
	SNEQ  3 2;
	SKIP  2;
	DBIT  18 3 90 5;
	TRTS;

	// 4 byte integer
	SNEQ  4 2;
	SKIP  2;
	DLNG  0 3 90 5;
	TRTS;

	// 5+ byte integer
	HEX#  0 2 90 5;
	MOVE  2 83;      // move g2 to g3
	TRTS;



void ASN1_FindNextValue;
	ASN1_Value;
	SNEQ 0 1;
	SKIP 2;
	INCR 0 2;  // incr length
	TNXT -ASN1_FindNextValue;
	TRTS; 

void ASN1_FindNextOID;
	GBYT 0 1;
	TEQU 6 1 0 0 RETURN;	// test the type for OID
	INCR 1;  // incr length
	TNXT -ASN1_FindNextOID;
	TRTS; 

void ASN1_FindNextInteger;
	GBYT 0 1;
	TEQU 2 1 0 0 RETURN;	// test the type for integer
	INCR 1;  // incr length
	TNXT -ASN1_FindNextInteger;
	TRTS; 

void ASN1_FindNextSmallInteger;
	GBYT 0 1;
	INCR 1;
	SNEQ 2 1;	// test the type for a 1 byte integer
	SKIP 6;

	GBYT 0 1;
	INCR 1;
	SNEQ 1 1;  // test for 1 octet length
	SKIP 2;

	DECR 2;
	TRTS;

	TNXT -ASN1_FindNextSmallInteger;
	TRTS; 

void ASN1_OID;
	MOVE 0 8;
	MOVE 2 94; // save OID length
	LABL 0 0 0 50 {;
	ASN1_ParseIdentifier;
	LABL 0 0 0 0 };
	DECR 0 14;
	HEX# 0 14 5;

	MOVE 10 95;  //save remaining
	ASN1_OID2; 
	MOVE 10 96;  //save remaining
	SUBG 15 96;  //how many to get back to end of OID
	INCR 0 16;   //inr to end of OID
	MOVE 14 82;  //put back OID length

void ASN1_OID2;
	LABL 0 0 90 c2 Description:\;
	MOVE 0 5;
	DECR 0 14;   //goto beginning of OID
	DECR 2;
	HEX# 2 0 5;
	MOVE 14 82;  //put back OID length
	ASN1_SubIDNext;
	MOVE  5 89;
	DIVG 28  9;
	TEQU 0 9 0 0 OID_itu;
	TEQU 1 9 0 0 OID_iso;
	TEQU 2 9 0 0 OID_itu-iso;

void OID_itu;
	LABL 0 0 0 54 itu;
	MULG ffffffd8 9 0 30;
	ADDG 9 85  0 30;
	ADDG 1 5;  //CST# is zerobased
	TLTE 7 5 0 0 RETURN;
	LABL 0 0 0 4 .;
	CST# 0 5 0 4 OID_ituStr;
	ASN1_SubIDNext;
	LABL 0 0 0 4 .;
	CST# 0 5 0 4 OID_lettersStr;
	ASN1_SubIDNext;
	DGLB 0 5 4 4 .;
	
