	if (g[5] == 12733)
	{
		LABL 0 5 0 4 Savvius, Inc.;
		//a "SAVVIUS" flagh could be set here to get more detailed data later TT33122
		ASN1_SubIDNext;
		if (g[5] == 2)
		{
			CEQU 2 5  0 54 2.;
			ASN1_SubIDNext;
			if (g[5] == 1)
			{
				CEQU 1 5  0 54 1.;
				ASN1_SubIDNext;
				if (g[5] == 1)
				{
					CEQU 1 5  0 54 1.;
					ASN1_SubIDNext;
					if (g[5] == 1)
					{
						LABL 0 0 0 54 WPOmni3EventSeverities;
					}
					if (g[5] == 2)
					{
						LABL 0 0 0 54 wpNotifyTime (OctectStringData);
					}
					if (g[5] == 3)
					{
						LABL 0 0 0 54 wpNotifyDescription;
					}
				}
			}
		}
		g[5] = 12733;
		LABL 0 0 90 c2 Enterprise:;
		CST# 0 5  0 54 OIDS_Prv;
		//g[1] = 0x1e;
	}
	
