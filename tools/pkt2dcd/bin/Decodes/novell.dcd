	HLNG 0 0 90 c2 Source Network:\SNet:;
	ETHR 0 0 90 c2 Source Node:\SNode:;
	HWRD 0 3 90 c2 Source Socket:\SSocket:;
	TNXT -NetWare_Src_MailSlot;

void NBoverIPX;
	g[2] = g[0x1e];
	while (g[0x1e] > 0)
	{
		HLNG 0 1 90 c2 IPX Network:;
		g[0x1e] -= 1;
	}
	if (g[2] < 8)
	{
		g[1] = 8;
		SUBG 2 81;
		g[1] *= 4;
		HEX# 0 1 90 c2 Padding:;
	}
	BBYT 0 1 90 c9 Name Type Flags:;
	 CBIT 7 1 0 d4 1... .... Unique Name;
	 CLSE 0 0 0 d4 0... .... Not a Unique Name;
	 CBIT 6 1 0 d4 .1.. .... Name in use;
	 CLSE 0 0 0 d4 .0.. .... Name not in use;
	 CBIT 2 1 0 d4 .... .1.. Name registered;
	 CLSE 0 0 0 d4 .... .0.. Name not registered;
	 CBIT 1 1 0 d4 .... ..1. Name duplicated;
	 CLSE 0 0 0 d4 .... ..0. Name not duplicated;
	 CBIT 0 1 0 d4 .... ...1 Name deregistered;
	 CLSE 0 0 0 d4 .... ...0 Name not deregistered;
	 LABL 0 0 0 c8;
	HBYT 0 1 90 c2 Packet Type:\Type:;
	CST# 0 1  0 14 NBoverIPX::Packet_Types;
	CHR# f 0 90 c2 Name:;
	HBYT 0 1 90 c2 Name Type:;
	CST# 0 1  0 14 NBoverIPX::Name_Types;
	TRTS;

