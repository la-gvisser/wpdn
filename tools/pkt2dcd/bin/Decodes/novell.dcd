void NetWare Src NetBIOS;
	SUBG 1e 1 0 30;
	LABL 0 0 0 b1 NetWare NetBIOS\NetBIOS:;
	INCR 1;
	GBYT 0 4;
	DECR 1 0;
	TST# 0 4 0 0 NW NBios DataStreams;
	TGTE 0 10 0 0 RETURN;
	//si: check if this was an ipx packet, if so, branch off now
	if (g[0x1f] == 0x697078)
	{
		TNXT -NBoverIPX;
	}

void NetWare_Src_MailSlot;
	LABL 00 0 00 b1 NetWare MailSlot Datagram\MailSlot:;
	HEX# 20 0 90 c2 Routing Info:;
	HBYT 00 1 90 c2 OpCode:;
	CST# 00 1 00 14 NetWare_Src_MailSlot::OpCodes;
	HBYT 00 1 90 c2 Name Type:;
	CST# 00 1 00 14 NetWare_Src_MailSlot::Name_Type;
	HWRD 00 1 90 c2 Message ID:;
	CHR# 10 0 90 c2 Name:;
	CHR# 10 0 90 c2 Src Name:;
	GLNG 0 4;
	TEQU ff534d42 4 0 0 SMB File Share;
	TRTS;

str# NetWare_Src_MailSlot::OpCodes;
	0xf1 | server name claim message;
	0xf2 | relinquish server name;
	0xf3 | locate server name;
	0xf4 | response to INAME_QUERY;
	0xf5 | Messenger Hangup;
	0xfc | packet contains mslot write, no resp needed;
	0xfd | find name for mslot write, no data included;
	0xfe | find name response;

str# NetWare_Src_MailSlot::Name_Type;
	1 | Machine;
	2 | Workgroup;
	3 | Browser;

void NW NBios Name Query;
	DUMP 20 0 90 c2 Reserved:;
	HBYT 0 4 90 c2 Name Type Flag:;
	CBIT 0 4 0 f4 Name Deregistered;
	CBIT 1 4 0 f4 Name Duplicated;
	CBIT 2 4 0 f4 Name Registered;
	CBIT 6 4 0 f4 Name Used;
	CBIT 7 4 0 f4 Group Name;
	DBYT 0 4 90 c2 Datastream Type:;
	CST# 0 4 0 14 NW NBios DataStreamTypes;
	CHR# f 0 90 c2 Name String:;
	LABL -<;
	HBYT 0 12;
	LABL ->;
	NB Special Name;
	TGTE 0 10 0 0 RETURN;
	GBYT 0 1;
	SNEQ 0 1;
	SKIP 1;	
	INCR 1;

void NW NBios Name Response;
	DUMP 20 0 90 c2 Reserved:;
	HBYT 0 4 90 c2 Name Type Flag:;
	CBIT 0 4 0 f4 Name Deregistered;
	CBIT 1 4 0 f4 Name Duplicated;
	CBIT 2 4 0 f4 Name Registered;
	CBIT 6 4 0 f4 Name Used;
	CBIT 7 4 0 f4 Group Name;
	DBYT 0 4 90 c2 Datastream Type:;
	CST# 0 4 0 14 NW NBios DataStreamTypes;
	CHR# f 0 90 c2 Name String:;
	LABL -<;
	HBYT 0 12;
	LABL ->;
	INCR 1;
	NB Special Name;
	
void NW NBios Session Data;
	HBYT 0 4 90 c2 Control Flag:;
	CBIT 3 4 0 f4 Resend Needed;
	CBIT 4 4 0 f4 End Of Message;
	CBIT 5 4 0 f4 Attention;
	CBIT 6 4 0 f4 Send ACK;
	CBIT 7 4 0 f4 System Packet;
	DBYT 0 4 90 c2 Datastream Type:;
	CST# 0 4 0 14 NW NBios DataStreamTypes;
	DWRD 40000000 4 90 c2 Source Connection ID:;
	DWRD 40000000 4 90 c2 Dest Connection ID:;
	DWRD 40000000 4 90 c2 Send Sequence:;
	DWRD 40000000 4 90 c2 Send Total Length:;
	DWRD 40000000 4 90 c2 Fragment Offset:;
	DWRD 40000000 4 90 c2 Fragment Length:;
	DWRD 40000000 4 90 c2 ACK Sequence:;
	DWRD 40000000 4 90 c2 ACK Fragment Offset:;
	SUBG 12 1;
	SEQU 0 1;
	SKIP 1f 0;
	GLNG 0 4;
	TEQU ff534d42 4 0 0 SMB File Share;
	CHR# f 0 90 c2 Source Name:;
	LABL -<;
	HBYT 0 12;
	LABL ->;
	NB Special Name;
	CHR# f 0 90 c2 Destination Name:;
	LABL -<;
	HBYT 0 12;
	LABL ->;
	NB Special Name;
	DWRD 40000000 4 90 c2 Max Data XMit Size:;
	GWRD 40000000 4;
	GWRD 40000000 5;
	DIVG 2 4;
	DGLB 0 4 90 c2 Max Packet Xmit Time:;
	MULG 2 4;
	SUBG 4 85;
	CNEQ 0 5 0 0 .5;
	LABL 0 0 0 10 seconds;
	INCR 2;
	GWRD 40000000 4;
	GWRD 40000000 5;
	DIVG 2 4;
	DGLB 0 4 90 c2 Start Packet Trip Time:;
	MULG 2 4;
	SUBG 4 85;
	CNEQ 0 5 0 0 .5;
	LABL 0 0 0 10 seconds;
	INCR 2;
	GLNG 0 4;
	TEQU ff534d42 4 0 0 SMB File Share;
	DUMP 1 0 90 c2 Remaining NetBIOS Data:;

void NW NBios Session End;
	HBYT 0 4 90 c2 Control Flag:;
	CBIT 3 4 0 f4 Resend Needed;
	CBIT 4 4 0 f4 End Of Message;
	CBIT 5 4 0 f4 Attention;
	CBIT 6 4 0 f4 Send ACK;
	CBIT 7 4 0 f4 System Packet;
	DBYT 0 4 90 c2 Datastream Type:;
	CST# 0 4 0 14 NW NBios DataStreamTypes;
	DWRD 40000000 4 90 c2 Source Connection ID:;
	DWRD 40000000 4 90 c2 Dest Connection ID:;

void NW NBios Session End Ack;
	HBYT 0 4 90 c2 Control Flag:;
	CBIT 3 4 0 f4 Resend Needed;
	CBIT 4 4 0 f4 End Of Message;
	CBIT 5 4 0 f4 Attention;
	CBIT 6 4 0 f4 Send ACK;
	CBIT 7 4 0 f4 System Packet;
	DBYT 0 4 90 c2 Datastream Type:;
	CST# 0 4 0 14 NW NBios DataStreamTypes;
	DWRD 40000000 4 90 c2 Source Connection ID:;
	DWRD 40000000 4 90 c2 Dest Connection ID:;

void NW NBios DGram;
	INCR 1;
	DBYT 0 4 90 c2 Datastream Type:;
	CST# 0 4 0 14 NW NBios DataStreamTypes;
	CHR# f 0 90 c2 Source Name:;
	LABL -<;
	HBYT 0 12;
	LABL ->;
	NB Special Name;
	CHR# f 0 90 c2 Destination Name:;
	LABL -<;
	HBYT 0 12;
	LABL ->; 
	NB Special Name;
	GLNG 0 4;
	TEQU ff534d42 4 0 0 SMB File Share;
	DUMP 1 0 90 c2 Remaining NetBIOS Data:;
	
void NetWare Src RIP;
	LABL 0 0 0 b1 RIP - Routing Information Protocol\RIP:;
	DWRD 0 4 90 c2 Operation:;
	CEQU 1 4 0 14 Request;
	CEQU 2 4 0 14 Response;
	SUBG 20 1;
	TGTE 0 1 0 0 XNS Done;
	TNXT -NetWare RIP;

void NetWare RIP;
	LSTS 0 1 0 b1 Network Number Set #\Net Num;
	HLNG 0 0 90 c2 Network Number:;
	DWRD 0 7 90 c2 Number of Hops:;
	CEQU ffff 7 0 14 Unknown;
	DWRD 0 0 90 c2 Number of Ticks:;
	SUBG 8 1;
	LSTZ 0 1 0 80;

void NetWare Dst NCP;
	HLNG 0 0 90 c2 Source Network:\SNet:;
	ETHR 0 0 90 c2 Source Node:\SNode:;
	HWRD 0 3 90 c2 Source Socket:\SSocket:;
	CLTE 8000 3 0 14 IPX Static;
	TTST -NetWare Src NCP;
	CLTE 6000 3 0 14 IPX Dynamic;
	TTST -NetWare Src NCP;
	CLTE 4000 3 0 14 IPX Ephemeral;
