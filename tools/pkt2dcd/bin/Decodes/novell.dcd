	TTST -NCP ConnReq;
	CEQU 2222 4 0 14 Request;
	TTST -NCP Request;
	CEQU 3333 4 0 14 Reply;
	TTST -NCP Reply;
	CEQU 5555 4 0 14 Destroy Connection;
	TTST -NCP Destroy Connection;
	CEQU 7777 4 0 14 Burst;
	TTST -NCP Burst;
	CEQU 9999 4 0 14 Server Delay;
	TTST -NCP Delay;
	TNXT -Netware Common;

