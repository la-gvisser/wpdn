
void Ethernet_802.3 IPX;
	DECR 2 0 0 40;
	PRV6 0 0 90 c2 Length:;
	TNXT -IPX Top;

void IPX Top;
	LABL 0 0 0 b1 IPX - NetWare Protocol\IPX:;
	g[0x1f] = 0x697078;							//si: i need a tag to say that i'm ipx
	HWRD 0 0 90 c2 Checksum:\;
	DWRD 0 1 90 c2 Length:\;
	g[0x1e] = 0;
	LABL 0 0  0 c9 Transport Control:;
	 BBIT 4 0 90 c2 Reserved:;
	 DBIT 4 1e 90 c2 Hop Count:;				//si: save for nw netbios
	 LABL 0 0  0 b8;
	DBYT 0 2 90 c2 Packet Type:\Type:;
	CST# ffffffff 2 0 14 NW Pkt Types;
	HLNG 0 0 90 c2 Destination Network:\DNet:;
	ETHR 0 0 90 c2 Destination Node:\Dode:;
	HWRD 0 3 90 c2 Destination Socket:\DSock:;
	CEQU 900f 3 0 14 SNMP over IPX;
	TTST -SNMP IPX Src;
	CEQU 9010 3 0 14 SNMP over IPX;
	TTST -SNMP IPX Src;
	CEQU 9001 3 0 14 NetWare NLSP;
	TTST -NetWare NLSP Src;
	CLTE 8000 3 0 14 IPX Static;
	TTST -Netware Src;
	CLTE 6000 3 0 14 IPX Dynamic;
	TTST -Netware Src;
	CLTE 4000 3 0 14 IPX Ephemeral;
	TTST -Netware Src;
	CST# 450 3 0 14 NetWare Socket Types;
	TEQU 451 3 0 0 NetWare Dst NCP;
	TEQU 452 3 0 0 NetWare Dst SAP;
	TEQU 453 3 0 0 NetWare Dst RIP;
	TEQU 455 3 0 0 NetWare Dst NetBIOS;
	TEQU 553 3 0 0 NetWare_Dst_MailSlot;
	TNXT -Netware Src;

void NetWare NLSP Src;
	HLNG 0 0 90 c2 Source Network:\SNet:\SNet:;
	ETHR 0 0 90 c2 Source Node:\SNode:\SNode:;
	HWRD 0 3 90 c2 Source Socket:\SSocket:;
	CEQU 9001 3 0 14 NetWare NLSP;
	LABL 0 0 0 b1 NLSP - NetWare Link Services Protocol\NLSP:;
	HBYT 0 0 90 c2 Protocol ID:\Proto:;
	DBYT 0 3 90 c2 Length:;
	DBYT 0 0 90 c2 Minor Version:\;
	HBYT 0 0 90 c2 Reserved:\;
	DBYT 0 4 90 c2 Packet Type:\Type:;
	DECR 1;
	ANDG 1f 4;
	CST# e 4 90 14 NLSP Packet Types;
	TST# e 4 0 0 NLSP Packet Types;

void NLSP WAN Hello;  * CBB this should do something
	TRTS;	

void NLSP LAN Hello;
	INCR 1;
	DBYT 0 0 90 c2 Major Version:;
	HWRD 0 0 90 c2 Reserved:;
	BBYT 0 4 90 c2 Circuit Type:;
	CBIT 4 4 0 f4 No Multicast;
	ANDG 3 4;
	CST# ffffffff 4 90 f4 NLSP Circuit Types;
	HEX# 6 0 90 c2 Source ID:;
	DWRD 0 0 90 c2 Holding Time:;
	DWRD 0 5 90 c2 Packet Length:;
	DBYT 0 0 90 c2 Priority:;
	HEX# 7 0 90 c2 LAN ID:;
	SUBG 3 85;
	TNEQ 0 5 0 0 NLSP Variable Fields;
	
void NLSP LSP;
	GBYT 0 4;
	CBIT 7 4 90 14 Multihomed non-routing server;
	INCR 1;
	DBYT 0 0 90 c2 Major Version:;
	HWRD 0 0 90 c2 Reserved:;
	DWRD 0 5 90 c2 Packet Length:;
	GWRD 0 6;
	ANDG 7fff 6;
	DGLB 0 6 90 c2 Remaining Lifetime:;
	GWRD 0 6;
	CBIT f 6 0 0  (Hours);
	CLSE f 6 0 0  (Seconds);
	INCR 2;
	HEX# 6 0 90 c2 Source ID:;
	HBYT 0 0 90 c2 Pseudonode ID:;
	HBYT 0 0 90 c2 LSP Number:;
	HLNG 0 0 90 c2 Sequence Number:;
	HWRD 0 0 90 c2 Checksum:;
	HBYT 0 6 90 c2 Flags:;
	CBIT 7 6 90 44  (Supports Partition Repair);
	CLSE 7 6 90 44  (Partition Repair Not Supported);
	SUBG 3 85;
	TNEQ 0 5 0 0 NLSP Variable Fields;
	
void NLSP CNSP;
	INCR 1;
	DBYT 0 0 90 c2 Major Version:;
	HWRD 0 0 90 c2 Reserved:;
	DWRD 0 5 90 c2 Packet Length:;
	HEX# 7 0 90 c2 Source ID:;
	HEX# 8 0 90 c2 Start LSP ID:;
	HEX# 8 0 90 c2 End LSP ID:;
	SUBG 3 85;
	TNEQ 0 5 0 0 NLSP Variable Fields;
	
void NLSP PNSP;
	INCR 1;
	DBYT 0 0 90 c2 Major Version:;
	HWRD 0 0 90 c2 Reserved:;
	DWRD 0 5 90 c2 Packet Length:;
	HEX# 7 0 90 c2 Source ID:;
	SUBG 3 85;
	TNEQ 0 5 0 0 NLSP Variable Fields;
	
void NLSP Variable Fields;
	HBYT 0 4 90 c2 Option Code:;
	CEQU 6 4 0 24 Neighbors;
	CEQU 9 4 0 24 LSP Entries;
	CEQU c0 4 0 24 Area Addresses;
	CEQU c1 4 0 24 Management Information;
	CEQU c2 4 0 24 Link Information;
	CEQU c3 4 0 24 Services Information;
	CEQU c4 4 0 24 External Routes;
	CEQU c5 4 0 24 Local MTU;
	DBYT 0 6 90 c2 Option Length:;
	SUBG 6 85;
	SUBG 2 5;
	TEQU 6 4 0 0 NLSP Neighbors;
	TEQU 9 4 0 0 NLSP LSP Entries;
	TEQU c0 4 0 0 NLSP Area Addresses;
	TEQU c1 4 0 0 NLSP Management Information;
	TEQU c2 4 0 0 NLSP Link Information;
	TEQU c3 4 0 0 NLSP Services Information;
	TEQU c4 4 0 0 NLSP External Routes;
	TEQU c5 4 0 0 NLSP Local MTU;
	INCR 0 6;
	TNEQ 0 5 0 0 NLSP Variable Fields;
	
void NLSP LSP Entries;
	LSTS 0 6 0 c9 LSP Entry #;
	GWRD 0 7;
	ANDG 7fff 7;
	DGLB 0 7 90 c2 Remaining Lifetime:;
	GWRD 0 7;
	CBIT f 7 0 14 (Hours);
	CLSE f 7 0 14 (Seconds);
	INCR 2;
	HEX# 8 0 90 c2 LSP ID:;
	DLNG 0 0 90 c2 Sequence Number:;
	DWRD 0 0 90 c2 Checksum:;
	LABL 0 0 0 c8;
	SUBG 10 6;
	LSTZ 0 6 0 40;
	TNEQ 0 5 0 0 NLSP Variable Fields;
	
void NLSP Area Addresses;
	LSTS 0 6 0 c9 Area Address #;
	 HLNG 0 0 90 c2 Address:;
	 HLNG 0 0 90 c2 Mask:;
	 LABL 0 0  0 b8;
	SUBG 8 6;
	LSTZ 0 6;
	TNEQ 0 5 0 0 NLSP Variable Fields;
	
void NLSP Neighbors;
	LSTS 0 6 0 c2 Neighbor #;
	HEX# 6 0 90 c2 NIC Address:;
	SUBG 6 6;
	LSTZ 0 6 0 40;
	TNEQ 0 5 0 0 NLSP Variable Fields;
	
void NLSP Local MTU;
	DLNG 0 0 90 c2 Maximum MTU:;
	TNEQ 0 5 0 0 NLSP Variable Fields;
	
void NLSP Management Information;
	HLNG 0 0 90 c2 Network Number:;
	HEX# 6 0 90 c2 Node Number:;
	DBYT 0 0 90 c2 IPX Version Number:;
	PSTR 0 7 90 c2 Router/Server Name:;
	TNEQ 0 5 0 0 NLSP Variable Fields;
	
void NLSP Link Information;
	GBYT 0 7;
	SBIT 6 7;
	SKIP 5 0;
	ANDG 3f 7;
	DGLB 0 7 90 c2 Cost:;
	GBYT 0 7;
	CBIT 6 7 0 4  (Internal Metric);
	CLSE 6 7 0 4  (External Metric);
	INCR 1;
	HEX# 3 0 90 c2 Reserved:;
	HEX# 6 0 90 c2 System ID:;
	HBYT 0 0 90 c2 Pseudonode ID:;
	DLNG 0 0 90 c2 MTU Size:;
	DLNG 0 0 90 c2 Delay:;
	DLNG 0 0 90 c2 Throughput:;
	DWRD 0 7 90 c2 Media Type:;
	CST# ffffffff 7 0 14 NLSP Media Types;
	TNEQ 0 5 0 0 NLSP Variable Fields;
	
void NLSP Services Information;
	DBYT 0 0 90 c2 Hops:;
	HEX# 4 0 90 c2 Network Number:;
	HEX# 6 0 90 c2 Node Number:;
	DWRD 0 0 90 c2 Socket:;
	DWRD 0 0 90 c2 Service Type:;
	SUBG f 6;
	CHR# 0 6 90 c2 Service Name:;
	TNEQ 0 5 0 0 NLSP Variable Fields;
	
void NLSP External Routes;
	LABL 0 0 0 b1 External Route;
	DBYT 0 1 90 c2 Hops:;
	HLNG 0 1 90 c2 Network Number:;
	DWRD 0 1 90 c2 RIP Delay (# ticks):;
	TNEQ 0 5 0 0 NLSP Variable Fields;
	
void Netware Src;
	HLNG 0 0 90 c2 Source Network:\SNet:;
	ETHR 0 0 90 c2 Source Node:\SNode:;
	HWRD 0 3 90 c2 Source Socket:\SSocket:;
	CEQU 900f 3 0 14 SNMP over IPX;
	TTST -SNMP;
	CEQU 9010 3 0 14 SNMP over IPX;
	TTST -SNMP;
	CLTE 8000 3 0 14 IPX Static;
	TTST -Netware Common;
	CLTE 6000 3 0 14 IPX Dynamic;
	TTST -Netware Common;
	CLTE 4000 3 0 14 IPX Ephemeral;
	TTST -Netware Common;
	CST# 450 3 0 14 NetWare Socket Types;
	TEQU 451 3 0 0 NetWare Src NCP;
	TEQU 452 3 0 0 NetWare Src SAP;
	TEQU 453 3 0 0 NetWare Src RIP;
	TEQU 455 3 0 0 NetWare Src NetBIOS;
	TEQU 553 3 0 0 NetWare Src NetBIOS;
	TNXT -Netware Common;

void Netware Common;
	SUBG 1e 1;
	TEQU 5 2 0 0 NetWare SPX;
	LABL 0 0 0 c2 IPX Data Area:;
	DUMP 1 0 90 40;

void NetWare Dst SAP;
	HLNG 0 0 90 c2 Source Network:\SNet:;
	ETHR 0 0 90 c2 Source Node:\SNode:;
	HWRD 0 3 90 c2 Source Socket:\SSocket:;
	CLTE 8000 3 0 14 IPX Static;
	TTST -NetWare Src SAP;
	CLTE 6000 3 0 14 IPX Dynamic;
	TTST -NetWare Src SAP;
	CLTE 4000 3 0 14 IPX Ephemeral;
	TTST -NetWare Src SAP;
	CST# 450 3 0 14 NetWare Socket Types;
	TNXT -NetWare Src SAP;

void NetWare Src SAP;
	LABL 0 0 0 b1 SAP - Service Advertising Protocol\SAP:;
	DWRD 0 4 90 c2 Type/Operation:;
	CST# 0 4 0 14 NW SAP Op Types;
	SUBG 20 1 20 40;
	TEQU 1 4 0 0 NW SAP Request;
	TEQU 3 4 0 0 NW SAP Request;
	TEQU c 4 0 0 NW Specific SAP Request;
	TEQU e 4 0 0 NW Specific SAP Request;
	MOVE 0 5;
	TEQU 2 4 0 0 NW SAP Reply;
	TEQU 4 4 0 0 NW SAP Reply;
	TEQU d 4 0 0 NW SAP Reply;
	TEQU f 4 0 0 NW SAP Reply;
	
void NW SAP Request;
	DWRD 0 4 90 c2 Service Type:;
	
void NW Specific SAP Request;
	DWRD 0 4 90 c2 Service Type:;
	CEQU ffff 4 0 14 Request Response;
	CHR# 30 0 90 c2 Service Name:;
	DLNG 0 4 90 c2 Network Number:;
	HLNG 0 4 90 c2 Network Mask:;
	SUBG 3a 1 80 c2;
	TLTE 3a 1 90 c2 NW Specific SAP Request;

void NW SAP Reply;
	ADDG 1 5;
	DGLB 0 5 0 b7 Service Advertising Set #;
	DWRD 0 4 90 c2 Service Type:;
	CST# 0 4 0 14 NW::SAP_Reply_Str;
	MOVE 30 3;  * default size for name
	BREM 0 2;   * make sure there is enough room
	SUBG 4 2;   * subtract FCS
	SLTE 30 2;  * is string <= remaining
	SKIP 2;
	MOVE 2 83;  * move g2 into g3;
	MOVE 0 2;   * set g2 to 0
	CHR# 0 3 90 c2 Service Name:;
	TEQU 0 2  0  0 RETURN;
	DLNG 0 4 90 c2 Service Network:;
	ETHR 0 4 90 c2 Service Node:;
	DWRD 0 4 90 c2 Service Socket:;
	DWRD 0 4 90 c2 Inter networks/hops:;
	SUBG 3c 1 80 c2;
	LABL 0 0 0 b8;
	TLTE 3c 1 90 c2 NW SAP Reply;

