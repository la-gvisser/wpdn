void NCP Burst;
	GWRD 0 b;
	BBYT 0 5 90 c2 Flags:;
	CBIT 8 5 0 f4 System;
	CBIT 5 5 0 f4 End Of Burst;
	CBIT 3 5 0 f4 Abort;
	HBYT 0 5 90 c2 Stream Type:;
	HLNG 0 5 90 c2 Source Connection ID:;
	HLNG 0 5 90 c2 Destination Connection ID:;
	DLNG 0 5 90 c2 Packet Sequence Number:;
	DLNG 0 5 90 c2 Send Delay Time:;
	DWRD 0 5 90 c2 Burst Sequence Number:;
	DWRD 0 5 90 c2 ACK Sequence Number:;
	DLNG 0 5 90 c2 Total Burst Length:;
	DLNG 0 5 90 c2 Data Offset:;
	DWRD 0 6 90 c2 Data Bytes:;
	DWRD 0 5 90 c2 Fragment List Count:;
	GLNG 0 5;
	TEQU 1000000 5 0 0 NCP Burst Request;
	TEQU 2000000 5 0 0 NCP Burst Request;
	TEQU 0 5 0 0 NCP Burst Reply;
	TNXT -NCP Burst Data;

void NCP Delay;
	GWRD 0 b;
	DBYT 0 0 90 c2 Sequence number:;
	DBYT 0 0 90 c2 Connection number:;
	DBYT 0 0 90 c2 Task number:;
	DBYT 0 0 90 c2 Reserved:;
	DBYT 0 0 90 c2 Completion Code:;
	DBYT 0 0 90 c2 Connection Status:;

void NCP Burst Request;
	DLNG 40000000 5 90 c2 Function:;
	CEQU 1 5 0 14 (Read Request);
	CEQU 2 5 0 14 (Write Request);
	HLNG 0 5 90 c2 File Handle:;
	*INCR 8 0;
	DLNG 0 5 90 c2 File Offset:;
	DLNG 0 5 90 c2 Number of Bytes:;
	SUBG 10 6;  *Subtract 12 from the data size
	TNXT -NCP Burst Data;

void NCP Burst Reply;
	DLNG 40000000 5 90 c2 Reply Code:;
	CEQU 0 5 0 14 (Successful);
	DLNG 0 5 90 c2 Bytes:;
	SUBG 8 6;  *Subtract 12 from the data size
	TNXT -NCP Burst Data;

void NCP Burst Data;
	DUMP 0 6 90 c2 Burst Data:;

