void NCP::Type_21::00;
	MARK 0 14 0 0 NCP::Type_21::MW_List;
	LABL 0 0 0 b7 Send Broadcast Message;
	DBYT 0 2 90 c2 Client List Len:;
	while (g[2] > 0)
	{
		TGTE 0 10 0 0 RETURN;
		HLNG 0 1 90 c2 Targ Client List:;
		g[2] -= 1;
	}
	DBYT 0 1 90 c2 Message Len:;
	CHR# 0 1 90 c2 Message:;
	LABL 0 0 0 b8;
	TRTS;

void NCP::Type_21::00R;
	LABL 0 0 0 b7 Send Broadcast Message Reply;
	DBYT 0 2 90 c2 Client List Len:;
	while (g[2] > 0)
	{
		TGTE 0 10 0 0 RETURN;
		HBYT 0 1 90 c2 Send Status:;
		 CEQU  0 1 0 14 Success;
		 CEQU ff 1 0 14 Failure;
		 CEQU fc 1 0 14 Message Pending;
		g[2] -= 1;
	}
	LABL 0 0 0 b8;
	TRTS;

