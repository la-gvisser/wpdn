void NCP Request;
	GWRD 0 b;
	DBYT 0 14 90 c2 Sequence number:\Seq;
	DBYT 0 5 90 c2 Low number:\;
	CEQU ff 5 0 14 To Be Determined;
	DBYT 0 0 90 c2 Task number:\;
	DBYT 0 5 90 c2 High number:\;
	DBYT 0 5 90 c2 Function Code:\Func Code:;
	SUBG 5 1;
	CST# 0 5 0 14 NCP_Request_String;
	TST# 0 5 0 0  NCP_Request_String_Func;
	TNXT -NW Request Any Left;
	
void NCP Reply;
	GWRD 0 b;
	DBYT 0 14 90 c2 Sequence number:\Seq:;
	DBYT 0 0 90 c2 Low number:\;
	DBYT 0 0 90 c2 Task number:\;
	DBYT 0 0 90 c2 High number:\;
	HBYT 0 9 90 c2 Completion Code:\Code:;
	CST# 0 9 0 14 NCP::Reply::Completion_Code_List;
	HBYT 0 5 90 c2 Status flags:\Flags:;
	CEQU 0 5 0 14 OK;
	SUBG 6 1;
	TLTE 1 9 0 0 HALT;

	// ------------------------------------------ whoa stuff
	WHOA 0 14 0 0 NCP::Type_17::MW_List;
	WHOA 0 14 0 0 NCP::Type_21::MW_List;
