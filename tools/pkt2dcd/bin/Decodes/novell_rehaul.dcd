void NDS::Verbs::DSV_Resolve_Name;				// 01
	DLNG 40000000 1 90 c2 Reply Buffer Size:;
	DLNG 40000000 1 90 c2 Version:;
	HWRD 40000000 1 90 c9 Flags:;
	CBIT		6 1 00 d4 .... .... .1.. .... Dereference Alias: True;
	CLSE		0 0 00 d4 .... .... .0.. .... Dereference Alias: False;
	CBIT		0 1 00 d4 .... .... .... ...1 Entry ID: True;
	CLSE		0 0 00 d4 .... .... .... ...0 Dereference Alias: False;
	LABL		0 0 00 c8;
	INCR 2;
	DLNG 40000000 1 90 c2 Scope:;
	DLNG 40000000 1 90 c2 Name Length:;
	CHR# 0 1 90 c2 Name:;
	INCR 2;
	DLNG 40000000 2 90 c9 Comm. Transports;
	while (g[2] > 0)
	{
		HLNG 40000000 1 90 c2 Protocol:;
		CST#		0 1 00 14 NDS::ProtocolType_Str;
		g[2] -= 1;
	}
	LABL		0 0 00 c8;
	DLNG 40000000 2 90 c9 TreeWalker Transport;
	while (g[2] > 0)
	{
		HLNG 40000000 1 90 c2 Protocol:;
		CST#		0 1 00 14 NDS::ProtocolType_Str;
		g[2] -= 1;
	}
	LABL		0 0 00 c8;
	TRTS;
	
str# NDS::ProtocolType_Str;
	0x0	| IPX;
	0x1	| IP;
	0x2	| SDLC;
	0x3	| TokenRing on Ethernet;
	0x4	| OSI;
	0x5	| AppleTalk;
	0x6	| NetBEUI;
	0x7	| Socket Address;
	0x8	| UDP;
	0x9	| TCP;
	0xa	| UDP v6;
	0xb	| TCP v6;
	0xc	| Internal;
	0xd	| URL;

void NDS::Verbs::DSV_Read_Entry_Info;			// 02
	DLNG 40000000 1 90 c2 Reply Buffer Size:;
	DLNG 40000000 1 90 c2 Version:;
	HWRD 40000000 1 90 c2 Request Flags:;
	HWRD 40000000 1 90 c2 Name Type:;
	HWRD 40000000 1 90 c2 Info Flags (low):;
	HWRD 40000000 1 90 c2 Info Flags (hi):;
	HLNG 40000000 1 90 c2 Entry ID:;
	TRTS;

