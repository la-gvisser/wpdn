void NCP::Type_21::10R;
	LABL 0 0 0 b7 Send Broadcast Message Reply;
	DWRD 0 2 90 c2 RClient List Cnt:;
	while (g[2] > 0)
	{
		TGTE 0 10 0 0 RETURN;
		DLNG 0 1 90 c2 RClient Comp Flag:;
		 CEQU 0 1 0 14 Successful;
