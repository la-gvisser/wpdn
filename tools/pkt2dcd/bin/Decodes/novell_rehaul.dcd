void NCP::Type_23::236R;
	LABL 0 0 0 b7 Get Connection Using A File Reply;
	DWRD 40000000 1 90 c2 Next Req Record:;
	DWRD 40000000 1 90 c2 Use Cnt:;
	DWRD 40000000 1 90 c2 Open Cnt:;
	DWRD 40000000 1 90 c2 Open For Read Cnt:;
	DWRD 40000000 1 90 c2 Open For Write Cnt:;
	DWRD 40000000 1 90 c2 Deny Read Cnt:;
	DWRD 40000000 1 90 c2 Deny Write Cnt:;
	HBYT		0 1 90 c2 Locked:;
	 CEQU 0 1 0 14 Not locked exclusively;
	 CEQU 1 1 0 14 Locked exclusively;
	HBYT		0 1 90 c2 Fork Cnt:;
	DWRD 40000000 2 90 c2 Num Of Records:;
 	while (g[2] > 0)
	{
		DWRD 40000000 1 90 c2 Connection Num:;
		DWRD 40000000 1 90 c2 Task Num:;
		HBYT 0 1 90 c2 Lock Type:;
		 CEQU 0 1 0 14 Locked;
		 CEQU 1 1 0 14 Open shareable;
		 CEQU 2 1 0 14 Logged;
		 CEQU 3 1 0 14 Open normal;
		 CEQU 6 1 0 14 TTS holding lock;
		 CEQU 7 1 0 14 Transaction flag set on this file;
		HBYT 0 1 90 c2 Access Control:;
		 CEQU 0 1 0 14 Open for read by this client;
		 CEQU 1 1 0 14 Open for write by this client;
		 CEQU 2 1 0 14 Deny read requests from other stations;
		 CEQU 3 1 0 14 Deny write requests from other stations;
		 CEQU 4 1 0 14 File detached;
		 CEQU 5 1 0 14 TTS holding detach;
		 CEQU 6 1 0 14 TTS holding open;
		HBYT 0 1 90 c2 Lock Flag:;
		 CEQU 00 1 0 14 Not locked;
		 CEQU FE 1 0 14 Locked by a file lock;
		 CEQU FF 1 0 14 Locked by Begin Share File Set;
		g[2] -= 1;
	}
	LABL 0 0 0 b8;
	TRTS;

