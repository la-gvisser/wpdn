void NCP::Type_87::31R;
	LABL 0 0  0 b7 Get File Information Reply;
	g[2] = s[1];
	if (g[2] == 0)
	{
		HLNG 40000000 1 90 c2 Volume:;
		HLNG 40000000 1 90 c2 Dir Base:;
		HLNG 40000000 1 90 c2 Data Stream:;
		 CEQU 0 1 0 14 Resource Fork or DOS;
		 CEQU 1 1 0 14 Data Fork;
	}
	if (g[2] == 1)
	{
		HLNG 40000000 1 90 c2 Volume:;
		HLNG 40000000 1 90 c2 Dir Base:;
		HLNG 40000000 1 90 c2 Data Stream:;
		 CEQU 0 1 0 14 Resource Fork or DOS;
		 CEQU 1 1 0 14 Data Fork;
	}
	if (g[2] == 2)
	{
		HLNG 40000000 1 90 c2 Volume:;
		HLNG 40000000 1 90 c2 Dir Base:;
		HLNG 40000000 1 90 c2 DOS Dir Base:;
		HLNG 40000000 1 90 c2 Name Space:;
		HLNG 40000000 1 90 c2 Data Stream:;
		 CEQU 0 1 0 14 Resource Fork or DOS;
		 CEQU 1 1 0 14 Data Fork;
	}
	if (g[2] == 3)
	{
		HLNG 40000000 1 90 c2 Volume:;
		HLNG 40000000 1 90 c2 Dir Base:;
		HLNG 40000000 1 90 c2 DOS Dir Base:;
		HLNG 40000000 1 90 c2 Name Space:;
