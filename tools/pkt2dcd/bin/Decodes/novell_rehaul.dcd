void NCP::Type_17::00;
	LABL 0 0 0 b7 Write To Spool File;
	DBYT 0 1 90 c2 Data Length:;
	CHR# 0 1 90 c2 Data:;
	LABL 0 0 0  b8;
	TRTS;

void NCP::Type_17::01;
	LABL 0 0 0 b7 Close Spool File;
	HBYT 0 1 90 c2 Abort Queue Flag:;
	 CEQU 0 1 0 14 Spool file is placed at the end of the print queue;
	 CLSE 0 0 0 14 Spool file is not placed in the print queue;
	LABL 0 0 0  b8;
	TRTS;

void NCP::Type_17::02;
	LABL 0 0 0 b7 Set Spool File Flags;
	HBYT 0 1 90 c9 Print Flags:;
	 CBIT 7 1 0 d4 1... .... Print the banner page;
	 CLSE 0 0 0 d4 0... .... Do Not print the banner page;
	 CBIT 6 1 0 d4 .1.. .... Expand the tabs in the file;
	 CLSE 0 0 0 d4 .0.. .... Do Not expand the tabs in the file;
	 CBIT 5 1 0 d4 ..1. .... Delete the spool file after printing;
	 CLSE 0 0 0 d4 ..0. .... Do Not delete the spool file after printing;
	 CBIT 3 1 0 d4 .... 1... Suppress form feeds;
	 CLSE 0 0 0 d4 .... 0... Do Not suppress form feeds;
	 LABL 0 0 0 c8;
	DBYT 0 1 90 c2 Tab Size:;
	DBYT 0 1 90 c2 Target Printer:;
	DBYT 0 1 90 c2 Copies:;
	DBYT 0 1 90 c2 Form Type:;
	HBYT 0 1 90 c2 Reserved:;
	CHR# d 0 90 c2 Banner Name:;
	LABL 0 0 0  b8;
	TRTS;

