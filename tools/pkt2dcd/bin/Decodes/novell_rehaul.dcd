	 CBIT 0 1 0 d4 .... ...1 Enable Broadcast Messages;
	 LABL 0 0 0 c8;
	HLNG 0 1 90 c2 Connection List Cnt:;
	while (g[2] > 0)
	{
		HLNG 0 1 90 c2 Connection List:;
		g[2] -= 1;
	}
	LABL 0 0 0 b8;
	TRTS;

