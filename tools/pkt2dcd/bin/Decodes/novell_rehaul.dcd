	 CEQU  0 1 0 14 Printer is not halted;
	 CEQU FF 1 0 14 Printer is halted;
	HBYT 0 1 90 c2 Printer Offline:;
	HBYT 0 1 90 c2 Current Form Type:;
	HBYT 0 1 90 c2 Redirected Printer:;
	LABL 0 0 0  b8;
	TRTS;

