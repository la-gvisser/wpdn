void NCP NetWare 5;
	LABL 0 0 0 b1 NCP - NetWare 5 IP\NCP v5:;
	GLNG 0 19;
	GWRD 0 20;
	TEQU 1111 20 0 0 NCP NetWare 5 Binary;
	TEQU 2222 20 0 0 NCP NetWare 5 Binary;
	TEQU 3333 20 0 0 NCP NetWare 5 Binary;
	TEQU 5555 20 0 0 NCP NetWare 5 Binary;
	TEQU 3e3e 20 0 0 NCP NetWare 5 Binary;
	CEQU 446d6454 19;
	SKIP 3;
	CEQU 744e6350 19;
	SKIP 1;
	TNXT -HALT;
	CHR# 4 0 90 c2 Signature:\Sig:;
	DLNG 0 1 90 c2 Length:\Len:;
	TEQU 446d6454 19 0 0 2222 NetWare 5;
	TEQU 744e6350 19 0 0 3333 NetWare 5;

void NCP NetWare 5 Binary;
	HWRD 2 1 90 c2 Signature:\Sig:;
	CEQU 1111 20 0 14 Create a Service Connection;
	CEQU 2222 20 0 14 Request;
	CEQU 3333 20 0 14 Reply;
	CEQU 3e3e 20 0 14 Watchdog;
	//CEQU 5555 -- do not do this one... the 5555::main func will take care of it
	TEQU 1111 20 0 0 NCP::1111::Main;
	TEQU 2222 20 0 0 2222 NetWare 5;
	TEQU 3333 20 0 0 3333 NetWare 5;
	TEQU 3e3e 20 0 0 NCP::3e3e::Main;
	TEQU 5555 20 0 0 NCP::5555::Main;

void 2222 NetWare 5;
	if (g[0x20] != 0x2222)
	{
		DLNG 0 0 90 c2 Version:\Vers:;
		DLNG 0 0 90 c2 Reply Size:\R_Size:;
		HWRD 0 41 90 c2 Request Type:\Type:;
		if (l[1] == 0x5555)
		{
			TNXT -NCP::5555::Main;
		}
	}
	DBYT 0 14 90 c2 Sequence:\Seq:;
	DBYT 0 0 90 c2 Connection Low:\;
	DBYT 0 0 90 c2 Task:;
	DBYT 0 0 90 c2 Connection High:\;
	DBYT 0 5 90 c2 Request Function:\Req Func:;
	CST# 0 5 0 14 NCP_Request_String;
	TST# 0 5 0 0  NCP_Request_String_Func;
	
	DBYT 0 0 90 c2 Sub-Function:;
	DBYT 0 0 90 c2 Source Name Space:;
	DBYT 0 0 90 c2 Dest Name Space:;
	HWRD 0 0 90 c2 Search Flags:;
	HWRD 0 0 90 c2 Return Info:;
	INCR 2;
	DBYT 0 0 90 c2 Volume:;
	HLNG 0 0 90 c2 Directory Base:;
	DBYT 0 0 90 c2 Handle Flag:;
	DBYT 0 0 90 c2 Components:;
	//DUMP 0 0 90 42;

void 3333 NetWare 5;
	if (g[0x20] != 0x3333)
	{
		HWRD 0 0 90 c2 Request Type:\Req Type:;
		TNXT -NCP Reply;
	}
	DBYT 0 14 90 c2 Sequence:\Seq:;
	DBYT 0 0 90 c2 Connection Low:\;
	DBYT 0 0 90 c2 Task:\;
	DBYT 0 0 90 c2 Connection High:\;
	DBYT 0 0 90 c2 Completion Code:\Code:;
