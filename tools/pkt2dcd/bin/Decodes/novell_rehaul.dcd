void NCP::Type_87::20R;
	LABL 0 0 0 b7 Search for File or Subdirectory Set Reply;
	DUMP 9 0 90 c2 Next Search Seq:;
	HBYT 0 1 90 c2 More Entries Flag:;
	 CEQU 0 1 0 14 No More Entries;
	DWRD 40000000 2 90 c2 Info Count:;
	while (g[2] > 0)
	{	
		LABL 0 0 0 b7 Structure;
		NW::Information_Struct;
		l[1] = s[1];
		SBIT 0 41;
		 SKIP 80000001;
		 NW::FileName_Struct;
		LABL 0 0 0 b8;
		g[2] -= 1;
	}
	LABL 0 0 0 b8;
	TRTS;

