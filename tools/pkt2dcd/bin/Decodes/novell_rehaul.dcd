void NCP_Limb_Struct;
	LABL		0 0  0 b9 Limb Structure;
	HLNG 40000000 1 90 c9 Flags:;
	 CBIT 6 1 0 d4 ..1. .... Allow Hidden Files/Folders;
	 CLSE 0 0 0 d4 ..0. .... Do Not Allow Hidden Files/Folders;
	 CBIT 5 1 0 d4 ...1 .... Allow System Files/Folders;
	 CLSE 0 0 0 d4 ...0 .... Do Not Allow System Files/Folders;
	 CBIT 4 1 0 d4 .... 1... Scan Folders Only (valid only if Scan Entire Folder is set);
	 CLSE 0 0 0 d4 .... 0... Don't Scan Folders Only;
	 CBIT 3 1 0 d4 .... .1.. Scan Files Only (valid only if Scan Entire Folder is set);
	 CLSE 0 0 0 d4 .... .0.. Don't Scan Files Only;
	 CBIT 2 1 0 d4 .... ..1. Scan Entire Folder (wild search);
	 CLSE 0 0 0 d4 .... ..0. Don't Scan Entire Folder;
	 LABL 0 0 0 c8;
	DLNG 40000000 1 90 c2 Volume Num:;
	HLNG 40000000 1 90 c2 Directory Base:;
	DLNG 40000000 1 90 c2 Scan Number:;
	HLNG 40000000 1 90 c2 Name Space:;
	LABL		0 0  0 b8;
	TRTS;

void NW_Handle_Path_Struct;
	DBYT 0 1 90 c2 Volume Number:;
	HLNG 40000000 1 90 c2 Directory Base:;
	HBYT 0 1 90 c2 Handle Flag:;
	 CEQU  0 1 0 14 Short Directory Handle;
	 CEQU  1 1 0 14 Directory Base;
	 CEQU ff 1 0 14 No Handle Present;	 
	DBYT 0 2 90 c2 Path Component Count:;
	NCP_Path_Components;

void NCP_Path_Components;
	MOVE 0 3;
	TNXT -NCP_Path_Components_Helper;
	
void NCP_Path_Components_Helper;
	TGTE 0  2 0 0 RETURN;		//si: holds number of paths
	TGTE 0 10 0 0 RETURN;
	SUBG 1  2;
	ADDG 1  3;
	LABL 0 0 90 c9 Path;
	DGLB 0 3 0 14 -;
	DBYT 0 1 90 c2 Path Length:;
	if (g[1] != 0) {
	  CHR# 0 1 90 c2 Path:;
	}
	LABL 0 0  0 c8;
	TNXT -NCP_Path_Components_Helper;

