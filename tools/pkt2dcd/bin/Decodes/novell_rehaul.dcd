void NCP::Type_87::24R;
	LABL 0 0  0 b7 Get Name Spaces Loaded List from Vol Num Reply;
	DWRD 40000000 2 90 c2 Num of NS Loaded:;
	while (g[2] > 0)
	{
		HBYT 0 1 90 c2 Name Space:;
		CEQU 0 1 00 14 DOS;
		CEQU 1 1 00 14 MAC;
		CEQU 2 1 00 14 NFS;
		CEQU 4 1 00 14 OS/2;
		g[2] -= 1;
	}
	LABL 0 0 0 b8;
	TRTS;

