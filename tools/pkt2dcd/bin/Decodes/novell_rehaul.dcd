void NCP::Type_22::32R;
	LABL 0 0 0 b7 Scan Vol's User Disk Restrictions Reply;
	DBYT 0 2 90 c2 Num of Entries:;
	while (g[2] > 0)
	{
		TGTE 0 10 0 0 RETURN;
		HLNG		0 1 90 c2 Object ID:;
		HLNG 40000000 1 90 c2 Restriction:;
		g[2] -= 1;
	}
	LABL 0 0 0 b8;
	TRTS;

