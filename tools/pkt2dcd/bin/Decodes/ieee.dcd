{
	LABL 0 0 0 b7 Vendor Specific;
	l[1] = pl[0];
	ANDG ffffff00 41;
	802_11::Print_OUI();
	DBYT 0 41 90 c2 Subtype:;
	DBYT 0 42 90 c2 P2P Action Subtype:;
		CST# 0 42 0 14 802_11::Mgmt_Action::IE_Type_221::P2P Action Subtype;
