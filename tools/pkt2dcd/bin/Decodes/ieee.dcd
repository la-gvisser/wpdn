	HEX# 80000001 0 90 c2 OUI:;
	HEX# 80000001 0 90 00 -;
	HEX# 80000001 0 90 00 -;
	SHFR 8 41;
	CST# 0 41 0 14 802_11::OUI_Strings;
	TRTS;
