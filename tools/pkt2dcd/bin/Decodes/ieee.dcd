void 802_11::IE_SSN Unicast Loop;
	TGTE 0 3 0 0 RETURN;
	HEX# 80000001 0 90 c2 Unicast cipher OUI:;
	HEX# 80000001 0 90 00 -;
	HEX# 80000001 0 90 00 -;
	HBYT 80000000 8b 90 00 -;
	CEQU 0 b 0 14 None;
	CEQU 1 b 0 14 WEP;
	CEQU 2 b 0 14 TKIP;
	CEQU 3 b 0 14 WRAP;
	CEQU 4 b 0 14 CCMP;
