		}

		if (l[3] != 1)
		{
			LABL( 0, 0, 0, 0x04, " dBm" );
		}
	}

	if (g[2] >= 1)
	{
		DBYT( 0, l[3], 0x90, 0xc2, "Local Maximum Transmit Power For 80 MHz:" );
