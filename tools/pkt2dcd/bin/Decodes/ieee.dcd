		ETHR( 0, 0, 0x90, 0xc2, "BSSID:");
		ETHR( 0, 0, 0x90, 0xc2, "Source:");
		ETHR( 0, 0, 0x90, 0xc2, "Destination:");
		HEX#( 0, g[16], 0x90, 0xc2, "Unknown Content:" );
	}
	
