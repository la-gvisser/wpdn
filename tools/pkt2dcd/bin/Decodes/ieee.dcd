	DECR 1;
	BBIT( 4, l[3], 0x90, 0xc2, "OI #1 Length" );
	BBIT( 4, l[4], 0x90, 0xc2, "OI #2 Length" );
	LABL( 0, 0, 0, 0xb8 );
