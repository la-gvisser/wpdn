void 802_11::TDLS_QoS_Traffic_Capability;
	DBYT( 0, 0, 0x90, 0xc2, "QoS Capability:" );
	DBYT( 0, l[1], 0x90, 0xc2, "Length:" );

	while (l[1] != 0)
	{
		DBYT( 0, 0, 0x90, 0xc2, "QoS Info:" );
		l[1] -= 1;
	}

	TRTS;

