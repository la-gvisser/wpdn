void 802_11::IE_Type_41;					//IBSS DFS
	ETHR 0 0 90 c2 DFS Owner:;
	DBYT 0 0 90 c2 DFS Recovery Interval:;
	g[2] -= 7;
	while (g[2] >= 2)
	{
		LABL 0 0 00 b9 Channel Map;
		DBYT 0 0 90 c2 Channel Num:;
		DBYT 0 0 90 c2 Map:;
		LABL 0 0 00 b8;
		g[2] -= 2;
	}
	TRTS;

