void 802_11::Mgmt_Action::Public::TDLS Discovery Response()
{
	LABL( 0, 0, 0x90, 0xc9, "TDLS Discovery Response:" );
	
		DBYT 0 42 90 c2 Dialog Token:;
		802_11::Capability Information;

		GBYT( 0, l[3] );
		if (l[3] == 0x01)
		{
			802_11::TDLS_IE_Supported_Rates;
