void 802_11::Wave_Service_Information()
{
	DBYT( 0, l[1], 0x90, 0xc2, "Timing Capabilities:" );
		CEQU( 1, l[1], 0x0, 0x14, "Timestamp offset based on UTC" );
		CLSE( 0, 0, 0, 0x14, "Reserved" );
	g[2] -= 1;
	if (g[2] >= 10)
	{
		HEX#( 0x40000000, 10, 0x90, 0xc2, "Time Value:" );
