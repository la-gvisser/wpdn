			l[44] /= 1000000;
			DGLB( 0x40000000, l[44], 0x04, 0 );
			LABL( 0, 0, 0, 0x04, " Seconds" );
		}
		* SamChen:  Conversion completed

		DBYT 0 45 90 c2 Reported Frame Information:;
		DBYT 0 46 90 c2 RCPI:;
		DBYT 0 47 90 c2 RSNI:;
		ETHR 0 0 90 c2 BSSID:;
