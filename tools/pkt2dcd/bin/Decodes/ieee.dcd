	CEQU 0 b 0 14 None;
	CEQU 1 b 0 14 SSN;
	CEQU 2 b 0 14 SSNPSK;
	SUBG 1 3;
	g[2] -= 4;
	TNXT -802_11::IE_SSN Auth Loop;

	
