	//DGLB 0 2 90 c2 g2:;
	while ( g[16] > 0)
	{
		ETHR( 0, l[1], 0x90, 0xc2, "Adress:" );
		ETHR( 0, l[1], 0x90, 0xc2, "Adress:" );
