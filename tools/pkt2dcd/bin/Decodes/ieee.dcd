	if (g[2] > 0)
	{
		DWRD( 0x40000000, l[2], 0x90, 0xc2, "Local Link ID:" );				g[2] -= 2;
	}
	if (g[2] > 0)
	{
		DWRD( 0x40000000, l[2], 0x90, 0xc2, "Peer Link ID:" );				g[2] -= 2;
	}
	if (g[2] > 0)
	{
		DWRD( 0x40000000, l[2], 0x90, 0xc2, "Reason Code:" );				g[2] -= 2;
	}
	if (g[2] >= 2)
	{
		DWRD( 0x40000000, l[2], 0x90, 0xc9, "Chosen PMK :" );				g[2] -= 2;
	}
}

