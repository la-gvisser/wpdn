				802_11::Main2;
			}
			if(l[2] == 221)
			{
				802_11::IE_Type_221;
			}
		}
	}
	if (g[5] == 6)
	{
		DBYT 0 41 90 c2 Operating Class:;
		DBYT 0 42 90 c2 Channel Number:;
		D64B 0 43 90 c2 Actual Measurement Start Time:;
		DWRD 0 44 90 c2 Measurement Duration:;
