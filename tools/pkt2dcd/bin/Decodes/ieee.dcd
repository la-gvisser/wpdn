	HEX#( 0, l[4], 0x90, 0xc2, "OI #2:" );
	g[2] -= l[4];
	if (g[2] != 0)
	{
		HEX#( 0, g[2], 0x90, 0xc2, "OI #3:" );	
	}	
	TRTS;
}
