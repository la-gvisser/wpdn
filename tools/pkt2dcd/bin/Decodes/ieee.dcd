void 802_11::PXUC()
{
	DBYT( 0, l[1], 0x90, 0xc2, "PXU ID:" );			g[2] -= 1;
	ETHR( 0, 0, 0x90, 0xc2, "PXU Recipient MAC Address:" );				g[2] -= 6;
