void SMB2::NEGOTIATE( )
{
	LABL( 0, 0, 0, 0xb7, "SMB2 NEGOTIATE Request" );
		DWRD( 0x40000000, 0, 0x90, 0xc2, "StructureSize:" );
		HWRD( 0x40000000, l[2], 0x90, 0xc2, "DialectCount:" );
		HWRD( 0x40000000, l[3], 0x90, 0xc2, "SecurityMode:" );	
			CEQU( 0x0001, l[3], 0, 0x14, "SMB2_NEGOTIATE_SIGNING_ENABLED" );
			CEQU( 0x0002, l[3], 0, 0x14, "SMB2_NEGOTIATE_SIGNING_REQUIRED" );	
			CEQU( 0x0002, l[3], 0, 0x14, "SMB2_NEGOTIATE_SIGNING_REQUIRED" );	
		HWRD( 0x40000000, , 0x90, 0xc2, "Reserved:" );	
		HLNG( 0x40000000, , 0x90, 0xc2, "Capatilities:" );
			CEQU( 0x00000001, l[5], 0, 0x14, "SMB2_GLOBAL_CAP_DFS" );
			CEQU( 0x00000002, l[5], 0, 0x14, "SMB2_GLOBAL_CAP_LEASING" );
			CEQU( 0x00000003, l[5], 0, 0x14, "SMB2_GLOBAL_CAP_LARGE_MTU" );
		SMB2::GUID;
		TIME( 4, 0, 0x90, 0xc2, "ClientStartTime:" );
		LABL( 0, 0, 0x90, 0xc9 , "Dialects:" );
			while( l[2] >= 1 )
			{
			HWRD( 0x40000000, l[3], 0x90, 0xc2, "Dialect:" );
					CEQU( 0x0202, l[3], 0, 0x14, "SMB 2.002 dialect revision number" );
					CEQU( 0x0210, l[3], 0, 0x14, "SMB 2.1 dialect revision number" );
				l[2] -= 1; 
			}
		LABL( 0, 0, 0, 0xb8 );
	LABL( 0, 0, 0, 0xb8 );
}

void SMB2::NEGOTIATE::Response( )
{
	LABL( 0, 0, 0, 0xb7 , "SMB2 NEGOTIATE Response" );
		DWRD( 0x40000000, 0, 0x90, 0xc2, "Length:" );
		HWRD( 0x40000000, l[2], 0x90, 0xc2, "SecurityMode:" );
			CEQU( 0x0001, l[2], 0, 0x14, "SMB2_NEGOTIATE_SIGNING_ENABLED" );
			CBIT( 0x0002, l[2], 0, 0x14, "SMB2_NEGOTIATE_SIGNING_REQUIRED" );	
		HWRD( 0x40000000, l[3], 0x90, 0xc2, "DialectRevision:" );
			CEQU( 0x0202, l[3], 0, 0x14, "SMB 2.002 dialect revision number" );
			CEQU( 0x0210, l[3], 0, 0x14, "SMB 2.1 dialect revision number" );
			CEQU( 0x02FF, l[3], 0, 0x14, "SMB2 wildcard revision number" );
		HWRD( 0x40000000, 0, 0x90, 0xc2, "Reserved:" );
		SMB2::GUID;
		HLNG( 0x40000000, l[5], 0x90, 0xc9, "Capatilities:" );
		SMB2::Capatilities;
		DLNG( 0x40000000, 0, 0x90, 0xc2, "MaxTransactionSize:" );
		DLNG( 0x40000000, 0, 0x90, 0xc2, "MaxReadSize:" );
		DLNG( 0x40000000, 0, 0x90, 0xc2, "MaxWriteSize:" );
		TIME( 4, l[13], 0x90, 0xc2, "ClientStartTime:" );
		TIME( 4, l[13], 0x90, 0xc2, "ServerStartTime:" );
		//DGLB( 0x40000000, g[16], 0x90, 0xc2, "g16:" );
		DWRD( 0x40000000, g[20], 0x90, 0xc2, "SecurityBufferOffset:" );
		DWRD( 0x40000000, g[21], 0x90, 0xc2, "SecurityBufferLength:" );
		DLNG( 0x40000000, l[2], 0x90, 0xc2, "Reserved:" );
		LABL( 0, 0, 0x90, 0xc2 , "SecurityBuffer:" );
		SMB2::GSSDUMPREST;
	LABL( 0, 0, 0, 0xb8 );
}

void SMB2::ERROR::Response( )
{
	LABL( 0, 0, 0, 0xb7, "SMB2 ERROR Response" );
		DWRD( 0x40000000, 0, 0x90, 0xc2, "StructureSize:" );
		DWRD( 0x40000000, l[3], 0x90, 0xc2, "Reserved:" );		
		HLNG( 0x40000000, l[14], 0x90, 0xc2, "ByteCount:" );
		if ( g[19] != 0x8000002D)
		{
			HBYT( 0x40000000, l[13], 0x90, 0xc2, "Error Message:" );
		}
		if ( g[19] == 0x8000002D)
		{		
			DLNG( 0x40000000, l[15], 0x90, 0xc2, "SymLinkLength:" );
			HLNG( 0x40000000, l[16], 0x90, 0xc2, "SymLinkErrorTag:" );
				CEQU( 0x4C4D5953, l[16], 0, 0x14, "right set" );
				CLSE( 0, l[16], 0, 0x14, "not right set" );
			HLNG( 0x40000000, l[17], 0x90, 0xc2, "ReparseTag:" );
				CEQU( 0xA000000C, l[17], 0, 0x14, "right set" );
				CLSE( 0, l[17], 0, 0x14, "not right set" );
			DWRD( 0x40000000, l[18], 0x90, 0xc2, "ReparseDataLength:" );
			DWRD( 0x40000000, l[19], 0x90, 0xc2, "UnparsedPathLength:" );
			DWRD( 0x40000000, l[20], 0x90, 0xc2, "SubstituteNameOffset:" );
			DWRD( 0x40000000, l[21], 0x90, 0xc2, "SubstituteNameLength:" );
			DWRD( 0x40000000, l[22], 0x90, 0xc2, "PrintNameOffset:" );
			DWRD( 0x40000000, l[23], 0x90, 0xc2, "PrintNameLength:" );
			HLNG( 0x40000000, l[24], 0x90, 0xc2, "Flags:" );
				CEQU( 0x00000000, l[24], 0, 0x14, "The substitute name is an absolute target path name" );
				CEQU( 0x00000001, l[24], 0, 0x14, "SYMLINK_FLAG_RELATIVE" );
				CLSE( 0, l[24], 0, 0x14, "not right set" );
		}	
	LABL( 0, 0, 0, 0xb8 );
}

void SMB2::SESSION_SETUP( )
{
	LABL( 0, 0, 0, 0xb7, "SMB2 SESSION_SETUP Request" );
		DWRD( 0x40000000, 0, 0x90, 0xc2, "StructureSize:" );
		HBYT( 0x40000000, l[2], 0x90, 0xc2, "VcNumber:" );
		HBYT( 0x40000000, l[2], 0x90, 0xc2, "SecurityMode:" );
			CEQU( 0x0001, l[3], 0, 0x14, "SMB2_NEGOTIATE_SIGNING_ENABLED" );
			CEQU( 0x0002, l[3], 0, 0x14, "SMB2_NEGOTIATE_SIGNING_REQUIRED" );		
		HLNG( 0x40000000, , 0x90, 0xc2, "Capatilities:" );
			CEQU( 0x00000001, l[5], 0, 0x14, "SMB2_GLOBAL_CAP_DFS" );
			CEQU( 0x00000002, l[5], 0, 0x14, "SMB2_GLOBAL_CAP_UNUSED1" );
			CEQU( 0x00000004, l[5], 0, 0x14, "SMB2_GLOBAL_CAP_UNUSED2" );
			CEQU( 0x00000008, l[5], 0, 0x14, "SMB2_GLOBAL_CAP_UNUSED3" );
		HLNG( 0x40000000, , 0x90, 0xc2, "Channel:" );
		DWRD( 0x40000000, g[20], 0x90, 0xc2, "SecurityBufferOffset:" );
		DWRD( 0x40000000, g[21], 0x90, 0xc2, "SecurityBufferLength:" );	
		D64B( 0x40000000, l[13], 0x90, 0xc2, "PreviousSessionId:" );
		LABL( 0, 0, 0x90, 0xc2 , "Buffer:" );
		SMB2::GSSDUMPREST:Ses_Set1;
	LABL( 0, 0, 0, 0xb8 );
}

void SMB2::SESSION_SETUP::Response( )
{
	LABL( 0, 0, 0, 0xb7, "SMB2 SESSION_SETUP Response" );
		DWRD( 0x40000000, 0, 0x90, 0xc2, "StructureSize:" );
		HWRD( 0x40000000, l[2], 0x90, 0xc2, "SessionFlags:" );
			CEQU( 0x0001, l[2], 0, 0x14, "SMB2_SESSION_FLAG_IS_GUEST" );
			CEQU( 0x0002, l[2], 0, 0x14, "SMB2_SESSION_FLAG_IS_NULL" );
		DWRD( 0x40000000, g[20], 0x90, 0xc2, "SecurityBufferOffset:" );
		DWRD( 0x40000000, g[21], 0x90, 0xc2, "SecurityBufferLength:" );	
		LABL( 0, 0, 0x90, 0xc2 , "Buffer:" );
		SMB2::GSSDUMPREST:Ses_RESP;
	LABL( 0, 0, 0, 0xb8 );
}

