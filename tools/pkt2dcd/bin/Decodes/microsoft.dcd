	LABL 0 0 0 24 milliseconds;
	HWRD 40000000 4 90 c2 Reserved:;
	DWRD 40000000 8 90 c2 Params This Buffer:\PTB:;
	DWRD 40000000 3 90 c2 Params Bytes Offset:\PBO:;
	DWRD 40000000 9 90 c2 Data This Buffer:\DTB:;		*SAVE
	DWRD 40000000 8 90 c2 Data Bytes Offset:\DBO:;		*SAVE
	DBYT 0 4 90 c2 Setup Word Count:\SWC:;
	HBYT 0 0 90 c2 Reserved:;

	SEQU 0  4;
	SKIP 4;
	SUBG 4 81;
	MULG 2  4;
	GWRD 40000000 5;  *save to test for subcommands

	SMB_Test_Generic_Mailslot;					*SI: tests all \MailSlot\BLAH
	TGTE 0 10 0 0 RETURN;

	GBYT 0 2;	*SI: if /PIPE/ is legit...this will tell you
	GWRD 0 6;	*SI: holds file ID#
	SMB_Pipe_Test1;

	if (g[4] > 0)
	{
		HEX# 0 4 90 c2 Setup Words:;
	}
	
	SUBG e  1;
	SEQU 0  1;
	SKIP 2;
	MULG 2  1;
	DUMP 0  1 90 c2 Additional Bytes:;

	DWRD 40000000 1 90 c2 Byte Count:\Bytes:;
	MOVE 1 8c;

	SMB Test Pipe;

	MARK 3 18 0 0 SMB_Func_Test;
	SMB Test MSRAP;
	TGTE 0 10 0 0 RETURN;

	SEQU 0 2;	*SI: g2 is precious....don't kill it
	SKIP 4;
	INCR 1;
	MARK 2 18  0  0 SMB_Func_Test;
	SMB Test Pipe; *Trans2 Transaction Name; 
	TGTE 0 10 0 0 RETURN;
	DECR 1;

	SUBG 10 92;
	*DGLB 0 12 90 C2 THIS IS GLOBAL 18-og:;
	MOVE 12 87;
							*SI: this takes care of padding...I DESPISE PADDING
	*MOVE 41 7;					* move 55 [amt bytes since header] into g7
	ADDG 1 87;					* add the byte count to g7
	SUBG 3 87;					* g7 = g7 - params bytes offset
	MOVE 1 82;					* move byte count to g2
	SUBG 7 82;					* g2 = g2 - g7
	*DGLB 0 2 90 C2 THIS IS GLOBAL 2:;
	HEX# 0  2 90 c2 Padding:;	* first padding
	MOVE 3 82;					* move param bytes offset to g2
	ADDG 6  2;					* add 6 to g2 [the 6 because most have 6 bytes accounted in the function]
	SUBG 2 88;					* sub g8 = g8 - g2 = data bytes offset - param bytes offset
	MOVE 8 86;					* save in g6
		*MOVE 10 82;				* move bytes left into g2
		*SUBG 8 82;					* g2 = g2 - params this buffer
		*SUBG 9 82;					* g2 = g2 - data bytes this buffer
		*MOVE 2 86;	*SI: pass g6 to next function for padding

	GBYT 0 3;		*SI: save to see if pad byte really occurred or not...
	Pad Byte;		*SI: this is the bane of my existence...who put this here!
	*SMB Test MSRAP;

	*SMB Test Pipe;
	*TEQU 0 10 0 0 RETURN;

	SNEQ 0 3;		*SI: gets rid of padbyte IF used
	SKIP 1;
	DECR 1;			*decr pad byte

	*SI: i moved this here...because trans2 trans name isn't supposed to be called for trans2 stuff
	*test Additional Setup Bytes for a command
	*MOVE 10 8d;		*added this 3-28
	*SUBG c 8d;			*added this 3-28
	*SEQU 0 d;			*added this 3-28
	*SKIP 2;			*added this 3-28
	*HEX# 0 d 90 c2 Padding:;*added this 3-28
	*LABL 0 0 90 c2 HERE;
	ADDG  1 5;						* add 1 t 5 because str# arrays are 1 based
	MARK  1 18 0 0 SMB_Func_Test;
	TST#  0 5 0 0 Trans2 Commands;

		*DECR 1;  * Pad byte we consumed earlier
	Trans2 Transaction Name; 
	Pad Byte;
		*SNEQ 50 4; 
		*SKIP 1;

		*SNEQ 500 3; *SI: 500 = trans2 query path...there's 1 byte of padding there
		*SKIP 1;		*    this takes that into account...
		*INCR 1;

		*SI: This is where that ADDG & TST# used to be

		*CSTR 0 4 90 c2 Transaction Name:\Name:;

	SUBG 4 81;
	DUMP 1 0 90 c2 Parameter And Data Bytes:;  *value==1 to leave room for FCS
	LABL 0 0 0 30;
	TRTS;

