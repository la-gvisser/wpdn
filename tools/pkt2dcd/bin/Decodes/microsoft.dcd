void SMB NTTransact Rsp;
	TGTE 24 10 0 0 RETURN;
	INCR 24;		* 35dec is 23hex
	GBYT 40000000 3 90 c2;	* this is setup count
	INCR 1;
	ADDG 12 3;		* 18dec is 12hex
				* 3 is now 18+setup
	DECR 25;
	GBYT 40000000 4 90 c2;	* this is word count
	SNEQ 3 84;
	SKIP 17;  	*21dec is 15hex
	LABL 0 0 0 b1 SMB NT Transact Response\Response:;
	DBYT 40000000 4 90 c2 Word Count:\;
	HBYT 40000000 4 90 c2 Reserved1:;
	HWRD 40000000 4 90 c2 Reserved2:;
	DLNG 40000000 4 90 c2 Total Parameter Count:;
	DLNG 40000000 4 90 c2 Total Data Count:;
	DLNG 40000000 4 90 c2 Parameter Count:;
	DLNG 40000000 4 90 c2 Parameter Offset:;
	DLNG 40000000 4 90 c2 Parameter Displacement:;
	DLNG 40000000 4 90 c2 Data Count:;
	DLNG 40000000 4 90 c2 Data Offset:;
	DLNG 40000000 4 90 c2 Data Displacement:;
	DBYT 40000000 3 90 c2 Setup Count:;
	MOVE 3 85;
	MULG 2 5;
	HEX# 0 5 90 c2 Setup Words:;
	DWRD 40000000 4 90 c2 Byte Count:\Bytes:;
	DUMP 0 4 90 c2 Byte Parameters:;
	DECR 0 4;
	*[CBB] the following eats the FCS (TT3553)
	*MULG 2 5;
	INCR 0 5;
	SUBG 5 84;
	DUMP 0 4 90 c2 Data Parameters:;

