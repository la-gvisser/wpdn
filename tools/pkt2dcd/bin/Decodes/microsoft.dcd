void SMB_Test_Generic_Mailslot;
	*test for Unicode
	INCR 8;	*SI: \mailslot\browse found 8 bytes forward...i acct for this in smb browse
	SBIT f 11;
	SKIP 3;

	SCMP a 0  0  0 \MAILSLOT\;
	SKIP 80000002;
	TNXT -SMB_Generic_Mailslot;

	*test for Non-Unicode
	SBIT f 11;
	SKIP 80000003;
	
	SCMP 4000000a 0  0  0 \MAILSLOT\; 
	SKIP 80000001;
	TNXT -SMB_Generic_Mailslot;

	DECR 8;
	TRTS;

