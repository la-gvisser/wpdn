					CBIT( 2, l[5], 0, 0xd4, ".... .... .... .... .... .... .... .1.. FILE_ATTRIBUTE_SYSTEM: System" );
					CLSE( 2, l[5], 0, 0xd4, ".... .... .... .... .... .... .... .0.. FILE_ATTRIBUTE_SYSTEM: Not System" );
					CBIT( 4, l[5], 0, 0xd4, ".... .... .... .... .... .... ...1 .... FILE_ATTRIBUTE_DIRECTORY: Directory" );
					CLSE( 4, l[5], 0, 0xd4, ".... .... .... .... .... .... ...0 .... FILE_ATTRIBUTE_DIRECTORY: File" );
					CBIT( 5, l[5], 0, 0xd4, ".... .... .... .... .... .... ..1. .... FILE_ATTRIBUTE_ARCHIVE: Archive" );
					CLSE( 5, l[5], 0, 0xd4, ".... .... .... .... .... .... ..0. .... FILE_ATTRIBUTE_ARCHIVE: Not Archive" );
					CBIT( 6, l[5], 0, 0xd4, ".... .... .... .... .... .... .1.. .... FILE_ATTRIBUTE_DEVICE: Device" );
					CLSE( 6, l[5], 0, 0xd4, ".... .... .... .... .... .... .0.. .... FILE_ATTRIBUTE_DEVICE: Not Device" );
					CBIT( 7, l[5], 0, 0xd4, ".... .... .... .... .... .... 1... .... FILE_ATTRIBUTE_NORMAL: Normal" );
					CLSE( 7, l[5], 0, 0xd4, ".... .... .... .... .... .... 0... .... FILE_ATTRIBUTE_NORMAL: Not Normal" );
					CBIT( 8, l[5], 0, 0xd4, ".... .... .... .... .... ...1 .... .... FILE_ATTRIBUTE_TEMPORARY: Temporary" );
					CLSE( 8, l[5], 0, 0xd4, ".... .... .... .... .... ...0 .... .... FILE_ATTRIBUTE_TEMPORARY: Permanent" );
					CBIT( 9, l[5], 0, 0xd4, ".... .... .... .... .... ..1. .... .... FILE_ATTRIBUTE_SPARSE_FILE: Sparse" );
					CLSE( 9, l[5], 0, 0xd4, ".... .... .... .... .... ..0. .... .... FILE_ATTRIBUTE_SPARSE_FILE: Not Sparse" );
					CBIT( 10, l[5], 0, 0xd4, ".... .... .... .... .... .1.. .... .... FILE_ATTRIBUTE_REPARSE_POINT: Reparse Point" );
					CLSE( 10, l[5], 0, 0xd4, ".... .... .... .... .... .0.. .... .... FILE_ATTRIBUTE_REPARSE_POINT: Not Reparse Point" );
					CBIT( 11, l[5], 0, 0xd4, ".... .... .... .... .... 1... .... .... FILE_ATTRIBUTE_COMPRESSED: Compressed" );
					CLSE( 11, l[5], 0, 0xd4, ".... .... .... .... .... 0... .... .... FILE_ATTRIBUTE_COMPRESSED: Uncompressed" );
					CBIT( 12, l[5], 0, 0xd4, ".... .... .... .... ...1 .... .... .... FILE_ATTRIBUTE_OFFLINE: online" );
					CLSE( 12, l[5], 0, 0xd4, ".... .... .... .... ...0 .... .... .... FILE_ATTRIBUTE_OFFLINE: offline" );
					CBIT( 13, l[5], 0, 0xd4, ".... .... .... .... ..1. .... .... .... FILE_ATTRIBUTE_NOT_CONTENT_INDEXED: Content indexed" );
					CLSE( 13, l[5], 0, 0xd4, ".... .... .... .... ..0. .... .... .... FILE_ATTRIBUTE_NOT_CONTENT_INDEXED: Content not indexed" );
					CBIT( 14, l[5], 0, 0xd4, ".... .... .... .... .1.. .... .... .... FILE_ATTRIBUTE_ENCRYPTED: Encrypted" );
					CLSE( 14, l[5], 0, 0xd4, ".... .... .... .... .0.. .... .... .... FILE_ATTRIBUTE_ENCRYPTED: Unencrypted" );
					LABL( 0, 0, 0, 0xd4, "xxxx xxxx xxxx xxxx x... .... .... .... Reserved" );
				LABL( 0, 0, 0, 0xb8 );
				DLNG( 0x40000000, l[3], 0x90, 0xc2, "FileNameLength:" );
				if (l[3] >= 1)
				{
					CHR#( 0x40000000, l[3], 0x90, 0xc2, "Filename:" ); 
				}
					g[20] -= l[1];
			if (l[1] != 0)
			{
				

				BREM( 0, l[22] );
