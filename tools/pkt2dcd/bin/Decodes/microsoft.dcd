void Trans2 Transaction Name;
	* test first byte for 0
	GBYT  0  1;
	SNEQ  0  1;
	SKIP  2;
	DWRD  40000000 1 90 c2 Transaction Name:\Name:;
	TRTS;

	* test first byte for FF
	SNEQ  FF 1;
	SKIP  2;
	DWRD  40000000 1 90 c2 Transaction Name:\Name:;
	TRTS;

	* test second byte for 0
	INCR  1;
	GBYT  0  1;
	DECR  1;
	SNEQ  0  1;
	SKIP  2;
	DWRD  40000000 1 90 c2 Transaction Name:\Name:;
	TRTS;

	* test second byte for FF
	SNEQ  FF 1;
	SKIP  2;
	DWRD  40000000 1 90 c2 Transaction Name:\Name:;
	TRTS;

	TSUB  -Trans2 Transaction Name 2;
	TRTS;

