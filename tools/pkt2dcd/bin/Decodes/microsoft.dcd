void SMB::NTLMSSP_Check;
	g[1] = pl[0];
	// 0x4e544c4d = NTLM
	if ( g[1] == 0x4e544c4d )
	{
		g[1] = pl[4];
		// 0x53535000 = SSP.
		if ( g[1] == 0x53535000 )
		{
			g[20] = 100;
			//TNXT -SMB::NTLMSSP;
			TNXT -NTLM2;
		}
	}
	g[20] = 20;
	TRTS;

void SMB::NTLMSSP;
	LABL		0 0  0 b7 NTLMSSP;
	CHR#		8 0 90 c2 NTLMSSP Identifier:;
	HLNG 40000000 1 90 c2 NTLMSSP Message Type:;
	CST#		0 1  0 14 SMB::NTLMSSP_Msg_Types::List;
	TST#		0 1  0  0 SMB::NTLMSSP_Msg_Types::Func;
	DUMP		0 10 90 c2 Unknown NTLMSSP Msg:;
	TRTS;

str# SMB::NTLMSSP_Msg_Types::List;
	1 | Negotiate;
	2 | Challenge;
	3 | Authenticate;
	8 | Unknonwn Message;

str# SMB::NTLMSSP_Msg_Types::Func;
	1 | SMB::NTLMSSP::Negotiate;
	2 | SMB::NTLMSSP::Challenge;
	3 | SMB::NTLMSSP::Authenticate;
	8 | SMB::NTLMSSP::Unknonwn_Message;

void SMB::NTLMSSP::Negotiate;					// 1
	HLNG 40000000 1 90 c9 Flags:;
	 CBIT f 1 0 d4 ........ ........ 1....... ........ Sign for All Security Levels;
	 CLSE 0 0 0 d4 ........ ........ 0....... ........ Do Not Sign for All Security Levels;
	 CBIT e 1 0 d4 ........ ........ .1...... ........ Client/Server on Same Machine;
	 CLSE 0 0 0 d4 ........ ........ .0...... ........ Client/Server Not on Same Machine;
	 CBIT 9 1 0 d4 ........ ........ ......1. ........ NTLM Authentication;
	 CLSE 0 0 0 d4 ........ ........ ......0. ........ NTLM Authentication Not Set;
	 CBIT 7 1 0 d4 ........ ........ ........ 1....... Use LM Session Key for Sign/Seal;
	 CLSE 0 0 0 d4 ........ ........ ........ 0....... Do Not Use LM Session Key for Sign/Seal;
	 CBIT 5 1 0 d4 ........ ........ ........ ..1..... Request Confidentiality;
	 CLSE 0 0 0 d4 ........ ........ ........ ..0..... Confidentiality Not Requested;
	 CBIT 4 1 0 d4 ........ ........ ........ ...1.... Request Signature Capability;
	 CLSE 0 0 0 d4 ........ ........ ........ ...0.... Signature Capability Not Requested;
	 CBIT 2 1 0 d4 ........ ........ ........ .....1.. Server Return its Auth Realm;
	 CLSE 0 0 0 d4 ........ ........ ........ .....0.. Server Not to Return its Auth Realm;
	 CBIT 1 1 0 d4 ........ ........ ........ ......1. Text Strings Are in OEM;
	 CLSE 0 0 0 d4 ........ ........ ........ ......0. Text Strings Are Not in OEM;
	 CBIT 0 1 0 d4 ........ ........ ........ .......1 Text Strings Are in Unicode;
	 CLSE 0 0 0 d4 ........ ........ ........ .......0 Text Strings Are Not in Unicode;
	LABL 0 0  0 c8;
	
	DWRD 40000000 2 90 c2 Domain Length:;
	DWRD 40000000 3 90 c2 Domain Length:;
	DWRD 40000000 4 90 c2 Domain Offset:;
	HEX# 2 0 90 c2 Padding:;
	
	DWRD 40000000 5 90 c2 Host Length:;
	DWRD 40000000 6 90 c2 Host Length:;
	DWRD 40000000 7 90 c2 Host Offset:;
	HEX# 2 0 90 c2 Padding:;
	g[7] -= 32;									//si: there were 32 bytes 
	if (g[7] > 0) {
		INCR 0 7;
	}
	if ( g[5] > 0 ) {
		CHR# 0 5 90 c2 Host String:;
	}
	g[4] -= 32;
	g[4] -= g[7];
	g[4] -= g[5];
	if (g[4] > 0) {
		INCR 0 4;
	}
	if (g[2] > 0) {
		CHR# 0 2 90 c2 Domain String:;
	}
	LABL 0 0 0 b8;
	TRTS;

void SMB::NTLMSSP::Challenge;					// 2
	g[2] = 0;									//si: used to count how many bytes we've used for the value fields
	g[3] = 12;									//si: used to count the bytes from the beginning of ntlmssp data
	
	LABL 0 0 0 c9 Target Domain:;
	 g[4] = 1;
	 SMB::NTLMSSP::Authenticate::Sec_Buffer;
	 LABL 0 0 0 c8;
	HLNG 40000000 1 90 c9 Negotiate Flags:;
	 CBIT 11 1 0 d4 ........ ......1. ........ ........ Target Type is an NT Server;
	 CLSE  0 0 0 d4 ........ ......0. ........ ........ Target Type is Not an NT Server;
	 CBIT 10 1 0 d4 ........ .......1 ........ ........ Target Type is a Domain Controller;
	 CLSE  0 0 0 d4 ........ .......0 ........ ........ Target Type is Not a Domain Controller;
	 CBIT  2 1 0 d4 ........ ........ ........ .....1.. Server Return its Auth Realm;
	 CLSE  0 0 0 d4 ........ ........ ........ .....0.. Server Not to Return its Auth Realm;
	 CBIT  0 1 0 d4 ........ ........ ........ .......1 Text Strings Are in Unicode;
	 CLSE  0 0 0 d4 ........ ........ ........ .......0 Text Strings Are Not in Unicode;
	LABL 0 0 0 c8;
	g[3] += 4;
	HEX# 8 0 90 c2 NTLM Challenge:;
	g[3] += 8;
	HEX# 8 0 90 c2 Reserved:;
	g[3] += 8;
	LABL 0 0 0 c9 Address List:;
	 g[4] = 1;
	 DWRD 40000000 41 90 c2 Length:;
	 g[2] += l[1];
	 DWRD 40000000 42 90 c2 Max Length:;
	 DLNG 40000000 43 90 c2 Buffer:;
	 g[3] += 8;
	 if (l[1] > 0)
	 {
		l[3] -= g[3];
		INCR 0 43;
		l[3] += l[1];
		
		while (l[1] > 0)
		{
			g[1] = pb[0];		// type field
			CST# 0 1 90 c2 SMB::NTLMSSP::Challenge::List;
			INCR 2;
			
			g[1] = pb[0];
			l[1] -= g[1];		// length field
			INCR 2;
			l[1] -= 4;			// for the type and length field
			
			CHR# 0 1 90 2;
		}
		DECR 0 43;
	 }
	 LABL 0 0 0 c8;
	
	INCR 0 2;									//si: we've already used this... so incr past it
	LABL 0 0 0 b8;


	l[1] = pb[0];
	if (l[1] == 0xa3) 
	{
		LABL	0	0	0	b7 mechListMIC;
		ASN1_SkipTag;*a3 ll
		HBYT 0 1 90 c2 Identifier:;
		HBYT 0 1 90 c2 Type:;
		DBYT 0 1 90 c2 Length:;
		TSUB -SMB::NTLMSSP_Check;
		LABL	0	0	0	b8 ;
	}

	TRTS;

