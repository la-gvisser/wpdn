void SMB_Browse-Domain_Announcement;
	DBYT 40000000 1 90 c2 Update Count:\;
	DLNG 40000000 1 90 c2 Update Frequency:\;
	LABL		0 0  0 14 milliseconds;
	CHR#	   10 0 90 c2 Domain Name:;			*si: not sure how long this is supposed to be
	DBYT		0 1 90 c2 OS Major Version:\;
	DBYT		0 1 90 c2 OS Minor Version:\;
	SMB_Browse-Server_Flags;
	DBYT		0 1 90 c2 Brwsr Major Vers:\;
	DBYT		0 1 90 c2 Brwsr Minor Vers:\;
	HWRD		0 1 90 c2 Padding:\;
	GSTR		0 1;
	CHR#	    0 1 90 c2 Mstr Brwsr Srvr Name:;
	GBYT		0 1;
	if (g[1] == 0)					*si: get rid of the two zero bytes
	{
	  INCR 1;
	  TGTE 0 10 0 0 RETURN;
	  GBYT 0 1;
	  	if (g[1] == 0) 
		{
		 INCR 1;
		} 
	}
	TRTS;	

void SMB_Browse-Local_Master_Announcement;
	DBYT 0 1 90 c2 Update Count:\;
	DLNG 0 1 90 c2 Update Periodicity:\;
	 LABL 0 0 0 14 milliseconds;
	CHR# 10 0 90 c2 Server Name:\Server:;
	DBYT 0 1 90 c2 Major Vers:\;
	DBYT 0 1 90 c2 Minor Vers:\;
	SMB_Browse-Server_Flags;
	DBYT 0 1 90 c2 Brwsr Prtcl Maj Vers:\;
	DBYT 0 1 90 c2 Brwsr Prtcl Min Vers:\;
	HWRD 0 1 90 c2 Signature:\;
	GSTR 0 1;
	CHR# 0 1 90 c2 Comment:\Cmt:;
	INCR 1;				*SI: get rid of last zero
	TRTS;


void SMB Transact Rsp;
	TEQU 0 1 0 0 SMB CloseF Rsp;
	DWRD 40000000 4 90 c2 Total Param Bytes:\TPB:;
	DWRD 40000000 4 90 c2 Total Data Bytes:\TDB:;
	HWRD 40000000 4 90 c2 Reserved:;
	DWRD 40000000 8 90 c2 Params This Buffer:\PTB:;  *SI: SAVE FOR PADDING in g8
	DWRD 40000000 3 90 c2 Params Bytes Offset:\PBO:;	*si: save FOR PADDING in g3
	DWRD 40000000 4 90 c2 Params Displacement:\PDis:;
	DWRD 40000000 4 90 c2 Data This Buffer:\DTB:;	*SI: SAVE FOR PADDING in g9
	DWRD 40000000 9 90 c2 Data Bytes Offset:\DBO:;
	DWRD 40000000 4 90 c2 Data Displacement:\DDis:;
	DBYT 0 4 90 c2 Setup Word Count:\SWC:;
	HBYT 0 0 90 c2 Reserved:;
	SEQU 0 4;
	SKIP 3;
	SUBG 4 81;
	MULG 2 4;
	GBYT 0 5;  * save setup[0] in g5
	INCR 1;
	GWRD 40000000 6;   * setup[1] in g6;
	DECR 1;
	DUMP 0 4 90 c2 Additional Setup Bytes:\ASB:;
	MOVE a b;
	DWRD 40000000 1 90 c2 Byte Count:\Bytes:;

	SUBG 10 92;
	*DGLB 0 12 90 C2 THIS IS GLOBAL 18-rsp:;
	MOVE 12 87;

	*SI: this takes care of padding...I DESPISE PADDING
	*MOVE 37 7;					* move 55 [amt bytes since header] into g7
	ADDG 1 87;					* add the byte count to g7
	SUBG 3 87;					* g7 = g7 - params bytes offset
	MOVE 1 82;					* move byte count to g2
	SUBG 7 82;					* g2 = g2 - g7
	HEX# 0 2 90 c2 Padding:;	* first padding
	SUBG 3 89;					* sub g9 = g9 - g2 = data bytes offset - param bytes offset
	MOVE 9 88; 

	*MOVE 10 82;					* move bytes left into g2
	*SUBG 8 82;					* g2 = g2 - params this buffer
	*SUBG 9 82;					* g2 = g2 - data bytes this buffer
	*MOVE 2 88;	*SI: pass g8 to next function for padding

	WHOA  0 18  0  0 SMB_Func_Test;
	
	SUBG 2 81;					*si: subtract out the padding.
	
	MOVE 1 8c;
	SEQU 0 1;
	SKIP 1;
	DUMP 0 1 90 c2 Parameter And Data Bytes:;
	LABL 0 0 0 30;

	SLTE 0 10;
	SKIP 1;
	TRTS;


