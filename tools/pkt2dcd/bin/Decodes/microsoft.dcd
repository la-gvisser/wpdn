	 g[4] = 1;
	 SMB::NTLMSSP::Authenticate::Sec_Buffer;
	 LABL 0 0 0 c8;
	LABL 0 0 0 c9 Session Key; 
	 g[4] = 2;
	 SMB::NTLMSSP::Authenticate::Sec_Buffer;
	 LABL 0 0 0 c8;
	HLNG 40000000 1 90 c2 Negotiate Flags;
	LABL 0 0 0 b8;
	INCR 0 2;									//si: we've already used this... so incr past it
	TRTS;

void SMB::NTLMSSP::Authenticate::Sec_Buffer;
	DWRD 40000000 41 90 c2 Length:;
	g[2] += l[1];
	DWRD 40000000 42 90 c2 Max Length:;
	DLNG 40000000 43 90 c2 Buffer:;
	g[3] += 8;
	if (l[1] > 0)
	{
		l[3] -= g[3];
		INCR 0 43;
		if (g[4] == 1)
		{
			CHR# 0 41 90 c2 Value:;
		}
		if (g[4] == 2)
		{
			HEX# 0 41 90 c2 Value:;
		}
		l[3] += l[1];
		DECR 0 43;
	}
	TRTS;

