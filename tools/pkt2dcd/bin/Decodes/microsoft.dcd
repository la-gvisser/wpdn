void SMB2::QUERY_DIRECTORY( )
{
	LABL( 0, 0, 0, 0xb7, "SMB2 QUERY_DIRECTORY Request" );
		DWRD( 0x40000000, 0, 0x90, 0xc2, "StructureSize:" );
		
		DBYT( 0, g[53], 0x90, 0xc2, "FileInformationClass:" );
		//l[1] = g[53];
			
			l[1] = 0x00000100;				//set the file class
			CST#( 0, g[53], 0, 0x14, MSFSCC::FileInfoClass );
			g[53] += l[1];
			s[53] = g[53];
		Superglobaltest;
		DBYT( 0, l[2], 0x90, 0xc2, "Flags:" );
			CBIT( 0x01, l[2], 0,0xd4, ".... ...1 SMB2_RESTART_SCANS" );
			CBIT( 0x02, l[2], 0,0xd4, ".... ..1. SMB2_RETURN_SINGLE_ENTRY" );
			CBIT( 0x04, l[2], 0,0xd4, ".... .1.. SMB2_INDEX_SPECIFIED" );
			CBIT( 0x10, l[2], 0,0xd4, "...1 .... SMB2_REOPEN" );
		HLNG( 0x40000000, l[3], 0x90, 0xc2, "FileIndex:" );
		LABL( 0, 0, 0, 0xc9, "FiledID" );
			D64B( 0x40000000, l[5], 0x90, 0xc2, "Persistent:" );
			D64B( 0x40000000, l[6], 0x90, 0xc2, "Volatile:" );
		LABL( 0, 0, 0, 0xb8 );
		DWRD( 0x40000000, l[10], 0x90, 0xc2, "FileNameOffset:" );
		DWRD( 0x40000000, l[11], 0x90, 0xc2, "FileNameLength:" );
		DLNG( 0x40000000, g[21], 0x90, 0xc2, "OutputBufferLength:" );
		if ( g[53] == 0x37)
		{
			CHR#( 0x40000000, l[11], 0x90, 0xc2, "FileName:" );
			//MODG( 8 , l[11]);
			//DGLB( 0x40000000, l[11], 0x90, 0xc2, "l11:" );
			g[58] -= l[11];
			if (g[58] != 0)
			{
				DUMP( 0, g[58], 0x90,0xc2, "padding:" );
			}
		}
		if ( g[53] != 0x37)
		{
			CHR#( 0x40000000, l[11], 0x90, 0xc2, "Name:" );
			g[58] -= l[11];
			if (g[58] != 0)
			{
				DUMP( 0, g[58], 0x90,0xc2, "padding:" );
			}
		}
	LABL( 0, 0, 0, 0xb8 );
}

void SMB2::QUERY_DIRECTORY::Response( )
{
	LABL( 0, 0, 0, 0xb7, "SMB2 QUERY_DIRECTORY Response" );
		DWRD( 0x40000000, 0, 0x90, 0xc2, "StructureSize:" );
		DWRD( 0x40000000, l[1], 0x90, 0xc2, "OutputBufferOffset:" );
		DLNG( 0x40000000, l[20], 0x90, 0xc2, "OutputBufferLength:" );
		g[20] = l[20];
		DGLB( 0, s[53], 0x90, 0xc2, "FileInformationClass:" );
			CST#( 0, s[53], 0, 0x14, MSFSCC::FileInfoClass );
			TSB#( 0, s[53], 0, 0x14, MSFSCC::FileInfoClass::load);
	LABL( 0, 0, 0, 0xb8 );
	s[53] = 0;
}

void SMB2::CHANGE_NOTIFY( )
{
	LABL( 0, 0, 0, 0xb7, "SMB2 CHANGE_NOTIFY Request" );
		DWRD( 0x40000000, g[21], 0x90, 0xc2, "StructureSize:" );
		DWRD( 0x40000000, l[2], 0x90, 0xc2, "Flags:" );
