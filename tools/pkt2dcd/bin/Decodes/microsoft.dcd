	DWRD 0 1 90 c2 Data Size:;
	TRTS;

	TBIT 7 9 0 0 SMB NTTransact Rsp;

	TGTE 24 10 0 0 RETURN;
	INCR 24;				* 35dec is 23hex
	GBYT 0 3 90 c2;			* this is setup count
	INCR 1;
	ADDG 13 3;				* 19dec is 13hex
							* 3 now holds for case 1
	DECR 25;
	GBYT 0 4 90 c2;			* this is word count
	SEQU 3 84;
	TTST -SMB NTTransact1 Cmd;
