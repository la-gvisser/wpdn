* CIFS / SMB

* notes:
*  - mark arrays: 
*     - 18: holds which array to jump to
*	  - 19: transaction2 responses
*     - 20: pipe responses
*     - 21: msrap / pipe_lanman responses
//------------------------------------SMBv2------------------------------------
//		g[20] length if the Response Values
//		g[30] command
//		g[55] Length of the SMB2 payload
//		g[56] Length consumed begin of the SMB2 payload
//		g[57] Length consumed end of the SMB2 payload

//_____________________________________SMB2_____________________________________
void SMB File Share( )
{
	g[56] = g[16];
	GBYT( 0, l[1] );
	GLNG( 0x40000014, g[59] );
	//DGLB( 0, g[56], 0x90, 0xc2, "SMB2 Check" );
	//DGLB( 0, g[59], 0x90, 0xc2, "SMB2 Check59" );
	if ( l[1] == 0xfe )
	{
		SMB2;
		g[57] = g[16];
		//DGLB( 0, g[57], 0x90, 0xc2, "SMB2 Checkg57" );
	}	
	if ( l[1] == 0xff )
	{
		SMB File Share2;
	}	
	if ( g[59] != 0 )
	{
		g[56] -= g[57];
		//DGLB( 0, g[56], 0x90, 0xc2, "SMB2 Padding1" );
		//DGLB( 0, g[59], 0x90, 0xc2, "SMB2 Padding159" );
		g[59] -= g[56];
		//DGLB( 0, g[56], 0x90, 0xc2, "SMB2 Padding156" );
		//DGLB( 0, g[59], 0x90, 0xc2, "SMB2 Check59" );
		DUMP( 0, g[59], 0x90, 0xc2, "Padding1" );
		SMB File Share;
	}
	if ( g[59] == 0 )
	{
		if ( g[16] != 0 )
		{
			g[56] -= g[57];
			//DGLB( 0, g[56], 0x90, 0xc2, "SMB2 Check56" );
			//DGLB( 0, g[59], 0x90, 0xc2, "SMB2 Check59" );			
			g[59] -= g[56];
			//DGLB( 0, g[56], 0x90, 0xc2, "SMB2 Check56" );
			//DGLB( 0, g[59], 0x90, 0xc2, "SMB2 Check59" );
			DUMP( 0, g[59], 0x90, 0xc2, "Padding" );			
			TRTS;
		}
	}
}

