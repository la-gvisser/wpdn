void SMB2::IOCTL( )
{
	LABL( 0, 0, 0, 0xb7, "SMB2 IOCTL Request" );
		DWRD( 0x40000000, g[21], 0x90, 0xc2, "StructureSize:" );
		g[55] -=g[21];
		DWRD( 0x40000000, l[2], 0x90, 0xc2, "Reserved:" );
		HLNG( 0x40000000, l[3], 0x90, 0xc2, "CtlCode:" );
			CST#( 0, l[3], 0, 0x14, SMB2::IOCTL::FSCTLCode );
		LABL( 0, 0, 0, 0xc9, "FileID" );
			D64B( 0x40000000, l[5], 0x90, 0xc2, "Persistent:" );
			D64B( 0x40000000, l[6], 0x90, 0xc2, "Volatile:" );
			LABL( 0, 0, 0, 0xb8 );	
		HLNG( 0x40000000, l[7], 0x90, 0xc2, "InputOffset:" );
		HLNG( 0x40000000, l[8], 0x90, 0xc2, "InputCount:" );
		HLNG( 0x40000000, l[12], 0x90, 0xc2, "MaxInputResponse:" );
		HLNG( 0x40000000, l[10], 0x90, 0xc2, "OutputOffset:" );
		HLNG( 0x40000000, l[11], 0x90, 0xc2, "OutputCount:" );
		HLNG( 0x40000000, l[13], 0x90, 0xc2, "MaxOutputResponse:" );

		HLNG( 0x40000000, l[9], 0x90, 0xc2, "Flags:" );
			CEQU( 0x00000000, l[9], 0, 0xd4, "The request is an IOCTL request." );
			CEQU( 0x00000001, l[9], 0, 0xd4, "SMB2_0_IOCTL_IS_FSCTL (the request is an FSCTL request)" );
		HLNG( 0x40000000, l[14], 0x90, 0xc2, "Reserved2:" );
		//CHR#( 0, g[55], 0x90, 0xc2, "Buffer:" );
		HGLB( 0, l[3], 0x90, 0xc2, "FSCTLCode:" );

		if (l[3] == 0x0011C017)
		{
			LABL( 0, 0, 0, 0xb8 );

			LABL  0  0  0 b1 DCE RPC\DCE:;
			DBYT  0  2 90 c2 "Major Version:\Ver:";
			TEQU  4  2  0 14 DCE::Protocol V4;
			TEQU  5  2  0 14 DCE::Protocol V5;
		}
		if (l[3] == 0x00060194)
		{
						LABL( 0, 0, 0, 0xb8 );

			LABL  0  0  0 b1 Get DFS Referral Request:;
			
			g[55] -= 4;
			DWRD( 0x40000000, l[2], 0x90, 0xc2, "MaxReferralLevel:" );
			CHR#( 0x40000000, g[55], 0x90, 0xc2, "Buffer:" );

		}
		if (l[3] != 0x0011C017)
		{
		if (l[3] != 0x00060194)
		{
			
			CHR#( 0, g[55], 0x90, 0xc2, "Buffer:" );
		}
		}
	LABL( 0, 0, 0, 0xb8 );
}

