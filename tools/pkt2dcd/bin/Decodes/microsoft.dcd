				l[1] -= l[23];
				DUMP( 0x40000000, l[1], 0x90, 0xc2, "Padding:" );
				g[20] -= l[1];
			}
			LABL( 0, 0, 0, 0xb8 );
			
			if (l[1] == 0)
			{
				
				g[20] = 0;
			}
		}
