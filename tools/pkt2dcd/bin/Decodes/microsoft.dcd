void SMB_String_Name_Better;			*SI: that's right...this is better
	TGTE 0  1 0 0 RETURN;				*si: fyi, 00 00 signifies that it's the end of the string... you should add a check for that
	TGTE 0 10 0 0 RETURN;
	
	/*
	GLNG 0 4;							*SI: this is the check to see if we've hit the end
	SNEQ 4 4;
	SKIP 3;
	 SUBG 4 1;
	 INCR 4;
	 TRTS;
	*/
	
	// si: some strings are null terminated
	l[1] = pw[0];
	if (l[1] == 0)
	{
		INCR 2;
		TRTS;
	}
	
	g[4] = pb[0];
	if (g[4] == 0)
	{
		INCR 1;
		SUBG 1 1;
		TNEQ 0 1 0 0 SMB_String_Name_Better;
		TRTS;
	}
	if (g[4] != 0 )
	{
		CHR# 1 0 90;						//si: this prolly isn't the best idea
		SUBG 1 1;
		TNEQ 0 1 0 0 SMB_String_Name_Better;
		TRTS;
	}
	TRTS;

