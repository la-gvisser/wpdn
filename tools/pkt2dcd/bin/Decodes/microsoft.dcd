void SMB_MailSlot_NetLogon;
	LABL  0  0  0 b1 SMB MailSlot Protocol\MailSlot:;
	DECR  8;		* the test browse sees if it's this by incr 8...so we must get the bytes back
	
	DWRD 40000000 1 90 c2 OpCode:\Type:;
	 CEQU 1 1 0 14 Write Mail Slot;
	DWRD 40000000 1 90 c2 Priority:;
	DWRD 40000000 1 90 c2 Class:;
	DWRD 40000000 1 90 c2 Size:;
	MOVE 10 4;		*SI: the mailslot name is always 16 chars long
	SBIT  f 11;
	SKIP  2;
	CSTR  0  4 90 c2 MailSlot name:\;
	SKIP 80000001;
	CSTR 40000000 4 90 c2 MailSlot name:\;
	DBYT  0  1 90 c2 Browser Command:\Cmd:;
	CST# FFFFFFFF 1 0 14 SMB_MailSlot_NetLogon_Str;
	TST# 0 1 0 14 SMB_MailSlot_NetLogon_Jump;
	SGTE 0 10;
	 SKIP 1;
	 DUMP 0 10 90 c2 Unknown Mailslot Command:;

str# SMB_MailSlot_NetLogon_Str;
	LM1.0/LM2.0 LOGON Request;						* 0x00
	LM1.0 Response to LOGON Request;				* 0x01
	LM1.0 Query for Centralized Initialization;		* 0x02
	LM1.0 Query for Distributed Initialization;		* 0x03
	LM1.0 Response to Centralized Query;			* 0x04
	LM1.0 Response to Distributed Query;			* 0x05
	LM2.0 Response to LOGON Request;				* 0x06
	Query for Primary DC;							* 0x07
	Announce Startup of Primary DC;					* 0x08
	Announce Failed Primary DC;						* 0x09
	Announce Change to UAS or SAM;					* 0x0a
	Announce no user on machine;					* 0x0b
	Response from Primary DC;						* 0x0c
	LM1.0/LM2.0 Response to Relogon Request;		* 0x0d
	LM1.0/LM2.0 Response to Interrogate Request;	* 0x0e
	LM2.0 Response During NETLOGON pause;			* 0x0f
	LM2.0 Response when user is unknown;			* 0x10
	LM2.1 Announce account updates;					* 0x11
	SAM LOGON Request from client;					* 0x12
	SAM Response to SAM LOGON Request;				* 0x13
	SAM Response during NETLOGON pause;				* 0x14
	SAM Response when user is unknown;				* 0x15
	SAM Response to Interrogate Request;			* 0x16

str# SMB_MailSlot_NetLogon_Jump;
	0x07 | SMB_MailSlot_NetLogon_QueryForPrimaryDC;	* 0x07
	0x12 | SMB_MS_Logon;							* 0x12
	0x13 | SMB_MailSlot_NetLogon_ResponseLogon;		* 0x13
	0x15 | SMB_MailSlot_NetLogon_UserUnknown;		* 0x15


