void SMB Status Code;
	INCR 5;
	GWRD 40000000 9;
	DECR 5;
	SBIT e 9;
	SKIP 3;
	SMB DOS Error Code;
	SBIT e 9;
	SKIP 80000001;
	SMB NT Error Code;
	TRTS;

void SMB NT Error Code;
	HLNG 40000000 9 90 c2 NT Status:;

	* first test 00000000 < g9 < 00000003
	SGTE FFFFFFFFFFFFFFFF 9;
	SKIP 5;
	SLTE 00000004 9;
	SKIP 3;
	 ADDG 1 9;
	 CST# 0 9 0 14 SMB_NT_Error_Code1;
	 TRTS;

	* second set
	SGTE 000000FF 9;
	SKIP 5;
	 SLTE 00000119 9;
	 SKIP 3;
	  ADDG 1 9;				*SI: starts at 0
	  CST# 0 9 0 14 SMB_NT_Error_Code2;
	  TRTS;

	* third set: 40000000 < G9 < 40000025 
	SGTE 3FFFFFFF 9;
	SKIP 8;
	 SLTE 40000026 9;
	 SKIP 6;
	  DECR 4;
	  GWRD 40000000 9;
	  INCR 4;
	  ADDG 1 9;				*SI: starts at 0
	  CST# 0 9 0 14 SMB_NT_Error_Code3;
	  TRTS;

	* fourth set: 80000001 < G9 <80000026 
	SGTE 80000000 9;
	SKIP 7;
	 SLTE 80000027 9;
	 SKIP 5;
	  DECR 4;
	  GWRD 40000000 9;
	  INCR 4;
	  CST# 0 9 0 14 SMB_NT_Error_Code4;
	  TRTS;

	* fifth set: C0000001 < G9 < C00002E8
	SGTE C0000000 9;
	SKIP 7;
	 SLTE C00002E9 9;
	 SKIP 5;
	  DECR 4;
	  GWRD 40000000 9;
	  INCR 4;
	  CST# 0 9 0 14 SMB_NT_Error_Code5;
	  TRTS;

	* last set: bunch of loners...
	CEQU 0000003F 9 0 14 STATUS_WAIT_63;
	CEQU 00000080 9 0 14 STATUS_ABANDONED;
	CEQU 00000080 9 0 14 STATUS_ABANDONED_WAIT_0;
	CEQU 000000BF 9 0 14 STATUS_ABANDONED_WAIT_63;
	CEQU 000000C0 9 0 14 STATUS_USER_APC;

	*SI: there's actually more...but they are really irregular...so i won't put htem in
	* i got these here: http://www.jache.com/stories/storyReader$181
	* the str#'s are found at the bottom of microsoft.dcd
	TRTS;

