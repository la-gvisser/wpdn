void SMB SessSetup Rsp;
	TEQU 2 10 0 0 SMB_SessSetup_Rsp2;
	HBYT 40000000 2 90 c2 Secondary command:;
	SMB Command Codes;
	HBYT 40000000 4 90 c2 Reserved:;
	DWRD 40000000 d 90 c2 AndX offset:;
	HWRD 40000000 4 90 c2 Action:;
	CBIT 0 4 0 14 logged in as GUEST;
	SBIT b 11;								* test extended security
	SKIP 80000001;							* is off
	DWRD 40000000 4 90 c2 Security Length:;
	DWRD 40000000 1 90 c2 Byte Count:\Bytes:;
	l[0x11] = g[0x11];
	if (g[4] > 0)
	{
		SBIT b 11;								* test extended security
		SKIP 80000004;     * is off						//------------------------------------------------------------
			SMB::NTLMSSP_Check;
			SEQU 64 14; 
			SKIP 1;
			SMB::SessSetup_Rsp::12::GSS;
			//INCR 21;
			//SMB::NTLMSSP_Check;
			//DUMP 0 4 90 c2 Security Blob:;
	}
	
	g[0x11] = l[0x11];
	SBIT f 11;         * test for flags fields for unicode support bit
	SKIP 3;	           * is on then skip
		CSTR 0 6 90 c2 Native OS:;
		CSTR 0 6 90 c2 Native Lan Man:;
		CSTR 0 6 90 c2 Primary Domain Name:;
	
	SBIT f 11;         * test for flags fields for unicode support bit
	SKIP 80000006;	   * is off then skip
		Pad Byte;
			*INCR 1;  * seems to be an undefined byte in 
		CSTR 40000000 6 90 c2 Native OS:;
		CSTR 40000000 6 90 c2 Native Lan Man:;
		SGTE 6 10;
		SKIP 1;
		 CSTR 40000000 6 90 c2 Primary Domain Name:;

	if (g[16] <= 0)
	{
		TRTS;
	}
	
	l[1] = g[0x12];
	l[1] -= g[16];
	l[2] = g[0xd];
	l[2] -= l[1];
 	if (l[2] > 0)
	{
		INCR 0 42;
	}
	
	TNXT -SMB Request/Response;

void SMB_SessSetup_Rsp2;
	DWRD 40000000 1 90 c2 Byte Count:\Bytes:;
	TRTS;

// -----------------------------------------------------------------
void SMB::SessSetup_Rsp::12::GSS;
	// si: this is GSS-API stuff... beware... it could be something diff... 
	ASN1_Init;
	ASN1_DisableEnum;

	ASN1_SkipTag;*A1 ll
	ASN1_SkipTag;*30 ll
	
	// taken from GSS::NegTokenTarg()
	l[1] = pb[0];
	if (l[1] == 0xa0)
	{
		LABL	0	0	0	b7	negResult;
		ASN1_SkipTag;*a0 ll
		ASN1_SkipTag;*0a ll
		DBYT	0	41	90	c2	Result:;
		CEQU	0	41	0	14	accept_completed;
		CEQU	1	41	0	14	accept_incomplete;
		CEQU	2	41	0	14	reject;
		LABL	0	0	0	b8 ;
	}

	l[1] = pb[0];
	if (l[1] == 0xa1) 
	{
		LABL	0	0	0	b7 supportedMech;
		ASN1_SkipTag;*a1 ll

		LABL	0	0	0	b7 Mechanism Type;
		ASN1_Value;
		LABL	0	0	0	b8 ;
		LABL	0	0	0	b8 ;
	}

	l[1] = pb[0];
	if (l[1] == 0xa2) 
	{
		LABL	0	0	0	b7 responseToken;
		ASN1_SkipTag;*a2 ll
		*DUMP	0	2	90	c2 responseToken Data:;

		ASN1_SkipTag;*04 ll
		//ASN1_SkipTag;*60 ll

		LABL	0	0	0	b7 Mechanism Type;
		SMB::NTLMSSP_Check;
		LABL	0	0	0	b8 ;
		
		LABL	0	0	0	b8 ;
	}
