void SMB2::LOCK( )
{
	LABL( 0, 0, 0, 0xb7, "SMB2 LOCK Request" );
		DWRD( 0x40000000, g[21], 0x90, 0xc2, "StructureSize:" );
		DWRD( 0x40000000, l[2], 0x90, 0xc2, "LockCount:" );
		HLNG( 0x40000000, l[3], 0x90, 0xc2, "LockSequence:" );
		LABL( 0, 0, 0, 0xc9, "FileID" );
			D64B( 0x40000000, l[5], 0x90, 0xc2, "Persistent:" );
			D64B( 0x40000000, l[6], 0x90, 0xc2, "Volatile:" );
			LABL( 0, 0, 0, 0xb8 );
		while (l[3] >= 1)
		{
			D64B( 0x40000000, l[7], 0x90, 0xc2, "Offset:" );
			D64B( 0x40000000, l[8], 0x90, 0xc2, "Length:" );
			HLNG( 0x40000000, l[9], 0x90, 0xc2, "Flags:" );
				CEQU( 0x00000001, l[9], 0, 0xd4, "SMB2_LOCKFLAG_SHARED_LOCK" );
				CEQU( 0x00000002, l[9], 0, 0xd4, "SMB2_LOCKFLAG_EXCLUSIVE_LOCK" );
				CEQU( 0x00000004, l[9], 0, 0xd4, "SMB2_LOCKFLAG_UNLOCK" );
				CEQU( 0x00000010, l[9], 0, 0xd4, "SMB2_LOCKFLAG_FAIL_IMMEDIATELY" );
				CEQU( 0x00000011, l[9], 0, 0xd4, "SMB2_LOCKFLAG_SHARED_LOCK | SMB2_LOCKFLAG_FAIL_IMMEDIATELY" );
				CEQU( 0x00000012, l[9], 0, 0xd4, "SMB2_LOCKFLAG_EXCLUSIVE_LOCK | SMB2_LOCKFLAG_FAIL_IMMEDIATELY" );				
			HLNG( 0x40000000, l[10], 0x90, 0xc2, "Reserved:" );
			l[3] -= 1;
		}
	LABL( 0, 0, 0, 0xb8 );
}

