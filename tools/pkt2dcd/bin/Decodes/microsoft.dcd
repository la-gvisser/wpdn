void SMB2::QUERY_INFO( )
{
	LABL( 0, 0, 0, 0xb7, "SMB2 QUERY_INFO Request" );
		DWRD( 0x40000000, 0, 0x90, 0xc2, "StructureSize:" );
		DBYT( 0, l[1], 0x90, 0xc2, "InfoType:" );		
			CST#( 0, l[1], 0, 0x14, SMB2::InfoType );
		DBYT( 0, l[2], 0x90, 0xc2, "FileInfoClass:" );
			//Calculation
			SHFL( 8, l[1] );
			l[1] += l[2];
			CST#( 0, l[1], 0, 0x14, MSFSCC::FileInfoClass );
			//HGLB( 0, g[54], 0x90, 0xc2, "g54" );
			l[3] = g[54];			*MessageID
			SHFL( 16, l[3] );
			l[1] += l[3];
			//HGLB( 0, l[1], 0x90, 0xc2, "l1" );
			//eof Calculation of l[1]
			if (s[1] == 0)
			{
				s[1] = l[1];			*first loop
			}
			l[4] = s[1];
			SHFR( 16, l[4] );
			if (l[4] != g[54])
			{
				if (s[1] != 0)
				{
					if (s[2] == 0)
					{
						s[2] = l[1];			//second loop 
					}
					
				}
				l[5] = s[2];
				SHFR( 16, l[5] );
				if (l[5] != g[54])				
				{
					if (s[2] != 0)
					{
						if (s[3] == 0)
						{
							s[3] = l[1];			//third loop, sorry no way for testing up to 60 loops.
						}
						
					}
				}
			}
			
			
		//HGLB( 0,s[1], 0x90,0xc2, "s1" );
		//HGLB( 0,s[2], 0x90,0xc2, "s2" );
		DLNG( 0x40000000, l[3], 0x90, 0xc2, "OutputBufferLength:" );
		DWRD( 0x40000000, 0, 0x90, 0xc2, "InputBufferOffset:" );
		DWRD( 0x40000000, 0, 0x90, 0xc2, "Reserved:" );
		HLNG( 0x40000000, l[4], 0x90, 0xc2, "InputBufferLength:" );
		BLNG( 0x40000000, l[5], 0x90, 0xc9, "AdditionalInformation:" );
			CBIT( 0x01, l[5], 0, 0xd4, ".... .... .... ...1 OWNER_SECURITY_INFORMATION" );
			CBIT( 0x02, l[5], 0, 0xd4, ".... .... .... ..1. GROUP_SECURITY_INFORMATION" );
			CBIT( 0x04, l[5], 0, 0xd4, ".... .... .... .1.. DACL_SECURITY_INFORMATION" );
			CBIT( 0x08, l[5], 0, 0xd4, ".... .... .... 1... SACL_SECURITY_INFORMATION" );
			CBIT( 0x10, l[5], 0, 0xd4, ".... .... ...1 .... LABEL_SECURITY_INFORMATION" );
		LABL( 0, 0, 0, 0xb8 );
		BLNG( 0x40000000, l[6], 0x90, 0xc9, "Flags:" );
			CBIT( 0x01, l[6], 0, 0xd4, ".... .... .... ...1 SL_RESTART_SCAN" );
			CBIT( 0x02, l[6], 0, 0xd4, ".... .... .... ..1. SL_RETURN_SINGLE_ENTRY" );
			CBIT( 0x04, l[6], 0, 0xd4, ".... .... .... 1... SL_INDEX_SPECIFIED" );
		LABL( 0, 0, 0, 0xb8 );
		LABL( 0, 0, 0, 0xc9, "FiledID" );
			D64B( 0x40000000, l[7], 0x90, 0xc2, "Persistent:" );
			D64B( 0x40000000, l[8], 0x90, 0xc2, "Volatile:" );
		LABL( 0, 0, 0, 0xb8 );
		//if( l[4] == 0)	{	l[4] = 1;	}
		CHR#( 0, l[4], 0x90, 0xc2, "Buffer:" );
	LABL( 0, 0, 0, 0xb8 );
}

str# SMB2::InfoType;
0x01 | SMB2_0_INFO_FILE (The file information is requested);
0x02 | SMB2_0_INFO_FILESYSTEM (The underlying object store information is requested);
0x03 | SMB2_0_INFO_SECURITY (The security information is requested);
0x04 | SMB2_0_INFO_QUOTA (The underlying object store quota information is requested);

void SMB2::QUERY_INFO::Response( )
{
	LABL( 0, 0, 0, 0xb7, "SMB2 QUERY_INFO Response" );
		DWRD( 0x40000000, 0, 0x90, 0xc2, "StructureSize:" );
		DWRD( 0x40000000, 0, 0x90, 0xc2, "OutputBufferOffset:" );
		DLNG( 0x40000000, l[1], 0x90, 0xc2, "OutputBufferLength:" );
		g[20] = l[1];
		HGLB( 0,s[1], 0x90,0xc2, "s1" );
		HGLB( 0,s[2], 0x90,0xc2, "s2" );
		if(s[1] != 0)
		{
			l[1] = s[1];
			SHFR( 16, l[1] );
			HGLB( 0,l[1], 0x90,0xc2, "l1" );
			if (l[1] == g[54])
			{
				l[2] = g[54];
				//HGLB( 0,l[2], 0x90,0xc2, "l	2" );
				SHFL( 16, l[2] );
				//HGLB( 0,l[2], 0x90,0xc2, "l	2" );
				SUBG( l[2], s[1] );
				//HGLB( 0,s[1], 0x90,0xc2, "s1" );
				DGLB( 0, s[1], 0x90, 0xc2, "FileInformationClass:" );
					CST#( 0, s[1], 0, 0x14, MSFSCC::FileInfoClass );
					CST#( 0, s[1], 0, 0xb7, MSFSCC::FileInfoClass );
					TSB#( 0, s[1], 0, 0, MSFSCC::FileInfoClass::load );
					TLSE( 0, 0, 0, 0, SMB2::DUMPG20 );
					
					s[1] = 0;
				SKIP( 0x1B , 0, 0, 0 );
			}
		}
		if(s[2] != 0)
		{
			l[1] = s[2];
			SHFR( 16, l[1] );
			//HGLB( 0,l[1], 0x90,0xc2, "l1" );
			if (l[1] == g[54])
			{
				l[2] = g[54];
				//HGLB( 0,l[2], 0x90,0xc2, "l	2" );
				SHFL( 16, l[2] );
				//HGLB( 0,l[2], 0x90,0xc2, "l	2" );
				//HGLB( 0,s[2], 0x90,0xc2, "s2" );
				SUBG( l[2], s[2] );
				//HGLB( 0,s[2], 0x90,0xc2, "s2" );
				DGLB( 0, s[2], 0x90, 0xc2, "FileInformationClass:" );
					CST#( 0, s[2], 0, 0x14, MSFSCC::FileInfoClass );
					CST#( 0, s[1], 0, 0xb7, MSFSCC::FileInfoClass );
					TSB#( 0, s[1], 0, 0, MSFSCC::FileInfoClass::load );
					TLSE( 0, 0, 0, 0, SMB2::DUMPG20 );
					s[2] = 0;
				SKIP( 0xB , 0, 0, 0 );
			}
		}
		

	LABL( 0, 0, 0, 0xb8 );
}

void SMB2::SET_INFO( )
{
	LABL( 0, 0, 0, 0xb7, "SMB2 SET_INFO Request" );
		DWRD( 0x40000000, 0, 0x90, 0xc2, "StructureSize:" );
		DBYT( 0, l[1], 0x90, 0xc2, "InfoType:" );
			CST#( 0, l[1], 0, 0x14, SMB2::InfoType );
		DBYT( 0, l[2], 0x90, 0xc2, "FileInfoClass:" );
		SHFL( 8, l[1] );
			//l[1] = 0x00000100;				//set the file class
			l[2] += l[1];
			
			CST#( 0, l[2], 0, 0x14, MSFSCC::FileInfoClass );
			
		HLNG( 0x40000000, g[20], 0x90, 0xc2, "BufferLength:" );
		DWRD( 0x40000000, 0, 0x90, 0xc2, "BufferOffset:" );
		DWRD( 0x40000000, 0, 0x90, 0xc2, "Reserved:" );
		HLNG( 0x40000000, l[3], 0x90, 0xc2, "AdditionalInformation:" );
			CBIT( 0x01, l[3], 0, 0xd4, ".... .... .... ...1 OWNER_SECURITY_INFORMATION" );
			CBIT( 0x02, l[3], 0, 0xd4, ".... .... .... ..1. GROUP_SECURITY_INFORMATION" );
			CBIT( 0x04, l[3], 0, 0xd4, ".... .... .... .1.. DACL_SECURITY_INFORMATION" );
			CBIT( 0x08, l[3], 0, 0xd4, ".... .... .... 1... SACL_SECURITY_INFORMATION" );
			CBIT( 0x10, l[3], 0, 0xd4, ".... .... ...1 .... LABEL_SECURITY_INFORMATION" );
		LABL( 0, 0, 0, 0xc9, "FileID" );
			D64B( 0x40000000, l[5], 0x90, 0xc2, "Persistent:" );
			D64B( 0x40000000, l[6], 0x90, 0xc2, "Volatile:" );
		LABL( 0, 0, 0, 0xb8 );
		CST#( 0, l[2], 0, 0xc9, MSFSCC::FileInfoClass );
			TSB#( 0, l[2], 0, 0, MSFSCC::FileInfoClass::load );
			TLSE( 0, 0, 0, 0, SMB2::DUMPG20 );
		LABL( 0, 0, 0, 0xb8 );
	LABL( 0, 0, 0, 0xb8 );
	LABL( 0, 0, 0, 0xb8 );
}

