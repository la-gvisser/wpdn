void Trans2 Open2 Response;
	LABL 0  0 90 c2 Trans2 Open2 Response;


void Trans2 Find First2 Response;
	LABL         0  0  0 b1 Trans2 Find First2 Response;
	DWRD  40000000  1 90 c2 Search handle:;
	DWRD  40000000  1 90 c2 Search count:;
	DWRD  40000000  1 90 c2 End of search?:;
	CEQU		 0  1  0 14 No;
	CEQU		 1  1  0 14 Yes;
	DWRD  40000000  1 90 c2 EA list offset:;
	DWRD  40000000  1 90 c2 Filename offset:;
	SUBG		 A 8;
	HEX#		 0  8 90 c2 Padding:;
	*HWRD  40000000  1 90 c2 Padding:; *SI: forced padding for findfirst2
	TNXT -SMB_Trans2_File_Entry; 

void Trans2 Find First2 Response Data Standard;
	TEQU  0 10  0  0 RETURN;
	LABL  0  0  0 b1 File Entry;
	DLNG  40000000  1 90 c2 Next entry offset:;	
	DLNG  40000000  1 90 c2 File index:;	
	TIME  40000000  1 90 c2 Creation time:;	
	TIME  40000000  1 90 c2 Last access time:;	
	TIME  40000000  1 90 c2 Last write time:;	
	TIME  40000000  1 90 c2 Last change time:;	
	HEX#  8  1 90 c2 EOF offset:;	
	HEX#  8  1 90 c2 Allocation size:;	
	DLNG  40000000  1 90 c2 File attributes:;	

	CBIT  6  1  0 d4 ........ ....... ........ 1....... Normal;
	CLSE  0  0  0 d4 ........ ....... ........ 0....... Not Normal;
	DLNG  40000000  1 90 c2 Filename length:;
	DLNG  40000000  1 90 c2 Size of files' EA:;

void SMB_Trans2_File_Entry;  *SI: used to be: Trans2_Find_First2_Response_Data_Query_EA_Size;
	TGTE 0 10 0 0 RETURN;
	LABL  0  0  0 b1 File Entry;
	MOVE  10 85;		* the file offset tells you how far you gotta go through pkt!
	DLNG  40000000  6 90 c2 Next entry offset:;	
	DLNG  40000000  1 90 c2 File index:;	
	*HLNG 40000000 0 90 C2 BALH:;
	HEX#  8  0 90 c2 Creation time:;	
	HEX#  8  0 90 c2 Last access time:;	
	HEX#  8  0 90 c2 Last write time:;	
	HEX#  8  0 90 c2 Last change time:;	
	D64B  40000000 1 90 c2 EOF offset:;
	D64B  40000000  0 90 c2 Allocation size:;	
	HLNG  40000000  1 90 c9 File attributes:;
	 CBIT 1E 1 0 d4 1....... ........ ........ ........ Write Through Required;
	 CLSE  0 1 0 d4 0....... ........ ........ ........ Write Through Not Required;
	 CBIT 1D 1 0 d4 ..1..... ........ ........ ........ No Buffering;
	 CLSE  0 1 0 d4 ..0..... ........ ........ ........ Buffering Allowed;
	 CBIT 1C 1 0 d4 ...1.... ........ ........ ........ Random Access Requested;
	 CLSE  0 1 0 d4 ...0.... ........ ........ ........ Random Access Not Requested;
	 CBIT 1B 1 0 d4 ....1... ........ ........ ........ Sequential Scan Optimization;
	 CLSE  0 1 0 d4 ....0... ........ ........ ........ Not Optimized for Sequential Scan;
	 CBIT 1A 1 0 d4 .....1.. ........ ........ ........ Deleted on Close;
	 CLSE  0 1 0 d4 .....0.. ........ ........ ........ Not Deleted on Close;
	 CBIT 19 1 0 d4 ......1. ........ ........ ........ Support for Backup;
	 CLSE  0 1 0 d4 ......0. ........ ........ ........ No Support for Backup;
	 CBIT 18 1 0 d4 .......1 ........ ........ ........ Support for Posix;
	 CLSE  0 1 0 d4 .......0 ........ ........ ........ No Support for Posix;
	 CBIT  E 1 0 d4 ........ ........ .1...... ........ Encrypted;
	 CLSE  0 1 0 d4 ........ ........ .0...... ........ Not Encrypted;
	 CBIT  D 1 0 d4 ........ ........ ..1..... ........ Can not be indexed ;
	 CLSE  0 1 0 d4 ........ ........ ..0..... ........ May be indexed;
	 CBIT  C 1 0 d4 ........ ........ ...1.... ........ Offline;
	 CLSE  0 1 0 d4 ........ ........ ...0.... ........ Not Offline;
	 CBIT  B 1 0 d4 ........ ........ ....1... ........ Compressed;
	 CLSE  0 1 0 d4 ........ ........ ....0... ........ Not Compressed;
	 CBIT  A 1 0 d4 ........ ........ .....1.. ........ Reparse Point;
	 CLSE  0 1 0 d4 ........ ........ .....0.. ........ No Reparse Point;
	 CBIT  9 1 0 d4 ........ ........ ......1. ........ Sparse;
	 CLSE  0 1 0 d4 ........ ........ ......0. ........ Not Sparse;
	 CBIT  8 1 0 d4 ........ ........ .......1 ........ Temporary;
	 CLSE  0 1 0 d4 ........ ........ .......0 ........ Not Temporary;
	 CBIT  7 1 0 d4 ........ ........ ........ 1....... Normal;
	 CLSE  0 1 0 d4 ........ ........ ........ 0....... Not Normal;
	 CBIT  6 1 0 d4 ........ ........ ........ .1...... Device;
	 CLSE  0 1 0 d4 ........ ........ ........ .0...... Not a device;
	 CBIT  5 1 0 d4 ........ ........ ........ ..1..... Archive;
	 CLSE  0 1 0 d4 ........ ........ ........ ..0..... Not an archive;
	 CBIT  4 1 0 d4 ........ ........ ........ ...1.... Directory;
	 CLSE  0 1 0 d4 ........ ........ ........ ...0.... Not a directory;
	 CBIT  3 1 0 d4 ........ ........ ........ ....1... Volume ID;
	 CLSE  0 1 0 d4 ........ ........ ........ ....0... Not a Volume ID;
	 CBIT  2 1 0 d4 ........ ........ ........ .....1.. System file;
	 CLSE  0 1 0 d4 ........ ........ ........ .....0.. Not a System file;
	 CBIT  1 1 0 d4 ........ ........ ........ ......1. Hidden file;
	 CLSE  0 1 0 d4 ........ ........ ........ ......0. Not a Hidden file;
	 CBIT  0 1 0 d4 ........ ........ ........ .......1 Read Only;
	 CLSE  0 1 0 d4 ........ ........ ........ .......0 Not Read Only;
	 LABL 0 0 0 b8;
	DLNG  40000000  4 90 c2 Filename length:;
	DLNG  40000000  1 90 c2 Size of files' EA:;
	DBYT  40000000  3 90 c2 Short(8.3) name size:;
	DBYT  40000000  1 90 c2 Reserved(MBZ):;

	MOVE 18 3;	*SI: is the short name always 18? i think so
	SBIT f 11;
	SKIP 2;
	CHR# 0 3 90 c2 Short(8.3) name:;	*global used to be 4 ... but short name is always 18
	SKIP 80000001;
	CHR# 40000000 3 90 c2 Short(8.3) name:;	*global used to be 4

	SBIT f 11;
	SKIP 2;
	CHR# 0 4 90 c2 Long filename:;
	SKIP 80000001;
	CHR# 40000000 4 90 c2 Long filename:;

	SUBG 10 85;
	SNEQ 0 6 0 0; * RETURN;
	SKIP 3;
	SEQU 0 10;
	SKIP 1;
	DUMP 0 10 90 b1 Unknown Data:;

	SGTE 10 86;
	SKIP 1;
	TRTS; * abort if g6 is zero or negative
	
	SGTE 5 86;		*SI: checks to see if offset is finished
	SKIP 4;
	SEQU 5 86;
	SKIP 2;
	SUBG 5 86;
	INCR 0 6;		*SI: if not, incr by what's in value 6

	TGTE 0 10 0 0 RETURN;	*if you hit the end of the file, you exit
	TNXT  -SMB_Trans2_File_Entry; 


