void SMB2( )
{
	LABL( 0, 0, 0, 0xb1 , SMB2 - Server Message Block version 2\SMB2:);
	//Superglobaltest;
	//Analyse SMB2 Structure
	INCR(16);
	BBIT( 4, 0, 0x05, 0x05, Consume);
	BBIT( 1, l[51], 0x05, 0x05, SigningBIT);
	BBIT( 1, l[52], 0x05, 0x05, ChainedBIT);
	BBIT( 1, l[53], 0x05, 0x05, AsyncBIT);
	BBIT( 1, l[54], 0x05,0x05, ResponseBIT);
	BBIT( 19, 0, 0x05,0x05, Consume);
	BBIT( 1, l[50], 0x05,0x05, DFSBIT);
	BBIT( 4, 0, 0x05,0x05, Consume);
	DECR(20);
	LABL( 0, 0, 0, 0xb7 , SMB2 Header );
	CHR#( 4, 0, 0x90, 0xc2, "ProtocolId:" );
	DWRD( 0x40000000, l[2], 0x90, 0xc2, "StructureSize:" );
	//DGLB( 0, g[55], 0x90, 0xc2, "StructureSize:" );
	DWRD( 0x40000000, l[3], 0x90, 0xc2, "CreditCharge:" );
	//NTSTATUS
	HWRD( 0, 0, 0x90, 0xc2, "Status:" );
	DECR 2;
	HLNG( 0x40000000, l[4], 0x90, 0xc9, "Status:" );
		//Analyse again!
	DECR 4 ;
		
	//HWRD( 0, 0, 0x90, 0xc2, "Status:" );
	//DECR 2;
		BWRD( 0, l[5], 0x05, 0x05, "consume:" );
		BBYT( 0, l[45], 0x05, 0x05, "consume:" );
		BBIT( 2, l[44], 0x05, 0x05, "consume:" );
		BBIT( 1, l[7], 0x05, 0x05, "consume:" );
		BBIT( 1, l[8], 0x05, 0x05, "consume:" );
		BBIT( 4, l[9], 0x05, 0x05, "consume:" );
		l[49] = l[9]; 
		SHFL( 8, l[49] );
		l[49] += l[45];
		DECR 4;
		BWRD( 0x40000000, l[5], 0x05, 0x05, "Code:" );
		if (l[44] == 0x0000)
		{
			CST#( 0, l[5], 0x90, 0x14, SMBNTStatusSystemSuccessCodeTable );
			CLSE( 0, l[5], 0x90, 0x14, "Unhandled Success code" );
		}
		if (l[44] == 0x0001)
		{
			CST#( 0, l[5], 0x90, 0x14, SMBNTStatusSystemInfoCodeTable );
			CLSE( 0, l[5], 0x90, 0x14, "Unhandled Info code" );
		}
		if (l[44] == 0x0002)
		{
			CST#( 0, l[5], 0x90, 0x14, SMBNTStatusSystemWarningCodeTable );
			CLSE( 0, l[5], 0x90, 0x14, "Unhandled Warning code" );
		}
		if (l[44] == 0x0003)
		{
			CST#( 0, l[5], 0x90, 0x14, SMBNTStatusSystemErrorCodeTable );
			CLSE( 0, l[5], 0x90, 0x14, "Unhandled Error code" );
		}
		if (l[44] != 0x0000)
		{
			if (l[44] != 0x0001)
			{
				if (l[44] != 0x0002)
				{
					if (l[44] != 0x0003)
					{
						CST#( 0, l[5], 0x90, 0x14, SMB2::Header::Status::Code );
					}
				}
			}
		}
		INCR 2;
		//-----------------	
		BGLB( 0x40000000, l[4], 0x94, 0xc2, "Status:" );
	//Analyse again!
		//DECR( 4 );		
		//extended dropdownInfos
		//BWRD( 0x40000000, l[5], 0x94, 0xc2, "Code:" );
			if (l[44] == 0x0000)
			{
				CST#( 0, l[5], 0x90, 0x14, SMBNTStatusSystemSuccessCodeTable );
				CLSE( 0, l[5], 0x90, 0x14, "Unhandled Success code" );
			}
			if (l[44] == 0x0001)
			{
				CST#( 0, l[5], 0x90, 0x14, SMBNTStatusSystemInfoCodeTable );
				CLSE( 0, l[5], 0x90, 0x14, "Unhandled Info code" );
			}
			if (l[44] == 0x0002)
			{
				CST#( 0, l[5], 0x90, 0x14, SMBNTStatusSystemWarningCodeTable );
				CLSE( 0, l[5], 0x90, 0x14, "Unhandled Warning code" );
			}
			if (l[44] == 0x0003)
			{
				CST#( 0, l[5], 0x90, 0x14, SMBNTStatusSystemErrorCodeTable );
				CLSE( 0, l[5], 0x90, 0x14, "Unhandled Error code" );
			}
			if (l[44] != 0x0000)
			{
				if (l[44] != 0x0001)
				{
					if (l[44] != 0x0002)
					{
						if (l[44] != 0x0003)
						{
							CST#( 0, l[5], 0x90, 0x14, SMB2::Header::Status::Code );
						}
					}
				}
				
			}
			g[19] = l[5];
		//	DGLB( 0, l[49], 0x90, 0xc2, "l49:" );
		BGLB( 0x40000000, l[49], 0x94, 0xc2, "Facility:" );
			CST#( 0, l[49], 0x90, 0x14, SMB2::Header::Status::FACILITY );
		BGLB( 0x40000000, l[8], 0x94, 0xc2, "Reserved:" );
		BGLB( 0x40000000, l[7], 0x94, 0xc2, "Customer:" );		// to map to HRESULT0x20000000
			CEQU( 0, l[7], 0, 0x14, "Microsoft-defined Value" );
			CEQU( 1, l[7], 0, 0x14, "Customer-defined Value" );
		BGLB( 0x40000000, l[44], 0x94, 0xc2, "Severity:" );
			CST#( 0, l[44],  0x94, 0x14, SMB2::Status );
		
		LABL( 0, 0, 0, 0xb8 );
				//	DGLB( 0, g[55], 0x90, 0xc2, Left SMB2 Length: );
	//-------------
	//Protocoll has length ids as backup to the commands!!!
	DWRD( 0x40000000, l[5], 0x90, 0xc2, "Command:" );
	CST#( 0, l[5], 0x90, 0x14, SMB2::Command );
	CEQU( 1, l[54], 0x00, 0x14, "Response" );
	CEQU( 0, l[54], 0x00, 0x14, "Request" );
	g[30] = l[54];
	SHFL( 8, g[30]);
	g[30] += l[5];	
	// BGLB( 0, g[30], 0x90, 0xc2, "g30:" );
	if (l[54] == 1)
	{
		DWRD( 0x40000000, l[6], 0x90, 0xc2, "Credits granted:" );
	}
	if (l[54] == 0)
	{
		DWRD( 0x40000000, l[6], 0x90, 0xc2, "Credits requested:" );
	}
	HLNG( 0x40000000, l[7], 0x90, 0xc9, "Flags:" );
		CEQU( 1, l[50], 0, 0xd4, ...1 .... .... .... .... .... .... .... DFS Operation: This is a normal operation);
		CLSE( 0, l[50], 0, 0xd4, ...0 .... .... .... .... .... .... .... DFS Operation: This is a DFS operation);
		CEQU( 1, l[51], 0, 0xd4, .... .... .... .... .... .... .... 1... Signing: This pdu is signed);
		CLSE( 0, l[51], 0, 0xd4, .... .... .... .... .... .... .... 0... Signing: This pdu is NOT signed);
		CEQU( 1, l[52], 0, 0xd4, .... .... .... .... .... .... .... .1.. Chained: This pdu is a chained command);
		CLSE( 0, l[52], 0, 0xd4, .... .... .... .... .... .... .... .0.. Chained: This pdu is NOT a chained command);
		CEQU( 1, l[53], 0, 0xd4, .... .... .... .... .... .... .... ..1. Async Command: This is a ASYNC command);
		CLSE( 0, l[53], 0, 0xd4, .... .... .... .... .... .... .... ..0. Async Command: This is a SYNC command);
		CEQU( 1, l[54], 0, 0xd4, .... .... .... .... .... .... .... ...1 Response: This is a response);
		CLSE( 0, l[54], 0, 0xd4, .... .... .... .... .... .... .... ...0 Response: This is a request);
		LABL( 0, 0, 0, 0xb8 );
	HLNG( 0x40000000, l[8], 0x90, 0xc2, "NextCommand:" );
	g[58] = l[8];
	D64B( 0x40000000, l[9], 0x90, 0xc2, "MessageId:" );
	DECR 8 ;
	DLNG( 0x40000000, l[9], 0x5, 0x5, "MessageId:" );
	INCR 4;
	g[54] = l[9];
	if (l[53] != 1) //SYNC Header
	{
		//LABL( 0, 0, 0, 0xc9, "ID" );
		D64B( 0x40000000, l[10], 0x90, 0xc9, "ID" );
		DECR( 8 );
		HLNG( 0x40000000, l[10], 0x90, 0xc2, "ProcessId:" );
		HLNG( 0x40000000, l[11], 0x90, 0xc2, "TreeId:" );
		LABL( 0, 0, 0, 0xb8 );
	}
	if (l[53] == 1) //ASYNC Header
	{
		D64B( 0x40000000, l[12], 0x90, 0xc2, "AsyncId:" );
	}
	D64B( 0x40000000, l[12], 0x90, 0xc2, "SessionId:" );
	DUMP( 0x40000000, 16, 0x90, 0xc2, "Signature:" );	
	GWRD( 0x40000000, l[40]);
	LABL( 0, 0, 0, 0xb8 );
	GWRD( 0x40000000, l[5]);
	
	g[55] -= l[2];
	//Padding
	g[58] -= l[2];
	l[5] -= 1;
	g[58] -= l[5];
	if ( l[44] != 0x03)
	{
		if (g[30] != 0xffff)
		{
		//DGLB( 0, g[30], 0x90, 0xc2, SMB2::Command::Link: );
		TSB#( 0, g[30], 0, 0, SMB2::Command::Link );
		}
	}	
	if ( l[44] == 0x03)
	{

		LABL( 0, 0, 0, 0xb7, "ErrorMessage" );
		DWRD( 0x40000000, 0, 0x90, 0xc2, "StructureSize:" );
		DWRD( 0x40000000, l[2], 0x90, 0xc2, "Reserved:" );
			
		HLNG( 0x40000000, l[3], 0x90, 0xc2, "ByteCount:" );
		if (l[3] == 0)
		{	
			l[3] = 1;		
			DBYT( 0, l[3], 0x90, 0xc2, "ErrorMessage:" );
		}
		if (l[3] != 0)
		{			
			CHR#( 0, l[3], 0x90, 0xc2, "ErrorMessage:" );
		}
		LABL( 0, 0, 0, 0xb8 );
	}
	LABL( 0, 0, 0, 0xb8 );

	LABL( 0, 0, 0, 0xb8 );
}

