void SMB File Share2;
	LABL 0 0 0 b1 SMB - Server Message Block\SMB:;
	MOVE 10 92;
	INCR 1;
	CHR# 3 0 90 c2 Protocol ID:\;
	DBYT 0 2 90 c2 Command Code:\Type:;
	SMB Command Codes;
	MOVE 20 f;
	SMB Status Code;

	BBYT 0 9 90 c9 SMB Flags:;
	BST# 0 9 0 d4 SMB::File_Share_Flags_Str;
	LABL 0 0 0 b8;

	BWRD 40000000 11 90 c9 SMB Flags2:;
	BST# 0 11 0 d4 SMB::File_Share_Flags2_Str;
	LABL 0 0 0 b8;

	HWRD 40000000 1 90 c2 Process ID High:;
	DGLB		0 1 00 70 (;
	LABL		0 0 00 00 ")";
	HEX# 40000008 0 90 c2 Signature:;
	HWRD 40000000 0 90 c2 Reserved:;
	HWRD 40000000 1 90 c2 Tree ID (TID):\TID:;
	DGLB		0 1 00 70 (;
	LABL		0 0 00 00 ")";
	HWRD 40000000 1 90 c2 Process ID (PID):\PID:;
	DGLB		0 1 00 70 (;
	LABL		0 0 00 00 ")";
	HWRD 40000000 1 90 c2 User ID (UID):\UID:;
	DGLB		0 1 00 70 (;
	LABL		0 0 00 00 ")";
	HWRD 40000000 1 90 c2 Multiplex ID (MID):\MID:;
	DGLB		0 1 00 70 (;
	LABL		0 0 00 00 ")";
	SMB File Share 2;
	TGTE 0 10 0 0 RETURN;
	//si: check to see if the remaining data is fcs + smb id
	SMB::File_Share::End_Check;
	TGTE 0 10 0 0 RETURN;
	DUMP 0 10 90 c2 Remaining SMB Data:;

