	HWRD  40000000 2  90 c2 Pipe Function:;
	SMB_Pipe_Name;			* SI: type of Pipe
	HWRD  40000000 1  90 c2 Pipe Handle:;
	INCR  2;				*incr past byte count
	*SMB_Padding;
	INCR  1;				*si....? TSUB  -Pad Byte;
	TSUB  -Trans2 Transaction Name 2;
	*TSUB  -Pad Byte;
	*TSUB  -Pad Byte;
	SMB_Padding;
	*INCR  2;				*unknown bytes
	
	g[1] = pb[0];
	if (g[1] == 104)
	{
		DWRD  40000000 1  90 c2 Function Code:;
		TNXT -SMB Network Server Enumerate 2;
	}
	TNXT -DCE::Protocol;
	



