			CLSE( 21, l[16], 0, 0xd4, ".... .... ..0. .... .... .... .... .... FILE_OPEN_REPARSE_POINT: not set" );
			CBIT( 22, l[16], 0, 0xd4, ".... .... .1.. .... .... .... .... .... FILE_OPEN_NO_RECALL: set" );
			CLSE( 22, l[16], 0, 0xd4, ".... .... .0.. .... .... .... .... .... FILE_OPEN_NO_RECALL: not set" );
			CBIT( 23, l[16], 0, 0xd4, ".... .... 1... .... .... .... .... .... FILE_OPEN_FOR_FREE_SPACE_QUERY: set" );
			CLSE( 23, l[16], 0, 0xd4, ".... .... 0... .... .... .... .... .... FILE_OPEN_FOR_FREE_SPACE_QUERY: not set" );	
		LABL( 0, 0, 0, 0xb8 );
		DWRD( 0x40000000, l[17], 0x90, 0xc2, "NameOffset:" );
		DWRD( 0x40000000, l[20], 0x90, 0xc2, "NameLength:" );
		DLNG( 0x40000000, l[19], 0x90, 0xc2, "CreateContextsOffset:" );
		DLNG( 0x40000000, l[18], 0x90, 0xc2, "CreateContextsLength:" );
		//CHR#( 0, l[18], 0x90, 0xc2, "UnicodeString Name:" ); 
		//the payload yould be should be FQDN or textual IP Adress or...  this mechanism knows only two types!
		//DGLB( 0, l[20], 0x90, 0xc2, "l20:" );
			l[21] = l[19];
			l[21] -= l[17];
			l[21] -= l[20];
		//DGLB( 0, l[21], 0x90, 0xc2, "l21:" );
		
		if ( l[20] != 0)
		{
			CHR#( 0x40000000, l[20], 0x90, 0xc2, "Unicode String Name:"  );
		}		
		if (l[21] >= 1)
		{
			CHR#( 0, l[21], 0x90, 0xc2, "ContextPadding:" );
		}
		if (l[18] > 0)
		{
			LABL( 0, 0, 0, 0xb7, "SMB2 CREATE_CONTEXTS " );	
			while (l[18] >= 1)
			{
				GLNG( 16, g[22] );
				CST#( 0, g[22], 0xb7, 0xb7, SMB2::CREATE_CONTEXT::Cntxts );
				CLSE( 0, g[22], 0x07, 0xb7, "BLOB" );
				LABL( 0, 0, 0, 0x07, " - SMB2 CREATE_CONTEXT Request" );
				//LABL( 0, 0, 0, 0xb7, "SMB2 CREATE_CONTEXT Request " );	
					DLNG( 0x40000000, l[1], 0x90, 0xc2, "Next:" );
					DWRD( 0x40000000, l[2], 0x90, 0xc2, "NameOffset:" );
					DWRD( 0x40000000, l[3], 0x90, 0xc2, "NameLength:" );
					DWRD( 0x40000000, l[4], 0x90, 0xc2, "Reserved:" );
					DWRD( 0x40000000, l[5], 0x90, 0xc2, "DataOffset:" );
					DLNG( 0x40000000, g[21], 0x90, 0xc2, "DataLength:" );
					GLNG( 0, g[22] ); g[27] = g[21];
					CHR#( 0, l[3], 0x90, 0xc2, "AsciiString Name:" ); 
					CHR#( 0, 4, 0x90, 0xc2, "DataPadding:" ); 
					if(g[21] != 0)
					{
						//DHnQ
						if (g[22] == 0x44486E51 )
						{
							HEX#( 0, g[21], 0x90, 0xc2, "DurableRequest:" ); 
							g[21] -= g[21];
						}
						//DHnC //"Reconnect Durable handle"
						if (g[22] == 0x44486E43 )
						{
							LABL( 0, 0, 0, 0xc9, "FileID" );
							D64B( 0x40000000, l[5], 0x90, 0xc2, "Persistent:" );
							D64B( 0x40000000, l[6], 0x90, 0xc2, "Volatile:" );
							LABL( 0, 0, 0, 0xb8 );
							g[21] -= 16;
						}
						//MxAc
						if (g[22] == 0x4D784163 )
						{
							TIME( 4, l[7], 0x90, 0xc2, "Timestamp:" );
							g[21] -= 8;
						}
						//ExtA SMB2_CREATE_EA_BUFFER
						if (g[22] == 0x45787441 )
						{
							//to fill
						}
						//"SecD":		SecurityDescriptorRelative SecurityDescriptor;
						if (g[22] == 0x53656344 )
						{
							HEX#( 0, g[21], 0x90, 0xc2, "SecurityDescriptor:" );	g[21] -= g[21];
							//to decode
						}
						// "AlSi":
						if (g[22] == 0x416C5369 )
						{
							D64B 40000000 1 90 c2 Allocation Size:;
							g[21] -= 8;
						}
						//TWrp":struct ECPTimeWarpToken = FormatString( "Open an earlier version file" ){FILETIME Timestamp;
						if (g[22] == 0x54577270 )
						{
							TIME( 4, l[7], 0x90, 0xc2, "Timestamp:" );
							g[21] -= 8;
						}
						//if (g[22] == "QFid") SMB2_CREATE_QUERY_ON_DISK_ID
						if (g[22] == 0x51466964)
						{
							HEX#( 0, 32, 0x90, 0xc2, "DiskIDBuffer:" ); 
							g[21] -= 32;
						}						
						//RqLs CreateRequestLease
						if (g[22] == 0x52714C73 )
						{
							HEX#( 0x40000000, 16, 0x90, 0xc2, "LeaseKey:" );
							BLNG( 0x40000000, l[8], 0x90, 0xc9, "LeaseState:" );
								CBIT( 0, l[8], 0x0, 0xd4, ".... .... .... .... .... .... .... ...1 READ: set" );
								CLSE( 0, l[8], 0x0, 0xd4, ".... .... .... .... .... .... .... ...0 READ: not set" );
								CBIT( 1, l[8], 0x0, 0xd4, ".... .... .... .... .... .... .... ..1. HANDLE: set" );
								CLSE( 1, l[8], 0x0, 0xd4, ".... .... .... .... .... .... .... ..0. FILE_WRITE_THROUGH: not set" );
								CBIT( 2, l[8], 0x0, 0xd4, ".... .... .... .... .... .... .... .1.. WRITE: set" );
								CLSE( 2, l[8], 0x0, 0xd4, ".... .... .... .... .... .... .... .0.. FILE_SEQUENTIAL_ONLY: not set" );
								LABL( 0, 0, 0, 0xd4, "xxxx xxxx xxxx xxxx xxxx xxxx xxxx x... Reserved" );
							LABL( 0, 0, 0, 0xb8 );
							BLNG( 0x40000000, l[9], 0x90, 0xc2, "LeaseFlags:" );
							D64B( 0x40000000, l[10], 0x90, 0xc2, "LeaseDuration:" );
							g[21] -= 32;
						}
						if(g[21] != 0)
						{
							HEX#( 0, g[21], 0x90, 0xc2, "BLOB:" );
							g[21] -= g[21];
						}
					}
				LABL( 0, 0, 0, 0xb8 );
				l[18] -= l[1];
				if (l[1] == 0)
				{
					
					l[18] -= l[5];
					l[18] -= g[27];
				}
			}	
		}
	LABL( 0, 0, 0, 0xb8 );
}

