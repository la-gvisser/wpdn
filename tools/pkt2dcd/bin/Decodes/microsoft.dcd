void SMB::SessSetup_Cmd::12;
	HBYT 40000000 2 90 c2 Secondary Command:;
	SMB Command Codes;
	l[0x11] = g[0x11];
	HBYT 20000000 4 90 c2 AndX Reserved:;
	DWRD 40000000 d 90 c2 AndX Offset:;					*si: save for use in same func below
	DWRD 40000000 4 90 c2 Max Buffer Size:;
	DWRD 40000000 4 90 c2 Max Multiplex Count:;
	DWRD 40000000 4 90 c2 VC Number:;
